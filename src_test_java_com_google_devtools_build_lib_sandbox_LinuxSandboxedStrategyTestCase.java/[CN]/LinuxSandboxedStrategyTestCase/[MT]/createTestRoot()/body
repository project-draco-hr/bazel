{
  fileSystem=FileSystems.initDefaultAsNative();
  Path testRoot=fileSystem.getPath(TestUtils.tmpDir());
  try {
    FileSystemUtils.deleteTreesBelow(testRoot);
  }
 catch (  IOException e) {
    System.err.println("Failed to remove directory " + testRoot + ": "+ e.getMessage());
    throw e;
  }
  return testRoot;
}
