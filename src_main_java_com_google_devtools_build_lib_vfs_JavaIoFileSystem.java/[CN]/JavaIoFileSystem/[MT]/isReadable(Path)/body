{
  File file=getIoFile(path);
  long startTime=Profiler.nanoTimeMaybe();
  try {
    if (!file.exists()) {
      throw new FileNotFoundException(path + ERR_NO_SUCH_FILE_OR_DIR);
    }
    return file.canRead();
  }
  finally {
    profiler.logSimpleTask(startTime,ProfilerTask.VFS_STAT,file.getPath());
  }
}
