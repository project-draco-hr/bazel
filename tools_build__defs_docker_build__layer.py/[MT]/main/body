def main(unused_argv):
    with DockerLayer(FLAGS.output, FLAGS.file_path, FLAGS.directory) as layer:
        for f in FLAGS.file:
            layer.add_file(f)
        for tar in FLAGS.tar:
            layer.add_tar(tar)
        for deb in FLAGS.deb:
            layer.add_deb(deb)
        for link in FLAGS.link:
            l = link.split(':', 1)
            layer.add_link(l[0], l[1])
