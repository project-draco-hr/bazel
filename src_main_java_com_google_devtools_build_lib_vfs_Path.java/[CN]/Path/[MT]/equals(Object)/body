{
  if (this == other) {
    return true;
  }
  if (!(other instanceof Path)) {
    return false;
  }
  Path otherPath=(Path)other;
  if (hashCode != otherPath.hashCode) {
    return false;
  }
  return fileSystem.equals(otherPath.fileSystem) && name.equals(otherPath.name) && Objects.equals(parent,otherPath.parent);
}
