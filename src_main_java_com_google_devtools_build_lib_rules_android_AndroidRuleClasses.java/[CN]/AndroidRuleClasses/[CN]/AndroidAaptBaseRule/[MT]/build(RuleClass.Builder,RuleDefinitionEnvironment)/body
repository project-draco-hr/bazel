{
  return builder.add(attr("$android_resources_processor",LABEL).cfg(HOST).exec().value(env.getLabel(env.getToolsRepository() + DEFAULT_RESOURCES_PROCESSOR))).add(attr("$android_aar_generator",LABEL).cfg(HOST).exec().value(env.getLabel(env.getToolsRepository() + DEFAULT_AAR_GENERATOR))).build();
}
