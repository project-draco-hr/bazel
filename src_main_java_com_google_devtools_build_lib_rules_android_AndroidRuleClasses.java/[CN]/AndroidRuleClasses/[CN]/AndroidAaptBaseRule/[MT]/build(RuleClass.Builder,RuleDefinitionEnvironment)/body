{
  return builder.add(attr("$android_resources_processor",LABEL).cfg(HOST).exec().value(env.getToolsLabel(DEFAULT_RESOURCES_PROCESSOR))).add(attr("$android_resource_shrinker",LABEL).cfg(HOST).exec().value(env.getToolsLabel(DEFAULT_RESOURCE_SHRINKER))).add(attr("$android_aar_generator",LABEL).cfg(HOST).exec().value(env.getToolsLabel(DEFAULT_AAR_GENERATOR))).build();
}
