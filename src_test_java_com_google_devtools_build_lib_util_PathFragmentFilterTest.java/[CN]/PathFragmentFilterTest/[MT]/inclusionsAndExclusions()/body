{
  createFilter("a,-c,,d,a/b/c,-a/b,a/b/d");
  assertIncluded("a");
  assertIncluded("a/c");
  assertExcluded("a/b");
  assertExcluded("a/b/c");
  assertExcluded("a/b/d");
  assertExcluded("c");
  assertExcluded("c/d");
  assertIncluded("d/e");
  assertExcluded("e");
  assertEquals("a,d,a/b/c,a/b/d,-c,-a/b",filter.toString());
}
