{
  out.printf("    '%s': {\n",category);
  for (  String function : tasks.keySet()) {
    out.printf("      '%s': google.visualization.arrayToDataTable(\n",function);
    out.print("        [['duration']");
    for (    Task task : tasks.get(function)) {
      out.printf(",[%f]",task.duration / 1000000.);
    }
    out.println("],\n        false),");
  }
  out.println("    },");
}
