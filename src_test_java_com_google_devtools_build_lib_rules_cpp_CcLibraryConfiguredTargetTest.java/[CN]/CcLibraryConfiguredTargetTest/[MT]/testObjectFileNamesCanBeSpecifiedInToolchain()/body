{
  AnalysisMock.get().ccSupport().setupCrosstool(mockToolsConfig,"artifact_name_pattern {" + "   category_name: 'object_file'" + "   pattern: 'object_%{base_name}.o'"+ "}");
  useConfiguration();
  ConfiguredTarget hello=getConfiguredTarget("//hello:hello");
  assertThat(artifactByPath(getFilesToBuild(hello),".a","object_hello.o")).isNotNull();
}
