{
  AnalysisMock.get().ccSupport().setupCrosstool(mockToolsConfig,"artifact_name_pattern {" + "   category_name: 'object_file'" + "   pattern: '%{base_name}.obj'"+ "}");
  useConfiguration();
  ConfiguredTarget hello=getConfiguredTarget("//hello:hello");
  assertThat(artifactByPath(getFilesToBuild(hello),".a","hello.obj")).isNotNull();
}
