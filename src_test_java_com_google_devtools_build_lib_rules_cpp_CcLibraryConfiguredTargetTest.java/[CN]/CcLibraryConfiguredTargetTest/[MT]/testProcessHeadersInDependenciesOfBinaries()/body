{
  AnalysisMock.get().ccSupport().setupCrosstool(mockToolsConfig,MockCcSupport.HEADER_PROCESSING_FEATURE_CONFIGURATION);
  useConfiguration("--features=parse_headers","--process_headers_in_dependencies");
  ConfiguredTarget x=scratchConfiguredTarget("foo","x","cc_binary(name = 'x', deps = [':y'])","cc_library(name = 'y', hdrs = ['y.h'])");
  assertThat(ActionsTestUtil.baseArtifactNames(getOutputGroup(x,OutputGroupProvider.HIDDEN_TOP_LEVEL))).contains("y.h.processed");
}
