{
  return new ConfigurationFactory(new BazelConfigurationCollection(),new BazelConfiguration.Loader(),new CppConfigurationLoader(Functions.<String>identity()),new PythonConfigurationLoader(Functions.<String>identity()),new BazelPythonConfiguration.Loader(),new JvmConfigurationLoader(BazelRuleClassProvider.JAVA_CPU_SUPPLIER),new JavaConfigurationLoader(),new ObjcConfigurationLoader(),new AppleConfiguration.Loader(),new J2ObjcConfiguration.Loader(),new AndroidConfiguration.Loader());
