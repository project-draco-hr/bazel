{
  return new CcLinkParamsStore(){
    @Override protected void collect(    CcLinkParams.Builder builder,    boolean linkingStatically,    boolean linkShared){
      Iterable<? extends TransitiveInfoCollection> deps=ruleContext.getPrerequisites("deps",Mode.TARGET);
      builder.addTransitiveTargets(deps);
      builder.addTransitiveLangTargets(deps,PyCcLinkParamsProvider.TO_LINK_PARAMS);
    }
  }
;
}
