{
  this(ruleContext,semantics,ruleContext.getPrerequisiteArtifacts("srcs",Mode.TARGET).list(),collectTargetsTreatedAsDeps(ruleContext,semantics,ClasspathType.COMPILE_ONLY),collectTargetsTreatedAsDeps(ruleContext,semantics,ClasspathType.RUNTIME_ONLY),collectTargetsTreatedAsDeps(ruleContext,semantics,ClasspathType.BOTH));
}
