{
  for (  FileProvider provider : ruleContext.getPrerequisites("srcs",Mode.TARGET,FileProvider.class)) {
    Iterable<Artifact> jarFiles=helper.filterGeneratedJarsThroughIjar(FileType.filter(provider.getFilesToBuild(),JavaSemantics.JAR));
    List<Artifact> jarsWithOwners=Lists.newArrayList(jarFiles);
    attributes.addDirectCompileTimeClassPathEntries(jarsWithOwners);
    attributes.addCompileTimeJarFiles(jarsWithOwners);
  }
}
