{
  addInstrumentationFilesProvider(builder,filesToBuild,instrumentationSpec);
  builder.add(JavaExportsProvider.class,new JavaExportsProvider(collectTransitiveExports())).addSkylarkTransitiveInfo(JavaSkylarkApiProvider.NAME,new JavaSkylarkApiProvider()).addOutputGroup(OutputGroupProvider.FILES_TO_COMPILE,getFilesToCompile(classJar)).add(JavaCompilationInfoProvider.class,createCompilationInfoProvider());
}
