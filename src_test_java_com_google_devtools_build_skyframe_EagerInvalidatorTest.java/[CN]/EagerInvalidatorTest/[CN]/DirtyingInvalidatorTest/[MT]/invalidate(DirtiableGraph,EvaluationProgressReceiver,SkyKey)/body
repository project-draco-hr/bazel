{
  Iterable<SkyKey> diff=ImmutableList.copyOf(keys);
  InvalidatingNodeVisitor invalidatingVisitor=EagerInvalidator.createInvalidatingVisitorIfNeeded(graph,diff,invalidationReceiver,state,dirtyKeyTracker);
  if (invalidatingVisitor != null) {
    visitor.set(invalidatingVisitor);
    invalidatingVisitor.run();
  }
}
