{
  final Pair<Map<PathFragment,Artifact>,Map<PathFragment,Artifact>> runfilesInputs=runfiles.getRunfilesInputs(root,eventHandler,getOwner().getLocation());
  return new DeterministicWriter(){
    @Override public void writeOutputFile(    OutputStream out) throws IOException {
      writeFile(out,runfilesInputs);
    }
  }
;
}
