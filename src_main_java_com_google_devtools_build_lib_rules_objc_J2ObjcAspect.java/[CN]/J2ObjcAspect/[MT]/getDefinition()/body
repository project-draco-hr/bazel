{
  return addAttributeAspects(new AspectDefinition.Builder("J2ObjCAspect")).requireProvider(JavaSourceInfoProvider.class).requireProvider(JavaCompilationArgsProvider.class).add(attr("$j2objc",LABEL).cfg(HOST).exec().value(parseLabel("//tools/j2objc:j2objc_deploy.jar"))).add(attr("$j2objc_wrapper",LABEL).allowedFileTypes(FileType.of(".py")).cfg(HOST).exec().singleArtifact().value(parseLabel("//tools/j2objc:j2objc_wrapper"))).build();
}
