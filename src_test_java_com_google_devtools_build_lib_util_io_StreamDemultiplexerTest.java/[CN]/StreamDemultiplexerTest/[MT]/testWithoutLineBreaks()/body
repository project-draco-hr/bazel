{
  byte[] multiplexed=lines("@1@","just ","@1@","one ","@1@","line","");
  StreamDemultiplexer demux=new StreamDemultiplexer((byte)'1',out);
  try {
    demux.write(multiplexed);
    demux.flush();
  }
  finally {
    demux.close();
  }
  assertEquals("just one line",out.toString("ISO-8859-1"));
}
