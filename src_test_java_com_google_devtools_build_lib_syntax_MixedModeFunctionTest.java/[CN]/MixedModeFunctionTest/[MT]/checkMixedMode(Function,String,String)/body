{
  setUp();
  update(func.getName(),func);
  if (expectedOutput.charAt(0) == '[') {
    assertEquals(expectedOutput,eval(callExpression).toString());
  }
 else {
    try {
      eval(callExpression);
      fail();
    }
 catch (    EvalException e) {
      assertThat(e).hasMessage(expectedOutput);
    }
  }
}
