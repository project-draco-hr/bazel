{
  AspectDefinition.Builder builder=new AspectDefinition.Builder(NAME).attributeAspect("runtime_deps",AndroidStudioInfoAspect.class).attributeAspect("resources",AndroidStudioInfoAspect.class).add(attr("$packageParser",LABEL).cfg(HOST).exec().value(Label.parseAbsoluteUnchecked(Constants.TOOLS_REPOSITORY + "//tools/android:PackageParser")));
  for (  PrerequisiteAttr prerequisiteAttr : PREREQUISITE_ATTRS) {
    builder.attributeAspect(prerequisiteAttr.name,AndroidStudioInfoAspect.class);
  }
  return builder.build();
}
