{
  AspectDefinition.Builder builder=new AspectDefinition.Builder(NAME).attributeAspect("runtime_deps",this).attributeAspect("resources",this).add(attr("$packageParser",LABEL).cfg(HOST).exec().value(Label.parseAbsoluteUnchecked(toolsRepository + "//tools/android:PackageParser")));
  for (  PrerequisiteAttr prerequisiteAttr : prerequisiteAttrs) {
    builder.attributeAspect(prerequisiteAttr.name,this);
  }
  return builder.build();
}
