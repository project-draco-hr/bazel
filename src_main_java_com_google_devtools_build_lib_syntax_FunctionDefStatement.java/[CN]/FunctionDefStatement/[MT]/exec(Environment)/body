{
  ImmutableMap.Builder<String,Object> defaultValues=ImmutableMap.builder();
  for (  Argument arg : args) {
    if (arg.hasValue()) {
      defaultValues.put(arg.getArgName(),arg.getValue().eval(env));
    }
  }
  env.update(ident.getName(),new UserDefinedFunction(ident,args,defaultValues.build(),statements,(SkylarkEnvironment)env));
}
