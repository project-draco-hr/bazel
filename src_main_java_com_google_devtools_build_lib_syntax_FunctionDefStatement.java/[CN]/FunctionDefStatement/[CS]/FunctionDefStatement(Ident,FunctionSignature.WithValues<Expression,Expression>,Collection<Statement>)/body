{
  FunctionSignature.Shape shape=args.getSignature().getShape();
  Preconditions.checkArgument(!shape.hasKwArg() && !shape.hasStarArg() && shape.getNamedOnly() == 0,"no star, star-star or named-only parameters (for now)");
  this.ident=ident;
  this.args=args;
  this.statements=ImmutableList.copyOf(statements);
}
