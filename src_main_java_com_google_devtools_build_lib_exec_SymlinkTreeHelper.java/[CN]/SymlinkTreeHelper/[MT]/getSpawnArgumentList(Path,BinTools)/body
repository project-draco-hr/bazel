{
  List<String> args=Lists.newArrayList(execRoot.getRelative(binTools.getExecPath(BUILD_RUNFILES)).getPathString());
  if (filesetTree) {
    args.add("--allow_relative");
    args.add("--use_metadata");
  }
  args.add(inputManifest.getPathString());
  args.add(symlinkTreeRoot.getPathString());
  return args;
}
