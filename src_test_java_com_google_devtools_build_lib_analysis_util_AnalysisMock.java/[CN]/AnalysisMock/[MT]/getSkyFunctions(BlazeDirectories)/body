{
  RepositoryFunction localRepositoryFunction=new LocalRepositoryFunction();
  localRepositoryFunction.setDirectories(directories);
  ImmutableMap<String,RepositoryFunction> repositoryHandlers=ImmutableMap.of(LocalRepositoryRule.NAME,localRepositoryFunction);
  return ImmutableMap.of(SkyFunctions.REPOSITORY_DIRECTORY,new RepositoryDelegatorFunction(directories,repositoryHandlers,null,new AtomicBoolean(true)),SkyFunctions.REPOSITORY,new RepositoryLoaderFunction());
}
