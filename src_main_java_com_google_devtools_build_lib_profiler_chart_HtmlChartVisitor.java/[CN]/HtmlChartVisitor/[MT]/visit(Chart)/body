  out.println("<p>To get more information about a task point the mouse at one of the bars.</p>");
  out.printf("<div style='position:relative; height: %dpx; margin: %dpx'>\n",chart.getRowCount() * ROW_HEIGHT,H_OFFSET + 10);
}
