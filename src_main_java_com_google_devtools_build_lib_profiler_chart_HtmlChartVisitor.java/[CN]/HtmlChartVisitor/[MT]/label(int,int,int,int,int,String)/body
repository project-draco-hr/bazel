{
  if (fontSize > 0) {
    out.printf("<div style=\"position:absolute; left:%dpx; top:%dpx; width:%dpx; " + "height:%dpx; font-size:%dpt\">%s</div>\n",x,y,width,height,fontSize,text);
  }
 else {
    out.printf("<div style=\"position:absolute; left:%dpx; top:%dpx; width:%dpx; " + "height:%dpx\">%s</div>\n",x,y,width,height,text);
  }
}
