{
  LoadedPackageProvider loadedPackageProvider=getLoadedPackageProvider();
  return new CyclesReporter(new TransitiveTargetCycleReporter(loadedPackageProvider),new ActionArtifactCycleReporter(loadedPackageProvider),new SkylarkModuleCycleReporter(),new ConfiguredTargetCycleReporter(loadedPackageProvider));
}
