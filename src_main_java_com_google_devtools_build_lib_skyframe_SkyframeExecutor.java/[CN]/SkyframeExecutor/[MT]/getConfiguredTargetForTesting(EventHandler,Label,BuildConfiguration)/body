{
  if (memoizingEvaluator.getExistingValueForTesting(PrecomputedValue.WORKSPACE_STATUS_KEY.getKeyForTesting()) == null) {
    injectWorkspaceStatusData();
  }
  return Iterables.getFirst(getConfiguredTargets(eventHandler,configuration,ImmutableList.of(configuration != null ? Dependency.withConfiguration(label,configuration) : Dependency.withNullConfiguration(label)),true),null);
}
