{
  EvaluationResult<WorkspaceStatusValue> result=buildDriver.evaluate(ImmutableList.of(WorkspaceStatusValue.SKY_KEY),true,1,reporter);
  WorkspaceStatusValue value=Preconditions.checkNotNull(result.get(WorkspaceStatusValue.SKY_KEY));
  return ImmutableList.of(value.getStableArtifact(),value.getVolatileArtifact());
}
