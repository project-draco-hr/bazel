{
  Iterable<SkyKey> fileStateSkyKeys=Iterables.transform(modifiedSourceFiles,new Function<PathFragment,SkyKey>(){
    @Override public SkyKey apply(    PathFragment pathFragment){
      Preconditions.checkState(!pathFragment.isAbsolute(),"found absolute PathFragment: %s",pathFragment);
      return FileStateValue.key(RootedPath.toRootedPath(pathEntry,pathFragment));
    }
  }
);
  Iterable<SkyKey> dirListingStateSkyKeys=Iterables.transform(modifiedSourceFiles,new Function<PathFragment,SkyKey>(){
    @Override public SkyKey apply(    PathFragment pathFragment){
      Preconditions.checkState(!pathFragment.isAbsolute(),"found absolute PathFragment: %s",pathFragment);
      return DirectoryListingStateValue.key(RootedPath.toRootedPath(pathEntry,pathFragment.getParentDirectory()));
    }
  }
);
  return Iterables.concat(fileStateSkyKeys,dirListingStateSkyKeys);
}
