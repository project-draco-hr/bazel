{
  return new BlazeExecutor(directories.getExecRoot(),directories.getOutputPath(),reporter,bus,BlazeClock.instance(),optionsParser,optionsParser.getOptions(ExecutionOptions.class).verboseFailures,optionsParser.getOptions(ExecutionOptions.class).showSubcommands,strategies,ImmutableMap.copyOf(spawnStrategyMap),ImmutableList.<ActionContextProvider>of());
}
