{
  return builder.requiresConfigurationFragments(ObjcConfiguration.class).setImplicitOutputsFunction(ImplicitOutputsFunction.fromFunctions(ReleaseBundlingSupport.IPA,XcodeSupport.PBXPROJ)).add(attr("binary",LABEL).allowedRuleClasses("ios_extension_binary").allowedFileTypes().mandatory().direct_compile_time_input().cfg(IosExtension.MINIMUM_OS_AND_SPLIT_ARCH_TRANSITION)).build();
}
