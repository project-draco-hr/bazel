{
  ZipFileEntry foo=new ZipFileEntry("foo");
  ExtraData[] extra=new ExtraData[]{new ExtraData((short)0xCAFE,new byte[]{0x01,0x02}),new ExtraData((short)0xADDE,new byte[]{(byte)0xBE,(byte)0xEF})};
  foo.setExtra(extra);
  assertThat(foo.getExtra()).isEqualTo(new byte[]{(byte)0xFE,(byte)0xCA,0x02,0x00,0x01,0x02,(byte)0xDE,(byte)0xAD,0x02,0x00,(byte)0xBE,(byte)0xEF});
}
