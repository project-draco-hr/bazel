{
  checkNotFinished();
  checkArgument(filename.endsWith("/"));
  checkState(!fileNames.containsKey(filename),"jar already contains a directory named %s",filename);
  int dosTime=dateToDosTime(date != null ? date : new Date());
  copyStreamToEntry(filename,new ByteArrayInputStream(new byte[0]),dosTime,extraDataEntries,false,DEFAULT_DIRECTORY_ENTRY_INFO);
}
