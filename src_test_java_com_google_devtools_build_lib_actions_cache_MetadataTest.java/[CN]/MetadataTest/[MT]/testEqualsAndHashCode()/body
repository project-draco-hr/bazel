{
  new EqualsTester().addEqualityGroup(new Metadata(toBytes("00112233445566778899AABBCCDDEEFF")),new Metadata(toBytes("00112233445566778899AABBCCDDEEFF"))).addEqualityGroup(new Metadata(1)).addEqualityGroup(new Metadata(toBytes("FFFFFF00000000000000000000000000"))).addEqualityGroup(new Metadata(2),new Metadata(2)).addEqualityGroup("a string").testEquals();
}
