{
  return new AspectDefinition.Builder("JackAspect").requireProvider(JavaSourceInfoProvider.class).add(attr(":android_sdk",LABEL).allowedRuleClasses("android_sdk").value(AndroidRuleClasses.ANDROID_SDK)).attributeAspect("deps",JackAspect.class).attributeAspect("exports",JackAspect.class).attributeAspect("runtime_deps",JackAspect.class).requiresConfigurationFragments(AndroidConfiguration.class).build();
}
