{
  return new BuiltinFunction("bind",FunctionSignature.namedOnly("name","actual"),BuiltinFunction.USE_LOC){
    public Object invoke(    String name,    String actual,    Location loc) throws EvalException, InterruptedException {
      Label nameLabel=null;
      try {
        nameLabel=Label.parseAbsolute("//external:" + name);
        builder.addBinding(nameLabel,new Binding(Label.parseAbsolute(actual),loc));
      }
 catch (      Label.SyntaxException e) {
        throw new EvalException(loc,e.getMessage());
      }
      return NONE;
    }
  }
;
}
