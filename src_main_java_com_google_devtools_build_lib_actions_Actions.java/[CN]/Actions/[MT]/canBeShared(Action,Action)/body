{
  if (!a.getMnemonic().equals(b.getMnemonic())) {
    return false;
  }
  if (!a.getKey().equals(b.getKey())) {
    return false;
  }
  if (!Iterables.elementsEqual(a.getMandatoryInputs(),b.getMandatoryInputs())) {
    return false;
  }
  if (!Iterables.elementsEqual(a.getOutputs(),b.getOutputs())) {
    return false;
  }
  return true;
}
