{
  String artifactName=container.getLabel().getName() + fileNameSuffix;
  PathFragment resourceJarsPathFragment=ruleContext.getUniqueDirectory("resource_jars");
  PathFragment artifactPathFragment=resourceJarsPathFragment.getRelative(container.getLabel().getPackageFragment().getRelative(artifactName));
  Artifact artifact=ruleContext.getAnalysisEnvironment().getDerivedArtifact(artifactPathFragment,ruleContext.getBinOrGenfilesDirectory());
  return artifact;
}
