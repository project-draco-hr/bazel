{
  Artifact binaryResourcesJar=ruleContext.getImplicitOutputArtifact(JavaSemantics.JAVA_BINARY_CLASS_JAR);
  compileResourceJar(javaSemantics,binaryResourcesJar,updatedResources.getJavaSourceJar());
  artifactsBuilder.addRuntimeJar(binaryResourcesJar);
  createRepackerActions(attributes,resourceContainers,updatedResources.getJavaPackage(),binaryResourcesJar);
}
