{
  HashSet<JavaFileObject> result=new HashSet<JavaFileObject>();
  Iterable<JavaFileObject> files;
  try {
    files=fileManager.list(StandardLocation.PLATFORM_CLASS_PATH,"",EnumSet.of(JavaFileObject.Kind.CLASS),true);
  }
 catch (  IOException e) {
    throw new IOError(e);
  }
  for (  JavaFileObject file : files) {
    result.add(file);
  }
  return result;
}
