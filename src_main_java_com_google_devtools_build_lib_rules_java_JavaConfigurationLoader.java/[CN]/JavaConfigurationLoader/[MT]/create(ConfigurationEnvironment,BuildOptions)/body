{
  JavaOptions javaOptions=buildOptions.get(JavaOptions.class);
  if (javaOptions.disableJava) {
    return null;
  }
  Label javaToolchain=RedirectChaser.followRedirects(env,javaOptions.javaToolchain,"java_toolchain");
  return create(javaOptions,javaToolchain,cpuSupplier.getJavaCpu(buildOptions,env));
}
