{
  mockFunc=new BuiltinFunction("mock"){
    @SuppressWarnings("unused") public Object invoke(    Object mandatory,    Object optional,    Object mandatoryKey,    Object optionalKey){
      return EvalUtils.optionMap("mandatory",mandatory,"optional",optional,"mandatory_key",mandatoryKey,"optional_key",optionalKey);
    }
  }
;
  assertFalse(mockFunc.isConfigured());
  mockFunc.configure(SkylarkRuleImplementationFunctionsTest.class.getDeclaredField("mockFunc").getAnnotation(SkylarkSignature.class));
  update("mock",mockFunc);
  eval(line);
}
