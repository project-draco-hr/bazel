{
  ConfiguredTarget fooLib=setupWithOptions();
  ImmutableList<PathFragment> quoteIncludeDirs=fooLib.getProvider(CppCompilationContext.class).getQuoteIncludeDirs();
  assertThat(filterExternalBazelTools(quoteIncludeDirs)).containsExactly(PathFragment.EMPTY_FRAGMENT,targetConfig.getGenfilesFragment()).inOrder();
}
