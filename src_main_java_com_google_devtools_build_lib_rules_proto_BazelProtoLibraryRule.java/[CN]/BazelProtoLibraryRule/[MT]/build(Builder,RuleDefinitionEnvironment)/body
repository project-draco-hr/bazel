{
  return builder.requiresConfigurationFragments(ProtoConfiguration.class).setOutputToGenfiles().add(attr(":proto_compiler",LABEL).cfg(HOST).exec().value(PROTO_COMPILER)).override(attr("deps",LABEL_LIST).allowedRuleClasses("proto_library").allowedFileTypes()).add(attr("srcs",LABEL_LIST).direct_compile_time_input().allowedFileTypes(FileType.of(".proto"))).advertiseProvider(ProtoSourcesProvider.class).build();
}
