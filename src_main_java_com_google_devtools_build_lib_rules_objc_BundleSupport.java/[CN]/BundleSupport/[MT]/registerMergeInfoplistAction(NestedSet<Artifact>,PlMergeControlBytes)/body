{
  if (!bundling.needsToMergeInfoplist()) {
    return;
  }
  Artifact plMergeControlArtifact=ObjcRuleClasses.artifactByAppendingToBaseName(ruleContext,".plmerge-control");
  ruleContext.registerAction(new BinaryFileWriteAction(ruleContext.getActionOwner(),plMergeControlArtifact,controlBytes,false));
  ruleContext.registerAction(new SpawnAction.Builder().setMnemonic("MergeInfoPlistFiles").setExecutable(attributes.plmerge()).addArgument("--control").addInputArgument(plMergeControlArtifact).addTransitiveInputs(mergingContentArtifacts).addOutput(ObjcRuleClasses.intermediateArtifacts(ruleContext).mergedInfoplist()).build(ruleContext));
}
