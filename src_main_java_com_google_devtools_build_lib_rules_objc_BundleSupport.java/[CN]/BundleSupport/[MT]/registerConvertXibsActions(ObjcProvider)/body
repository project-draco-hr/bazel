{
  for (  Artifact original : objcProvider.get(ObjcProvider.XIB)) {
    Artifact zipOutput=bundling.getIntermediateArtifacts().compiledXibFileZip(original);
    String archiveRoot=BundleableFile.flatBundlePath(FileSystemUtils.replaceExtension(original.getExecPath(),".nib"));
    ruleContext.registerAction(ObjcRuleClasses.spawnXcrunActionBuilder(ruleContext).setMnemonic("XibCompile").setExecutable(attributes.ibtoolWrapper()).setCommandLine(ibActionsCommandLine(archiveRoot,zipOutput,original)).addOutput(zipOutput).addInput(original).build(ruleContext));
  }
}
