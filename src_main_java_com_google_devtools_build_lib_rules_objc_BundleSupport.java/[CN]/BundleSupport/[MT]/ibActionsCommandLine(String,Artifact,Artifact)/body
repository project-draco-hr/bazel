{
  CustomCommandLine.Builder commandLine=CustomCommandLine.builder().addPath(zipOutput.getExecPath()).add(archiveRoot).addPath(ObjcActionsBuilder.IBTOOL).add("--minimum-deployment-target").add(bundling.getMinimumOsVersion());
  for (  TargetDeviceFamily targetDeviceFamily : targetDeviceFamilies) {
    commandLine.add("--target-device").add(targetDeviceFamily.name().toLowerCase(Locale.US));
  }
  return commandLine.addPath(storyboardInput.getExecPath()).build();
}
