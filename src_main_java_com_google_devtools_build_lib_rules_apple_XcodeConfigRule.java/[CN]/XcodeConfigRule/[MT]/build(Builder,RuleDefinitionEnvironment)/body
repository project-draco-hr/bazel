{
  return builder.requiresConfigurationFragments(AppleConfiguration.class).add(attr(DEFAULT_ATTR_NAME,LABEL).allowedRuleClasses("xcode_version").allowedFileTypes()).add(attr(VERSIONS_ATTR_NAME,LABEL_LIST).allowedRuleClasses("xcode_version").allowedFileTypes()).add(attr(REQUIRE_DEFINED_VERSIONS_ATTR_NAME,BOOLEAN).value(false)).build();
}
