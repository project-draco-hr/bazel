{
  Object input=ImmutableMap.of("foo","//foo:bar","wiz",Arrays.asList(":bang"));
  try {
    BuildType.LABEL_LIST_DICT.convert(input,null,currentRule);
    fail();
  }
 catch (  Type.ConversionException e) {
    assertThat(e).hasMessage("expected value of type 'list(label)' for dict value element, " + "but got \"//foo:bar\" (string)");
  }
}
