{
  try {
    if (!Files.isDirectory(path)) {
      if (inValuesSubtree) {
        throw new UnsupportedOperationException();
      }
 else {
        overwritingResources.add(FileDataResource.fromPath(path,fqnFactory));
      }
    }
  }
 catch (  IllegalArgumentException e) {
    errors.add(e);
  }
  return super.visitFile(path,attrs);
}
