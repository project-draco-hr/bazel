{
  Map<DataKey,DataResource> overwritingResources=new HashMap<>();
  Map<DataKey,DataResource> nonOverwritingResources=new HashMap<>();
  final Map<DataKey,DataAsset> assets=new HashMap<>();
  Set<MergeConflict> conflicts=new HashSet<>();
  final ResourceFileVisitor resourceVisitor=new ResourceFileVisitor(conflicts,overwritingResources,nonOverwritingResources);
  return new AndroidDataSetBuildingPathWalker(conflicts,assets,overwritingResources,nonOverwritingResources,resourceVisitor);
}
