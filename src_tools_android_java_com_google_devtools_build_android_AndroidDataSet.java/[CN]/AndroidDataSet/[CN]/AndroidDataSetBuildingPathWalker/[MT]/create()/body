{
  List<DataResource> overwritingResources=new ArrayList<>();
  List<DataResource> nonOverwritingResources=new ArrayList<>();
  final List<DataAsset> assets=new ArrayList<>();
  final ResourceFileVisitor resourceVisitor=new ResourceFileVisitor(overwritingResources,nonOverwritingResources);
  return new AndroidDataSetBuildingPathWalker(assets,overwritingResources,nonOverwritingResources,resourceVisitor);
}
