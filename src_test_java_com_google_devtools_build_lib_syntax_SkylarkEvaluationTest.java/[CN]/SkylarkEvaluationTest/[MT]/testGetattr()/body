{
  exec(parseFileForSkylark("s = struct(a='val')\n" + "x = getattr(s, 'a')\n" + "y = getattr(s, 'b', 'def')\n"+ "z = getattr(s, 'b', default = 'def')\n"+ "w = getattr(s, 'a', default='ignored')"),env);
  assertEquals("val",env.lookup("x"));
  assertEquals("def",env.lookup("y"));
  assertEquals("def",env.lookup("z"));
  assertEquals("val",env.lookup("w"));
}
