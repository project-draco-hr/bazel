{
  ConfiguredTarget actual=(ConfiguredTarget)ruleContext.getPrerequisite("actual",Mode.TARGET);
  return new AliasConfiguredTarget(actual,ImmutableMap.of(AliasProvider.class,AliasProvider.fromAliasRule(ruleContext.getLabel(),actual),VisibilityProvider.class,new VisibilityProviderImpl(ruleContext.getVisibility())));
}
