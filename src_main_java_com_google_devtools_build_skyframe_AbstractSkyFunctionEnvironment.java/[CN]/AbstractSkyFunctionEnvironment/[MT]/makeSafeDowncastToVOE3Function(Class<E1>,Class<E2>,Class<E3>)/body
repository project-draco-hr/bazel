{
  return new Function<ValueOrException4<E1,E2,E3,BottomException>,ValueOrException3<E1,E2,E3>>(){
    @Override public ValueOrException3<E1,E2,E3> apply(    ValueOrException4<E1,E2,E3,BottomException> voe){
