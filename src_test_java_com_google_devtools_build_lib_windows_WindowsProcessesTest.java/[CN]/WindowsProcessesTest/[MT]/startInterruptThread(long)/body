{
  Thread thread=new Thread(new Runnable(){
    @Override public void run(){
      while (true) {
        Uninterruptibles.sleepUninterruptibly(delayMilliseconds,TimeUnit.MILLISECONDS);
        WindowsProcesses.nativeInterrupt(process);
      }
    }
  }
);
  thread.setDaemon(true);
  thread.start();
}
