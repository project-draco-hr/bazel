{
  process=WindowsProcesses.nativeCreateProcess("ThisExecutableDoesNotExist",null,null,null,null);
  assertThat(WindowsProcesses.nativeGetLastError(process)).contains("The system cannot find the file specified.");
  byte[] buf=new byte[1];
  assertThat(WindowsProcesses.nativeReadStdout(process,buf,0,1)).isEqualTo(-1);
}
