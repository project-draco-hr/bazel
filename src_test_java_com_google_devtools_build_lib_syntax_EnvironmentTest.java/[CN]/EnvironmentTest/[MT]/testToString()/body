{
  update("subject",new StringLiteral("Hello, 'world'.",'\''));
  update("from",new StringLiteral("Java",'"'));
  assertThat(getEnvironment().toString()).startsWith("Environment(lexicalFrame=null, " + "globalFrame=Frame[test]{\"from\": \"Java\", \"subject\": 'Hello, \\'world\\'.'}=>" + "(BUILD){");
}
