{
  update("foo","bar");
  update("wiz",3);
  Environment nestedEnv=new Environment(getEnvironment());
  nestedEnv.update("foo","bat");
  nestedEnv.update("quux",42);
  assertEquals(Sets.newHashSet("True","False","None","foo","wiz"),getEnvironment().getVariableNames());
  assertEquals(Sets.newHashSet("True","False","None","foo","wiz","quux"),nestedEnv.getVariableNames());
}
