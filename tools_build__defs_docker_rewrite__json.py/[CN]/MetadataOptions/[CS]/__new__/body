    'Constructor.'
    return super(MetadataOptions, cls).__new__(cls, name=name, parent=parent, size=size, entrypoint=entrypoint, cmd=cmd, user=user, labels=labels, env=env, ports=ports, volumes=volumes, workdir=workdir)
