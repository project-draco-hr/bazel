def __new__(cls, name=None, parent=None, size=None, entrypoint=None, cmd=None, env=None, ports=None, volumes=None, workdir=None):
    'Constructor.'
    return super(MetadataOptions, cls).__new__(cls, name=name, parent=parent, size=size, entrypoint=entrypoint, cmd=cmd, env=env, ports=ports, volumes=volumes, workdir=workdir)
