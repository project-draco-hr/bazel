{
  Path previousValue=get(key);
  if (previousValue == null) {
    return super.put(key,value);
  }
 else   if (previousValue.equals(value)) {
    return value;
  }
 else {
    throw new IllegalArgumentException(String.format("Cannot mount both '%s' and '%s' onto '%s'",previousValue,value,key));
  }
}
