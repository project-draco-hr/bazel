{
  if (!(target instanceof Rule)) {
    return null;
  }
  Rule rule=(Rule)target;
  if (!rule.getRuleClass().equals("filegroup")) {
    return null;
  }
  List<Label> labels=new StaticValuedAttributeMapper(rule).getAndValidate("srcs",Type.LABEL_LIST);
  if (labels.size() != 1) {
    return null;
  }
  return labels.get(0);
}
