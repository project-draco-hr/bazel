{
  Rule rule=createRule("a","myrule","sh_binary(name = 'myrule',","          srcs = select({","              '//conditions:a': ['a.sh'],","              '//conditions:b': ['b.sh'],","              '" + Type.Selector.DEFAULT_CONDITION_KEY + "': ['default.sh'],","          }))");
  assertSameContents(ImmutableList.of(ImmutableList.of(Label.create("a","a.sh")),ImmutableList.of(Label.create("a","b.sh")),ImmutableList.of(Label.create("a","default.sh"))),AggregatingAttributeMapper.of(rule).visitAttribute("srcs",Type.LABEL_LIST));
}
