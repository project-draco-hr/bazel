{
  try {
    for (    Path target : targetDirectory.getDirectoryEntries()) {
      RootedPath symlinkPath=RootedPath.toRootedPath(repositoryDirectory,new PathFragment(target.getBaseName()));
      if (createSymbolicLink(symlinkPath,target,env) == null) {
        return false;
      }
    }
  }
 catch (  IOException e) {
    throw new RepositoryFunctionException(e,Transience.TRANSIENT);
  }
  return true;
}
