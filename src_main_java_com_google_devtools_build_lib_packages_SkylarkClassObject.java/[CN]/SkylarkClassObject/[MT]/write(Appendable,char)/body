{
  boolean first=true;
  Printer.append(buffer,"struct(");
  for (  String key : Ordering.natural().sortedCopy(values.keySet())) {
    if (!first) {
      Printer.append(buffer,", ");
    }
    first=false;
    Printer.append(buffer,key);
    Printer.append(buffer," = ");
    Printer.write(buffer,values.get(key),quotationMark);
  }
  Printer.append(buffer,")");
}
