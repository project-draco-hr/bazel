{
  assertEquals("A",longestPathPrefixStr("A/b","A","B"));
  assertEquals("A",longestPathPrefixStr("A","A","B"));
  assertEquals("A/B",longestPathPrefixStr("A/B/c","A","A/B"));
  assertNull(longestPathPrefixStr("C/b","A","B"));
  assertNull(longestPathPrefixStr("A","A/B","B"));
  assertEquals("A/B/C",longestPathPrefixStr("A/B/C/d/e/f.h","A/B/C","B/C/d"));
}
