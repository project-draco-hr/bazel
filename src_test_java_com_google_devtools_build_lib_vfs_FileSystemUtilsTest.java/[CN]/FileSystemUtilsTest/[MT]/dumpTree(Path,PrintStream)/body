{
  out.println("\n" + root);
  for (  Path p : FileSystemUtils.traverseTree(root,Predicates.alwaysTrue())) {
    if (p.isDirectory(Symlinks.NOFOLLOW)) {
      out.println("  " + p + "/");
    }
 else     if (p.isSymbolicLink()) {
      out.println("  " + p + " => "+ p.readSymbolicLink());
    }
 else {
      out.println("  " + p + " ["+ p.resolveSymbolicLinks()+ "]");
    }
  }
}
