{
  if (attributeValue instanceof List<?>) {
    Attribute attr=getAttribute(attrIndex);
    if (attr.isNonEmpty() && ((List<?>)attributeValue).isEmpty()) {
      rule.reportError(rule.getLabel() + ": non empty " + "attribute '"+ attr.getName()+ "' in '"+ name+ "' rule '"+ rule.getLabel()+ "' has to have at least one value",eventHandler);
    }
  }
}
