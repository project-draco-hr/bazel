{
  return new Runnable(){
    @Override public void run(){
      while (true) {
        Optional<RunResponse> item;
        try {
          item=queue.take();
        }
 catch (        InterruptedException e) {
          continue;
        }
        if (!item.isPresent()) {
          return;
        }
        observer.onNext(item.get());
      }
    }
  }
;
}
