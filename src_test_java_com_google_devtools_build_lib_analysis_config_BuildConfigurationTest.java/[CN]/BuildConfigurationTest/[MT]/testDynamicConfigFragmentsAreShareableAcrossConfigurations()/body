{
  BuildConfiguration config1=create("--experimental_dynamic_configs","--javacopt=foo");
  BuildConfiguration config2=create("--experimental_dynamic_configs","--javacopt=bar");
  BuildConfiguration config3=create("--experimental_dynamic_configs","--j2objc_translation_flags=baz");
  assertThat(config1.getFragment(J2ObjcConfiguration.class)).isSameAs(config2.getFragment(J2ObjcConfiguration.class));
  assertThat(config1.getFragment(J2ObjcConfiguration.class)).isNotSameAs(config3.getFragment(J2ObjcConfiguration.class));
}
