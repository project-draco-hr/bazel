{
  return new CcLinkParamsStore(){
    @Override protected void collect(    Builder builder,    boolean linkingStatically,    boolean linkShared){
      if (legacyCollectCppAndJavaLinkOptions) {
        builder.addTransitiveTargets(deps,JavaCcLinkParamsProvider.TO_LINK_PARAMS);
        builder.addTransitiveTargets(deps,CcLinkParamsProvider.TO_LINK_PARAMS,CcSpecificLinkParamsProvider.TO_LINK_PARAMS);
      }
 else {
        builder.addTransitiveTargets(deps,JavaCcLinkParamsProvider.TO_LINK_PARAMS,CcLinkParamsProvider.TO_LINK_PARAMS,CcSpecificLinkParamsProvider.TO_LINK_PARAMS);
      }
    }
  }
;
}
