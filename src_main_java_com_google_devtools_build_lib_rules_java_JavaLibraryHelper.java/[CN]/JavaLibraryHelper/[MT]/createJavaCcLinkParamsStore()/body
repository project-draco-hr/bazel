{
  return new CcLinkParamsStore(){
    @Override protected void collect(    Builder builder,    boolean linkingStatically,    boolean linkShared){
      builder.addTransitiveLangTargets(deps,JavaCcLinkParamsProvider.TO_LINK_PARAMS);
      builder.addTransitiveTargets(deps);
      builder.addTransitiveLangTargets(deps,CcSpecificLinkParamsProvider.TO_LINK_PARAMS);
    }
  }
;
}
