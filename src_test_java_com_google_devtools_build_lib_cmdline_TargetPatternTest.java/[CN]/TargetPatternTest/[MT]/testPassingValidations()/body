{
  parse("foo:bar");
  parse("foo:all");
  parse("foo/...:all");
  parse("foo:*");
  parse("//foo");
  parse("//foo:bar");
  parse("//foo:all");
  parse("//foo/all");
  parse("java/com/google/foo/Bar.java");
  parse("//foo/...:all");
  parse("//...");
  parse("@repo//foo:bar");
  parse("@repo//foo:all");
  parse("@repo//:bar");
}
