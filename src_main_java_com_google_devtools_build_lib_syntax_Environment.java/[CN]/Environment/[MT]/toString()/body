{
  StringBuilder out=new StringBuilder();
  out.append("Environment{");
  List<String> keys=new ArrayList<>(env.keySet());
  Collections.sort(keys);
  for (  String key : keys) {
    out.append(key).append(" -> ").append(env.get(key)).append(", ");
  }
  out.append("}");
  if (parent != null) {
    out.append("=>");
    out.append(parent);
  }
  return out.toString();
}
