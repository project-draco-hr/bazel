{
  PathPackageLocator pkgLocator=PathPackageLocator.create(null,packageCacheOptions.packagePath,reporter,rootDirectory,rootDirectory);
  skyframeExecutor.preparePackageLoading(pkgLocator,packageCacheOptions.defaultVisibility,true,7,ruleClassProvider.getDefaultsPackageContent(TestConstants.TEST_INVOCATION_POLICY),UUID.randomUUID(),new TimestampGranularityMonitor(BlazeClock.instance()));
  skyframeExecutor.setDeletedPackages(ImmutableSet.copyOf(packageCacheOptions.getDeletedPackages()));
}
