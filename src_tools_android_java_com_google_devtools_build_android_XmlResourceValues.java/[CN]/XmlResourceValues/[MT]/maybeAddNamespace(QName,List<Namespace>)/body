{
  if (name.getNamespaceURI() != null && !name.getNamespaceURI().isEmpty() && !(XLIFF_NAMESPACE.equals(name.getNamespaceURI()) && XLIFF_PREFIX.equals(name.getPrefix()))) {
    if (namespaces == null) {
      namespaces=new ArrayList<>();
    }
    namespaces.add(XML_EVENT_FACTORY.createNamespace(name.getPrefix(),name.getNamespaceURI()));
  }
  return namespaces;
}
