{
  String expandedDoc=htmlDocumentation;
  for (  Entry<String,String> docVariable : docVariables.entrySet()) {
    expandedDoc=expandedDoc.replace("${" + docVariable.getKey() + "}",expandBuiltInVariables(docVariable.getKey(),docVariable.getValue()));
  }
  expandedDoc=expandedDoc.replace("${" + DocgenConsts.VAR_ATTRIBUTE_SIGNATURE + "}",generateAttributeSignatures());
  expandedDoc=expandedDoc.replace("${" + DocgenConsts.VAR_ATTRIBUTE_DEFINITION + "}",generateAttributeDefinitions(true));
  return String.format("<h3 id=\"%s\"%s>%s</h3>\n\n%s",ruleName,getDeprecatedString(hasFlag(DocgenConsts.FLAG_DEPRECATED)),ruleName,expandedDoc);
}
