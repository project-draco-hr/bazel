__pychecker__ = 'no-local'
import gflags
from flags_modules_for_testing import module_bar
FLAGS = gflags.FLAGS
DECLARED_KEY_FLAGS = ['tmod_bar_x', 'tmod_bar_z', 'tmod_bar_t', 'flagfile']
