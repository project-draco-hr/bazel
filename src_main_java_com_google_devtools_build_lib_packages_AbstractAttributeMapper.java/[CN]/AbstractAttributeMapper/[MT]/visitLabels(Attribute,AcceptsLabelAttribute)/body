{
  Type<?> type=attribute.getType();
  Object value=get(attribute.getName(),type);
  if (value != null) {
    for (    Label label : extractLabels(type,value)) {
      Label absoluteLabel;
      if (attribute.isImplicit() || attribute.isLateBound() || !attributes.isAttributeValueExplicitlySpecified(attribute)) {
        absoluteLabel=label;
      }
 else       if (label.getPackageIdentifier().getRepository().isDefault() && (VISIBILITY.equals(label.getPackageIdentifier().getPackageFragment()) || EXTERNAL.equals(label.getPackageIdentifier().getPackageFragment()))) {
        absoluteLabel=label;
      }
 else {
        absoluteLabel=ruleLabel.resolveRepositoryRelative(label);
      }
      observer.acceptLabelAttribute(absoluteLabel,attribute);
    }
  }
}
