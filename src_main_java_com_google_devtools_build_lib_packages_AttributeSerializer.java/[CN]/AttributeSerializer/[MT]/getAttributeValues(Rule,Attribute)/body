{
  if (attr.getName().equals("visibility")) {
    List<Object> result=new ArrayList<>(1);
    result.add(rule.getVisibility().getDeclaredLabels());
    return result;
  }
 else {
    return Lists.<Object>newArrayList(AggregatingAttributeMapper.of(rule).visitAttribute(attr.getName(),attr.getType()));
  }
}
