{
  InputStream stream=IntelliJSkylarkAspectTest.class.getResourceAsStream("intellij_info.bzl");
  BufferedReader reader=new BufferedReader(new InputStreamReader(stream,StandardCharsets.UTF_8));
  String line;
  ArrayList<String> contents=new ArrayList<>();
  while ((line=reader.readLine()) != null) {
    contents.add(line);
  }
  scratch.file("intellij_tools/BUILD","# empty");
  scratch.file("intellij_tools/intellij_info.bzl",contents.toArray(new String[contents.size()]));
}
