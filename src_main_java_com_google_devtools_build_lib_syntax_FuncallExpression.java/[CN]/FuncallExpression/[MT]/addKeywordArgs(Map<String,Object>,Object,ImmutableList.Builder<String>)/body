{
  if (!(items instanceof Map<?,?>)) {
    throw new EvalException(getLocation(),"Argument after ** must be a dictionary, not " + EvalUtils.getDataTypeName(items));
  }
  for (  Map.Entry<?,?> entry : ((Map<?,?>)items).entrySet()) {
    if (!(entry.getKey() instanceof String)) {
      throw new EvalException(getLocation(),"Keywords must be strings, not " + EvalUtils.getDataTypeName(entry.getKey()));
    }
    addKeywordArg(kwargs,(String)entry.getKey(),entry.getValue(),duplicates);
  }
}
