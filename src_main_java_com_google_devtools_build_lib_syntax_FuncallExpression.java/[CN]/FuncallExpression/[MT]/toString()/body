{
  if (func.getName().equals("$slice")) {
    return obj + "[" + args.get(0)+ ":"+ args.get(1)+ "]";
  }
  if (func.getName().equals("$index")) {
    return obj + "[" + args.get(0)+ "]";
  }
  StringBuilder sb=new StringBuilder();
  if (obj != null) {
    sb.append(obj).append(".");
  }
  sb.append(func);
  Printer.printList(sb,args,"(",", ",")",null,Printer.SUGGESTED_CRITICAL_LIST_ELEMENTS_COUNT,Printer.SUGGESTED_CRITICAL_LIST_ELEMENTS_STRING_LENGTH);
  return sb.toString();
}
