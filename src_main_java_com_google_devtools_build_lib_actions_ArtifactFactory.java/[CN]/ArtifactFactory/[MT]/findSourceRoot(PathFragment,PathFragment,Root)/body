{
  PathFragment dir;
  for (dir=execPath.getParentDirectory(); dir != null && !dir.equals(baseExecPath); dir=dir.getParentDirectory()) {
    Root sourceRoot=packageRoots.get(PackageIdentifier.createInDefaultRepo(dir));
    if (sourceRoot != null) {
      return sourceRoot;
    }
  }
  return dir != null && dir.equals(baseExecPath) ? baseRoot : null;
}
