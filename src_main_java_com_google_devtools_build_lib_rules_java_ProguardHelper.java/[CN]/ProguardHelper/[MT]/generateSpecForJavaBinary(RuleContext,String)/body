{
  Artifact result=ProguardHelper.getProguardConfigArtifact(ruleContext,"jvm");
  ruleContext.registerAction(new FileWriteAction(ruleContext.getActionOwner(),result,String.format("-libraryjars <java.home>/lib/rt.jar%n" + "-keep class %s {%n" + "  public static void main(java.lang.String[]);%n"+ "}",mainClassName),false));
  return result;
}
