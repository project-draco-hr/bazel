{
  Artifact result=ProguardHelper.getProguardConfigArtifact(ruleContext,"jvm");
  ruleContext.registerAction(new FileWriteAction(ruleContext.getActionOwner(),result,String.format("-libraryjars %s%n" + "-keep class %s {%n" + "  public static void main(java.lang.String[]);%n"+ "}",Artifact.joinExecPaths(ruleContext.getConfiguration().getHostPathSeparator(),bootclasspath),mainClassName),false));
  return result;
}
