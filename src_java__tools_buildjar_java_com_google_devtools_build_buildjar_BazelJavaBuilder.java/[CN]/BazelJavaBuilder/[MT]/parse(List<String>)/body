{
  ImmutableList<BlazeJavaCompilerPlugin> plugins=ImmutableList.<BlazeJavaCompilerPlugin>of(new FileManagerInitializationPlugin(),new ErrorPronePlugin());
  JavaLibraryBuildRequest build=new JavaLibraryBuildRequest(args,plugins,new DependencyModule.Builder());
  build.setJavacOpts(JavacOptions.normalizeOptions(build.getJavacOpts()));
  return build;
}
