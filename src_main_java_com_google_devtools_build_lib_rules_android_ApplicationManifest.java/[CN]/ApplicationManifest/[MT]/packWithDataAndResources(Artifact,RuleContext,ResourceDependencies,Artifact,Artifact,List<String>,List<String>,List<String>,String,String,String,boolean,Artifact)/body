{
  LocalResourceContainer data=new LocalResourceContainer.Builder(ruleContext).withAssets(AndroidCommon.getAssetDir(ruleContext),ruleContext.getPrerequisites(ResourceType.ASSETS.getAttribute(),Mode.TARGET,FileProvider.class)).withResources(ruleContext.getPrerequisites("resource_files",Mode.TARGET,FileProvider.class)).build();
  if (ruleContext.hasErrors()) {
    return null;
  }
  return createApk(resourceApk,ruleContext,resourceDeps,rTxt,symbolsTxt,configurationFilters,uncompressedExtensions,densities,applicationId,versionCode,versionName,incremental,data,proguardCfg);
}
