{
  TransitiveInfoCollection resourcesPrerequisite=ruleContext.getPrerequisite("resources",Mode.TARGET);
  if (resourcesPrerequisite != null) {
    ResourceContainer resourceContainer=Iterables.getOnlyElement(resourcesPrerequisite.getProvider(AndroidResourcesProvider.class).getTransitiveAndroidResources());
    return resourceContainer.getRenameManifestPackage();
  }
 else {
    return null;
  }
}
