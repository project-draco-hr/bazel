{
  LocalResourceContainer data=new LocalResourceContainer.Builder(ruleContext).withAssets(AndroidCommon.getAssetDir(ruleContext),ruleContext.getPrerequisites(ResourceType.ASSETS.getAttribute(),Mode.TARGET,FileProvider.class)).build();
