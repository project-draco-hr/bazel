{
  FileSystemUtils.writeIsoLatin1(workspacePath,"bind(","    name = 'my-rule',","    actual = '//foo:bar',",")","new_local_repository(","    name = 'my-rule',","    path = '/foo/bar',","    build_file = 'baz',",")");
  invalidatePackages();
  assertEquals("new_local_repository rule",getTarget("//external:my-rule").getTargetKind());
}
