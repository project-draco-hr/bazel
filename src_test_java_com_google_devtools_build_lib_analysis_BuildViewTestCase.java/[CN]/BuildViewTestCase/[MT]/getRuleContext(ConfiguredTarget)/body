{
  return new RuleContext.Builder(new StubAnalysisEnvironment(),(Rule)target.getTarget(),target.getConfiguration(),ruleClassProvider.getPrerequisiteValidator()).setVisibility(NestedSetBuilder.<PackageSpecification>create(Order.STABLE_ORDER,PackageSpecification.EVERYTHING)).setPrerequisites(view.getPrerequisiteMapForTesting(target)).setConfigConditions(ImmutableSet.<ConfigMatchingProvider>of()).build();
}
