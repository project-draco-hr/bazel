{
  createMap();
  map.put("foo","bar");
  map.put("baz","bang");
  assertEquals("bar",map.get("foo"));
  assertEquals("bang",map.get("baz"));
  assertEquals(2,map.size());
  long size=map.save();
  assertEquals(mapFile.getFileSize(),size);
  assertEquals("bar",map.get("foo"));
  assertEquals("bang",map.get("baz"));
  assertEquals(2,map.size());
  createMap();
  assertEquals("bar",map.get("foo"));
  assertEquals("bang",map.get("baz"));
  assertEquals(2,map.size());
}
