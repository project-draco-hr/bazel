{
  createMap();
  map.put("foo","bar");
  map.put("baz","bang");
  map.save();
  assertTrue(mapFile.exists());
  assertFalse(journalFile.exists());
  map.clear();
  assertEquals(0,map.size());
  assertTrue(mapFile.exists());
  assertFalse(journalFile.exists());
  createMap();
  assertEquals(0,map.size());
}
