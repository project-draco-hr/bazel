{
  createMap();
  map.put("foo","bar");
  map.put("baz","bang");
  map.save();
  assertTrue(mapFile.exists());
  assertFalse(journalFile.exists());
  map.clear();
  assertThat(map).isEmpty();
  assertTrue(mapFile.exists());
  assertFalse(journalFile.exists());
  createMap();
  assertThat(map).isEmpty();
}
