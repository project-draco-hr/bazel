{
  createMap();
  map.put("foo","bar");
  map.put("baz","bang");
  long size=map.save();
  assertEquals(mapFile.getFileSize(),size);
  assertFalse(journalFile.exists());
  map.remove("foo");
  assertEquals(1,map.size());
  assertTrue(journalFile.exists());
  createMap();
  assertEquals(1,map.size());
}
