{
  createMap();
  map.put("foo","bar");
  map.save();
  assertFalse(journalFile.exists());
  map.put("baz","bang");
  assertThat(map).hasSize(2);
  assertTrue(journalFile.exists());
  createMap();
  assertThat(map).hasSize(2);
  map.put("baz2","bang2");
  assertThat(map).hasSize(3);
  assertTrue(journalFile.exists());
  createMap();
  assertThat(map).hasSize(3);
}
