{
  createMap();
  map.put("foo","bar");
  map.save();
  assertFalse(journalFile.exists());
  map.put("baz","bang");
  assertEquals(2,map.size());
  assertTrue(journalFile.exists());
  createMap();
  assertEquals(2,map.size());
  map.put("baz2","bang2");
  assertEquals(3,map.size());
  assertTrue(journalFile.exists());
  createMap();
  assertEquals(3,map.size());
}
