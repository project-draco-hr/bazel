{
  return new ImmutableList.Builder<String>().add(args.subtoolCmd()).add("--errors").add("--warnings").add("--notices").add("--auto-activate-custom-fonts").add("--output-format").add("human-readable-text").add("--compile").add(Wrappers.CANONICALIZE_IF_PATH.apply(outputDirectory)).addAll(Iterables.transform(args.subtoolExtraArgs(),Wrappers.CANONICALIZE_IF_PATH)).build();
}
