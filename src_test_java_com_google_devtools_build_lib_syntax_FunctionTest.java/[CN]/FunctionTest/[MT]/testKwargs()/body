{
  List<Statement> input=parseFileForSkylark("def foo(a, b = 'b', c = 'c'):\n" + "  return a + b + c\n" + "args = {'a': 'x', 'c': 'z'}\n"+ "v1 = foo(**args)\n"+ "v2 = foo('x', **{'b': 'y'})\n"+ "v3 = foo(c = 'z', a = 'x', **{'b': 'y'})");
  exec(input,env);
  assertEquals("xbz",env.lookup("v1"));
  assertEquals("xyc",env.lookup("v2"));
  assertEquals("xyz",env.lookup("v3"));
}
