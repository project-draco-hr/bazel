{
  List<Statement> input=parseFileForSkylark("def f(name, value = '1', optional = '2'): return name + value + optional\n" + "v1 = f(*['name', 'value'])\n" + "v2 = f('0', *['name', 'value'])\n"+ "v3 = f('0', *['b'], optional = '3')\n"+ "v4 = f(*[],name='a')\n");
  exec(input,env);
  assertEquals("namevalue2",env.lookup("v1"));
  assertEquals("0namevalue",env.lookup("v2"));
  assertEquals("0b3",env.lookup("v3"));
  assertEquals("a12",env.lookup("v4"));
}
