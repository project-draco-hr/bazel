{
  List<Statement> input=parseFileForSkylark(functionWithOptionalArgs() + "v1 = func('1', 1, 1)\n" + "v2 = func(b = 2, a = '2', c = 2)\n"+ "v3 = func('3')\n"+ "v4 = func('4', c = 1)\n");
  exec(input,env);
  assertEquals("1abc",env.lookup("v1"));
  assertEquals("2abc",env.lookup("v2"));
  assertEquals("3a",env.lookup("v3"));
  assertEquals("4ac",env.lookup("v4"));
}
