{
  List<Statement> input=parseFileForSkylark("def func(a, b = 'b', c = 'c'):\n" + "  return a + b + c\n" + "v1 = func('a', 'x', 'y')\n"+ "v2 = func(b = 'x', a = 'a', c = 'y')\n"+ "v3 = func('a')\n"+ "v4 = func('a', c = 'y')\n");
  exec(input,env);
  assertEquals("axy",env.lookup("v1"));
  assertEquals("axy",env.lookup("v2"));
  assertEquals("abc",env.lookup("v3"));
  assertEquals("aby",env.lookup("v4"));
}
