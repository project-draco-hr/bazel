{
  usedCpu-=resources.getCpuUsage();
  usedRam-=resources.getMemoryMb();
  usedIo-=resources.getIoUsage();
  usedLocalTestCount-=resources.getLocalTestCount();
  double epsilon=0.0001;
  if (usedCpu < epsilon) {
    usedCpu=0;
  }
  if (usedRam < epsilon) {
    usedRam=0;
  }
  if (usedIo < epsilon) {
    usedIo=0;
  }
  if (requestList.size() > 0) {
    processWaitingThreads();
    return true;
  }
  return false;
}
