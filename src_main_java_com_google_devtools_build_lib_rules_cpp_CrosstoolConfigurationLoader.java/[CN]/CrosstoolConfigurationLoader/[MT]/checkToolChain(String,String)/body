{
  if (selectedIdentifier == null) {
    throw new InvalidConfigurationException("No toolchain found for cpu '" + cpu + "'");
  }
  String rx="[a-zA-Z_][\\.\\- \\w]*";
  if (!selectedIdentifier.matches(rx)) {
    throw new InvalidConfigurationException("Toolchain identifier for cpu '" + cpu + "' "+ "is illegal (does not match '"+ rx+ "')");
  }
}
