{
  try {
    clearProgressBar();
    if (debugAllEvents) {
      terminal.flush();
      outErr.getOutputStream().write((event + "\n").getBytes(StandardCharsets.UTF_8));
      outErr.getOutputStream().flush();
    }
    addProgressBar();
    terminal.flush();
  }
 catch (  IOException e) {
    LOG.warning("IO Error writing to output stream: " + e);
  }
}
