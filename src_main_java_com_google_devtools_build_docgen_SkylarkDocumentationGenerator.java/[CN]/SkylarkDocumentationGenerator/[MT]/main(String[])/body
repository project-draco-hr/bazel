{
  if (checkArgs(args)) {
    System.out.println("Generating Skylark documentation...");
    SkylarkDocumentationProcessor processor=new SkylarkDocumentationProcessor();
    try {
      processor.generateDocumentation(args[0]);
    }
 catch (    Throwable e) {
      fail(e,true);
    }
    System.out.println("Finished.");
  }
}
