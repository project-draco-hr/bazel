{
  ListComprehension list=(ListComprehension)parseExpr("['foo/%s.java' % x " + "for x in []]");
  assertThat(list.getLists()).hasSize(1);
  list=(ListComprehension)parseExpr("['foo/%s.java' % x " + "for x in ['bar', 'wiz', 'quux']]");
  assertThat(list.getLists()).hasSize(1);
  list=(ListComprehension)parseExpr("['%s/%s.java' % (x, y) " + "for x in ['foo', 'bar'] for y in ['baz', 'wiz', 'quux']]");
  assertThat(list.getLists()).hasSize(2);
}
