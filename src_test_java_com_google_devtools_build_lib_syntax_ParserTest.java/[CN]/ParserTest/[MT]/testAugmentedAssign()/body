{
  assertEquals("[x = x + 1\n]",parseFile("x += 1").toString());
  assertEquals("[x = x - 1\n]",parseFile("x -= 1").toString());
  assertEquals("[x = x * 1\n]",parseFile("x *= 1").toString());
  assertEquals("[x = x / 1\n]",parseFile("x /= 1").toString());
  assertEquals("[x = x % 1\n]",parseFile("x %= 1").toString());
}
