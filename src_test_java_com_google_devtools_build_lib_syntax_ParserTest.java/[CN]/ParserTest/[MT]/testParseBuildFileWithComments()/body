{
  Parser.ParseResult result=parseFileWithComments("# Test BUILD file","# with multi-line comment","","genrule(name = 'foo',","   srcs = ['input.csv'],","   outs = [ 'result.txt',","           'result.log'],","   cmd = 'touch result.txt result.log')");
  assertThat(result.statements).hasSize(1);
  assertThat(result.comments).hasSize(2);
}
