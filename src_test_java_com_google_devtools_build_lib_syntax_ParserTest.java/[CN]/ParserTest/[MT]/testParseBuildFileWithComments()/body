{
  Parser.ParseResult result=parseFileWithComments("# Test BUILD file" + '\n' + "# with multi-line comment"+ '\n'+ ""+ '\n'+ "genrule(name = 'foo',"+ '\n'+ "   srcs = ['input.csv'],"+ '\n'+ "   outs = [ 'result.txt',"+ '\n'+ "           'result.log'],"+ '\n'+ "   cmd = 'touch result.txt result.log')"+ '\n');
  assertThat(result.statements).hasSize(1);
  assertThat(result.comments).hasSize(2);
}
