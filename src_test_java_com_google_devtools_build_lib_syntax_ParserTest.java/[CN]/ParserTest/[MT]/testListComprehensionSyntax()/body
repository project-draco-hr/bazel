{
  setFailFast(false);
  parseExpression("[x for");
  assertContainsEvent("syntax error at 'newline'");
  clearEvents();
  parseExpression("[x for x");
  assertContainsEvent("syntax error at 'newline'");
  clearEvents();
  parseExpression("[x for x in");
  assertContainsEvent("syntax error at 'newline'");
  clearEvents();
  parseExpression("[x for x in []");
  assertContainsEvent("syntax error at 'newline'");
  clearEvents();
  parseExpression("[x for x for y in ['a']]");
  assertContainsEvent("syntax error at 'for'");
  clearEvents();
}
