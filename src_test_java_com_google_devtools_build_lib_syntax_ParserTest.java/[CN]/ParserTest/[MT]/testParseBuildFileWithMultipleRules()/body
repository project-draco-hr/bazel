{
  List<Statement> stmts=parseFile("genrule(name = 'foo'," + '\n' + "   srcs = ['input.csv'],"+ '\n'+ "   outs = [ 'result.txt',"+ '\n'+ "           'result.log'],"+ '\n'+ "   cmd = 'touch result.txt result.log')"+ '\n'+ ""+ '\n'+ "genrule(name = 'bar',"+ '\n'+ "   srcs = ['input.csv'],"+ '\n'+ "   outs = [ 'graph.svg'],"+ '\n'+ "   cmd = 'touch graph.svg')"+ '\n');
  assertThat(stmts).hasSize(2);
}
