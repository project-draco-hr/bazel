{
  Path root=fileSystem.getPath("/src");
  ImmutableMap<PackageIdentifier,Path> packageRootMap=ImmutableMap.<PackageIdentifier,Path>builder().put(Label.EXTERNAL_PACKAGE_IDENTIFIER,root).build();
  SymlinkForest.builder().setLegacyExternalRunfiles(false).setPackageRoots(packageRootMap).setWorkspace(linkRoot).setProductName("mock-name").setWorkspaceName("wsname").build().plantSymlinkForest();
  assertThat(linkRoot.getRelative(Label.EXTERNAL_PACKAGE_NAME).exists()).isFalse();
}
