{
  return new CustomCommandLine.Builder().add(attributes.getProtoCompiler().getExecPathString()).add("--input-file-list").add(protoInputsFile.getExecPathString()).add("--output-dir").add(getWorkspaceRelativeOutputDir().getSafePathString()).add("--force").add("--proto-root-dir").add(".").addBeforeEachExecPath("--config",getPortableProtoFilters()).build();
}
