{
  Rule rule=createRule("x","myrule","sh_binary(","    name = 'myrule',","    srcs = select({","        '//conditions:a': ['a.sh', 'b.sh'],","        '//conditions:b': ['b.sh', 'c.sh'],","    }))");
  RawAttributeMapper rawMapper=RawAttributeMapper.of(rule);
  assertThat(rawMapper.getMergedValues("srcs",Type.LABEL_LIST)).containsExactly(Label.parseAbsolute("//x:a.sh"),Label.parseAbsolute("//x:b.sh"),Label.parseAbsolute("//x:c.sh")).inOrder();
}
