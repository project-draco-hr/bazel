{
  Iterable<Artifact> generatedSourceFiles=checkShouldCreateSources(ruleContext) ? ProtoCommon.getGeneratedOutputs(ruleContext,protoSources,".j2objc.pb.m") : ImmutableList.<Artifact>of();
  PathFragment objcFileRootExecPath=ruleContext.getConfiguration().getGenfilesDirectory(ruleContext.getRule().getRepository()).getExecPath();
  Iterable<PathFragment> headerSearchPaths=J2ObjcLibrary.j2objcSourceHeaderSearchPaths(ruleContext,objcFileRootExecPath,protoSources);
  return new J2ObjcSource(ruleContext.getTarget().getLabel(),generatedSourceFiles,ProtoCommon.getGeneratedOutputs(ruleContext,protoSources,".j2objc.pb.h"),objcFileRootExecPath,SourceType.PROTO,headerSearchPaths);
}
