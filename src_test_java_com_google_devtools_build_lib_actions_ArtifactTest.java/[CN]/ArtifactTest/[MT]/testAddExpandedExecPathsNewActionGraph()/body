{
  List<PathFragment> paths=new ArrayList<>();
  MutableActionGraph actionGraph=new MapBasedActionGraph();
  Artifact.addExpandedExecPaths(getFooBarArtifacts(actionGraph,true),paths,ActionInputHelper.actionGraphMiddlemanExpander(actionGraph));
  assertSameContents(ImmutableList.of(new PathFragment("bar1.h"),new PathFragment("bar2.h"),new PathFragment("bar3.h")),paths);
}
