{
  super(xcodeprojDirectory);
  checkArgument(transformer.name(xcodeprojDirectory).endsWith(XCODEPROJ_DIRECTORY_SUFFIX),"xcodeprojDirectory should end with %s, but it is '%s'",XCODEPROJ_DIRECTORY_SUFFIX,xcodeprojDirectory);
  this.xcodeprojDirectory=xcodeprojDirectory;
  this.transformer=transformer;
}
