def main(unused_argv):
    CreateDeb(FLAGS.output, FLAGS.data, preinst=GetFlagValue(FLAGS.preinst, False), postinst=GetFlagValue(FLAGS.postinst, False), prerm=GetFlagValue(FLAGS.prerm, False), postrm=GetFlagValue(FLAGS.postrm, False), package=FLAGS.package, version=GetFlagValue(FLAGS.version), description=GetFlagValue(FLAGS.description), maintainer=FLAGS.maintainer, section=FLAGS.section, architecture=FLAGS.architecture, depends=FLAGS.depends, suggests=FLAGS.suggests, enhances=FLAGS.enhances, preDepends=FLAGS.pre_depends, recommends=FLAGS.recommends, homepage=FLAGS.homepage, builtUsing=GetFlagValue(FLAGS.built_using), priority=FLAGS.priority, conflicts=FLAGS.conflicts, installedSize=GetFlagValue(FLAGS.installed_size))
    CreateChanges(FLAGS.changes, FLAGS.output, architecture=FLAGS.architecture, short_description=GetFlagValue(FLAGS.description).split('\n')[0], maintainer=FLAGS.maintainer, package=FLAGS.package, version=GetFlagValue(FLAGS.version), section=FLAGS.section, priority=FLAGS.priority)
