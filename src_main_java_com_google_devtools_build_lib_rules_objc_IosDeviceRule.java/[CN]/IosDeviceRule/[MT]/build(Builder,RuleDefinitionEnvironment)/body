{
  return builder.requiresConfigurationFragments(AppleConfiguration.class).add(attr(IOS_VERSION_ATTR_NAME,STRING)).add(attr(XCODE_ATTR_NAME,LABEL).allowedRuleClasses("xcode_version").allowedFileTypes()).add(attr(TYPE_ATTR_NAME,STRING).mandatory()).add(attr(LOCALE_ATTR_NAME,STRING).undocumented("this is not yet supported by any test runner").value("en")).build();
}
