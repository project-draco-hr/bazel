{
  return builder.override(attr("tags",Type.STRING_LIST).value(ImmutableList.of("manual")).nonconfigurable("low-level attribute, used in TargetUtils without configurations")).removeAttribute(RuleClass.COMPATIBLE_ENVIRONMENT_ATTR).removeAttribute(RuleClass.RESTRICTED_ENVIRONMENT_ATTR).exemptFromConstraintChecking("this rule *defines* a constraint").setUndocumented().build();
