{
  AggregatingAttributeMapper mapper=AggregatingAttributeMapper.of(rule);
  boolean executable=(mapper.has("executable",Type.BOOLEAN) && mapper.get("executable",Type.BOOLEAN));
  return DecompressorValue.key(FileFunction.NAME,DecompressorDescriptor.builder().setTargetKind(rule.getTargetKind()).setTargetName(rule.getName()).setArchivePath(downloadPath).setRepositoryPath(outputDirectory).setExecutable(executable).build());
}
