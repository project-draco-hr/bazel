{
  final List<Set<Node<T>>> sccs=new ArrayList<Set<Node<T>>>();
  NodeSetReceiver<T> r=new NodeSetReceiver<T>(){
    @Override public void accept(    Set<Node<T>> scc){
      sccs.add(scc);
    }
  }
;
  SccVisitor<T> v=new SccVisitor<T>();
  for (  Node<T> node : nodes.values()) {
    v.visit(r,node);
  }
  return sccs;
}
