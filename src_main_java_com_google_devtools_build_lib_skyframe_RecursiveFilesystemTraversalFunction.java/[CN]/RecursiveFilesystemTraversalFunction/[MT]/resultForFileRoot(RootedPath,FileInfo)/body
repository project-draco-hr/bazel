{
  Preconditions.checkState(info.type.isFile() && info.type.exists(),"{%s} {%s}",path,info.type);
  if (info.type.isSymlink()) {
    return RecursiveFilesystemTraversalValue.of(ResolvedFile.symlinkToFile(info.realPath,path,info.unresolvedSymlinkTarget,info.metadata));
  }
 else {
    return RecursiveFilesystemTraversalValue.of(ResolvedFile.regularFile(path,info.metadata));
  }
}
