def Footer(self):
    self.fp.write(('.SH COPYRIGHT\nCopyright \\(co %s Google.\n' % time.strftime('%Y', self.info.date)))
    self.fp.write(('Gflags2man created this page from "%s %s" output.\n' % (self.info.name, FLAGS.help_flag)))
    self.fp.write(('\nGflags2man was written by Dan Christian.  Note that the date on this page is the modification date of %s.\n' % self.info.name))
