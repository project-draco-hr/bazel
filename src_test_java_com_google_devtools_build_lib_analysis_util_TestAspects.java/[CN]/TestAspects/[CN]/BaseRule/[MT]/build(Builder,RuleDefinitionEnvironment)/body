{
  return builder.add(attr("testonly",BOOLEAN).nonconfigurable("test").value(false)).add(attr("deprecation",STRING).nonconfigurable("test")).add(attr("tags",STRING_LIST)).add(attr("visibility",NODEP_LABEL_LIST).orderIndependent().cfg(HOST).nonconfigurable("test")).add(attr(RuleClass.COMPATIBLE_ENVIRONMENT_ATTR,LABEL_LIST).allowedFileTypes(FileTypeSet.NO_FILE)).add(attr(RuleClass.RESTRICTED_ENVIRONMENT_ATTR,LABEL_LIST).allowedFileTypes(FileTypeSet.NO_FILE)).build();
}
