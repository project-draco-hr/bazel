{
  return builder.add(attr("hdrs",LABEL_LIST).orderIndependent().direct_compile_time_input().allowedFileTypes(FileTypeSet.ANY_FILE)).add(attr("textual_hdrs",LABEL_LIST).orderIndependent().direct_compile_time_input().legacyAllowAnyFileType()).add(attr("linkstamp",LABEL).allowedFileTypes(CPP_SOURCE,C_SOURCE)).build();
}
