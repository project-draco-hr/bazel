{
  backgroundWorkers.execute(new Runnable(){
    @Override public void run(){
      try {
        while (!Thread.currentThread().isInterrupted()) {
          try {
            runner.cleanup();
            return;
          }
 catch (          IOException e2) {
            Thread.sleep(250);
          }
        }
      }
 catch (      InterruptedException e) {
        Thread.currentThread().interrupt();
      }
    }
  }
);
}
