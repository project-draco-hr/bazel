{
  ActionExecutionContext actionExecutionContext=ActionExecutionContext.forInputDiscovery(executorEngine,new DelegatingPairFileCache(graphFileCache,perBuildFileCache),metadataHandler,actionLogBufferPathGenerator.generate(),env);
  try {
    return action.discoverInputs(actionExecutionContext);
  }
 catch (  ActionExecutionException e) {
    throw processAndThrow(e,action,actionExecutionContext.getFileOutErr());
  }
}
