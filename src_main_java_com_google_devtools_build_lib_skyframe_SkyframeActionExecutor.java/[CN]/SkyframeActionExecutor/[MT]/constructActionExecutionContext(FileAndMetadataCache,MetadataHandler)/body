{
  FileOutErr fileOutErr=actionLogBufferPathGenerator.generate();
  return new ActionExecutionContext(executorEngine,new DelegatingPairFileCache(graphFileCache,perBuildFileCache),metadataHandler,fileOutErr,new MiddlemanExpander(){
    @Override public void expand(    Artifact middlemanArtifact,    Collection<? super Artifact> output){
      output.addAll(graphFileCache.expandInputMiddleman(middlemanArtifact));
    }
  }
);
}
