{
  return androidSdk != null ? new SpawnAction.Builder().setExecutable(androidSdk.getApkBuilder()) : new SpawnAction.Builder().setExecutable(ruleContext.getHostConfiguration().getFragment(Jvm.class).getJavaExecutable()).addTransitiveInputs(JavaCompilationHelper.getHostJavabaseInputs(ruleContext)).addArgument("-jar").addInputArgument(apkBuilderTool);
}
