{
  if (!ruleContext.getRule().isAttrDefined(attribute,Type.LABEL)) {
    return null;
  }
  Artifact artifact=getOptionalArtifact(ruleContext,attribute);
  if (artifact == null) {
    return null;
  }
  return FilesToRunProvider.fromSingleArtifact(ruleContext.attributes().get(attribute,Type.LABEL),artifact);
}
