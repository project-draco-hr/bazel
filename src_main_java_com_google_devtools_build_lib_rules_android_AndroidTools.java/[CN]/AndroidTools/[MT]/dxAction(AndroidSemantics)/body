{
  return androidSdk != null ? new SpawnAction.Builder().setExecutable(androidSdk.getDx()) : new SpawnAction.Builder().addTransitiveInputs(BaseJavaCompilationHelper.getHostJavabaseInputs(ruleContext)).setExecutable(ruleContext.getHostConfiguration().getFragment(Jvm.class).getJavaExecutable()).addArguments(semantics.getDxJvmArguments()).addArgument("-jar").addInputArgument(dxJar);
}
