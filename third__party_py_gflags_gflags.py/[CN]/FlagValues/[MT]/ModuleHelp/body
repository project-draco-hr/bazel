def ModuleHelp(self, module):
    'Describe the key flags of a module.\n\n    Args:\n      module: A module object or a module name (a string).\n\n    Returns:\n      string describing the key flags of a module.\n    '
    helplist = []
    self.__RenderOurModuleKeyFlags(module, helplist)
    return '\n'.join(helplist)
