{
  if (genericType.equals(other)) {
    return this;
  }
 else   if (other instanceof Combination && genericType.equals(((Combination)other).getGenericType()) && argType.includes(((Combination)other).getArgType())) {
    return other;
  }
 else   if ((LIST.equals(other) || SET.equals(other)) && genericType.equals(other)) {
    return this;
  }
 else {
    return BOTTOM;
  }
}
