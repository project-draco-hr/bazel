{
  Simple cached=simpleCache.get(type);
  if (cached != null) {
    return cached;
  }
  Simple simple;
  if (type == Object.class) {
    simple=TOP;
  }
 else   if (type == Empty.class) {
    simple=BOTTOM;
  }
 else   if (type == Environment.NoneType.class) {
    simple=TOP;
  }
 else {
    Class<?> skylarkType=EvalUtils.getSkylarkType(type);
    if (skylarkType != type) {
      simple=Simple.of(skylarkType);
    }
 else {
      simple=new Simple(type);
    }
  }
  simpleCache.put(type,simple);
  return simple;
}
