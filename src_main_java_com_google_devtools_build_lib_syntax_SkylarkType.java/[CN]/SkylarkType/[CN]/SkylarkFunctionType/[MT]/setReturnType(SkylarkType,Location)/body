{
  if (returnType == null) {
    returnType=newReturnType;
    returnTypeLoc=newLoc;
  }
 else   if (newReturnType != SkylarkType.NONE) {
    returnType=returnType.infer(newReturnType,"return type of " + name,newLoc,returnTypeLoc);
    if (returnType == newReturnType) {
      returnTypeLoc=newLoc;
    }
  }
}
