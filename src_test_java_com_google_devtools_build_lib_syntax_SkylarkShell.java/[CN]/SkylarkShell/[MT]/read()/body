{
  StringBuilder input=new StringBuilder();
  ev.print(START_PROMPT);
  try {
    while (true) {
      String line=reader.readLine();
      if (line == null) {
        return null;
      }
      if (line.isEmpty()) {
        return input.toString();
      }
      input.append("\n").append(line);
      ev.print(CONTINUATION_PROMPT);
    }
  }
 catch (  IOException io) {
    io.printStackTrace();
    return null;
  }
}
