{
  Set<Target> visited=new HashSet<>();
  List<Target> result=new ArrayList<>(targets);
  int i=0;
  while (i < result.size()) {
    for (    Target dep : getFwdDeps(result.get(i))) {
      if (visited.add(dep)) {
        result.add(dep);
      }
    }
    i++;
  }
  return ImmutableSet.copyOf(result);
}
