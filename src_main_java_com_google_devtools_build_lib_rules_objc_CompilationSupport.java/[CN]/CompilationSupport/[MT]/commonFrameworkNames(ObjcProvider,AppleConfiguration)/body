{
  Platform platform=Platform.forIosArch(appleConfiguration.getIosCpu());
  return new ImmutableList.Builder<String>().add(AppleToolchain.sdkFrameworkDir(platform,appleConfiguration)).add(AppleToolchain.platformDeveloperFrameworkDir(appleConfiguration)).addAll(PathFragment.safePathStrings(uniqueParentDirectories(provider.get(FRAMEWORK_DIR)))).build();
}
