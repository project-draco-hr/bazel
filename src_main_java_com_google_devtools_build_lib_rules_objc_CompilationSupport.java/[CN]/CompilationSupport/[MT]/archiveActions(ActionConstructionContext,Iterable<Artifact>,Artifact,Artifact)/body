{
  ImmutableList.Builder<Action> actions=new ImmutableList.Builder<>();
  actions.add(objFilelistAction(context,objFiles,objList));
  actions.add(ObjcRuleClasses.spawnAppleEnvActionBuilder(ruleContext,appleConfiguration.getPlatform(PlatformType.IOS)).setMnemonic("ObjcLink").setExecutable(libtool(ruleContext)).setCommandLine(new CustomCommandLine.Builder().add("-static").add("-filelist").add(objList.getExecPathString()).add("-arch_only").add(appleConfiguration.getSingleArchitecture(PlatformType.IOS)).add("-syslibroot").add(AppleToolchain.sdkDir()).add("-o").add(archive.getExecPathString()).build()).addInputs(objFiles).addInput(objList).addOutput(archive).build(context));
  return actions.build();
}
