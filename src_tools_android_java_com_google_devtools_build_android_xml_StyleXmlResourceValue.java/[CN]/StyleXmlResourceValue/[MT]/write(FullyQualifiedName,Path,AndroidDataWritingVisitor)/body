{
  ValuesResourceDefinition definition=mergedDataWriter.define(key).derivedFrom(source).startTag("style").named(key).optional().attribute("parent").setTo(parent).closeTag();
  for (  Entry<String,String> entry : values.entrySet()) {
    definition=definition.startItemTag().named(entry.getKey()).closeTag().addCharactersOf(entry.getValue()).endTag();
  }
  definition.endTag().save();
}
