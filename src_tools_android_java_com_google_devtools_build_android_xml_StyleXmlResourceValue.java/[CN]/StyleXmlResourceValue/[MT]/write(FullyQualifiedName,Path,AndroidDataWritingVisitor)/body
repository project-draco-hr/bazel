{
  mergedDataWriter.writeToValuesXml(key,FluentIterable.from(ImmutableList.of(String.format("<!-- %s -->",source),parent == null || parent.isEmpty() ? String.format("<style name='%s'>",key.name()) : String.format("<style name='%s' parent='@%s'>",key.name(),parent))).append(FluentIterable.from(values.entrySet()).transform(ENTRY_TO_ITEM)).append("</style>"));
}
