{
  builder.add(InstrumentedFilesProvider.class,new InstrumentedFilesProviderImpl(new InstrumentedFilesCollector(ruleContext,semantics.getCoverageInstrumentationSpec(),METADATA_COLLECTOR,filesToBuild))).add(PythonSourcesProvider.class,new PythonSourcesProvider(transitivePythonSources,usesSharedLibraries())).addOutputGroup(OutputGroupProvider.FILES_TO_COMPILE,transitivePythonSources).addOutputGroup(OutputGroupProvider.COMPILATION_PREREQUISITES,transitivePythonSources);
}
