{
  String errorText;
  if (explicit) {
    errorText="file name '" + proposedMainName + "' specified by 'main' attribute matches multiple files: e.g., '"+ match1+ "' and '"+ match2+ "'";
  }
 else {
    errorText="default main file name '" + proposedMainName + "' matches multiple files.  Perhaps specify an explicit file with 'main' attribute?  "+ "Matches were: '"+ match1+ "' and '"+ match2+ "'";
  }
  return errorText;
}
