{
  if (!requires.containsKey(feature)) {
    return true;
  }
  for (  ImmutableSet<Feature> requiresAllOf : requires.get(feature)) {
    boolean requirementMet=true;
    for (    Feature required : requiresAllOf) {
      if (!enabled.contains(required)) {
        requirementMet=false;
        break;
      }
    }
    if (requirementMet) {
      return true;
    }
  }
  return false;
}
