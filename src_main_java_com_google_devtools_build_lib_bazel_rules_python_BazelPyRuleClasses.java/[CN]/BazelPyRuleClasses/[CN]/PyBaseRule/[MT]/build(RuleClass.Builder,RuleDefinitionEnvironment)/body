{
  return builder.override(builder.copy("deps").allowedRuleClasses(ALLOWED_RULES_IN_DEPS).allowedFileTypes()).add(attr("imports",STRING_LIST).value(ImmutableList.<String>of())).add(attr("srcs_version",STRING).value(PythonVersion.defaultValue().toString())).add(attr("$python2to3",LABEL).cfg(HOST).exec().value(env.getToolsLabel("//tools/python:2to3"))).setPreferredDependencyPredicate(PyRuleClasses.PYTHON_SOURCE).build();
}
