{
  return builder.override(builder.copy("deps").allowedRuleClasses(ALLOWED_RULES_IN_DEPS).allowedFileTypes()).add(attr("srcs_version",STRING).value(PythonVersion.defaultValue().toString())).add(attr("$python2to3",LABEL).cfg(HOST).exec().value(env.getLabel(Constants.TOOLS_REPOSITORY + "//tools/python:2to3"))).setPreferredDependencyPredicate(PyRuleClasses.PYTHON_SOURCE).build();
}
