{
  new BothModesTest().testEval("[1, 2, 3, 4, 5][::1]","[1, 2, 3, 4, 5]").testEval("[1, 2, 3, 4, 5][1::1]","[2, 3, 4, 5]").testEval("[1, 2, 3, 4, 5][:2:1]","[1, 2]").testEval("[1, 2, 3, 4, 5][1:3:1]","[2, 3]").testEval("[1, 2, 3, 4, 5][-4:-2:1]","[2, 3]").testEval("[1, 2, 3, 4, 5][-10:10:1]","[1, 2, 3, 4, 5]").testEval("[1, 2, 3, 4, 5][::42]","[1]");
}
