{
  new BothModesTest().testStatement("str(1)","1").testStatement("str(-2)","-2").testStatement("str([1, 2])","[1, 2]").testStatement("str(True)","True").testStatement("str(False)","False").testStatement("str(None)","None").testStatement("str(str)","<function str>");
  new SkylarkTest().testStatement("str(struct(x = 2, y = 3, z = 4))","struct(x = 2, y = 3, z = 4)");
}
