{
  new BothModesTest().testStatement("'Apricot'.endswith('cot')",true).testStatement("'a'.endswith('')",true).testStatement("''.endswith('')",true).testStatement("'Apricot'.endswith('co')",false).testStatement("'Apricot'.endswith('co', -1)",false).testStatement("'abcd'.endswith('c', -2, -1)",true).testStatement("'abcd'.endswith('c', 1, 8)",false).testStatement("'abcd'.endswith('d', 1, 8)",true);
}
