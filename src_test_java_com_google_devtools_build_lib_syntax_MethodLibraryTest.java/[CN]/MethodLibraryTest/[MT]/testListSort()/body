{
  new BothModesTest().testEval("sorted([0,1,2,3])","[0, 1, 2, 3]").testEval("sorted([])","[]").testEval("sorted([3, 2, 1, 0])","[0, 1, 2, 3]").testEval("sorted([[1], [], [2], [1, 2]])","[[], [1], [1, 2], [2]]").testEval("sorted([True, False, True])","[False, True, True]").testEval("sorted(['a','x','b','z'])","[\"a\", \"b\", \"x\", \"z\"]").testEval("sorted([sorted, sorted])","[sorted, sorted]").testEval("sorted({1: True, 5: True, 4: False})","[1, 4, 5]").testEval("sorted(set([1, 5, 4]))","[1, 4, 5]");
}
