{
  new BothModesTest().testIfExactError("invalid literal for int() with base 3: \"123\"","int('123', 3)").testIfExactError("invalid literal for int() with base 15: \"FF\"","int('FF', 15)").testIfExactError("int() base must be >= 2 and <= 36","int('123', -1)").testIfExactError("int() base must be >= 2 and <= 36","int('123', 1)").testIfExactError("int() base must be >= 2 and <= 36","int('123', 37)");
}
