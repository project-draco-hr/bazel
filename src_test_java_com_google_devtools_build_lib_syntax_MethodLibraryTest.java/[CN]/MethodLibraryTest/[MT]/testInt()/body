{
  new BothModesTest().testStatement("int('1')",1).testStatement("int('-1234')",-1234).testIfErrorContains("invalid literal for int(): \"1.5\"","int('1.5')").testIfErrorContains("invalid literal for int(): \"ab\"","int('ab')").testStatement("int(42)",42).testStatement("int(-1)",-1).testStatement("int(True)",1).testStatement("int(False)",0).testIfErrorContains("None is not of type string or int or bool","int(None)");
