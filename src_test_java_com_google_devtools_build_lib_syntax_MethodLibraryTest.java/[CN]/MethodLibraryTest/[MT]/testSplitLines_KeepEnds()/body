{
  new SkylarkTest().testEval("''.splitlines(True)","[]").testEval("'\\n'.splitlines(True)","['\\n']").testEval("'this\\nis\\r\\na\\rtest'.splitlines(True)","['this\\n', 'is\\r\\n', 'a\\r', 'test']").testEval("'\\ntest'.splitlines(True)","['\\n', 'test']").testEval("'test\\n'.splitlines(True)","['test\\n']").testEval("'\\n\\\\n\\\\\\n'.splitlines(True)","['\\n', '\\\\n\\\\\\n']");
}
