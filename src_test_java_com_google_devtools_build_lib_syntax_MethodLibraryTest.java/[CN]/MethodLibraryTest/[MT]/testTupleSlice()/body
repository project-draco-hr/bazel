{
  new BothModesTest().testEval("()[1:2]","()").testEval("()[::1]","()").testEval("(0, 1, 2, 3)[0:-1]","(0, 1, 2)").testEval("(0, 1, 2, 3, 4, 5)[2:4]","(2, 3)").testEval("(0, 1, 2, 3)[-10:10]","(0, 1, 2, 3)").testEval("(1, 2, 3, 4, 5)[-10:10:1]","(1, 2, 3, 4, 5)").testEval("(1, 2, 3, 4, 5, 6, 7, 8, 9)[1:7:3]","(2, 5)").testEval("(1, 2, 3, 4, 5)[::-1]","(5, 4, 3, 2, 1)").testEval("(1, 2, 3, 4, 5)[3:1:-1]","(4, 3)").testEval("(1, 2, 3, 4, 5)[::-2]","(5, 3, 1)").testEval("(1, 2, 3, 4, 5)[::-10]","(5,)").testIfExactError("slice step cannot be zero","(1, 2, 3)[1::0]");
}
