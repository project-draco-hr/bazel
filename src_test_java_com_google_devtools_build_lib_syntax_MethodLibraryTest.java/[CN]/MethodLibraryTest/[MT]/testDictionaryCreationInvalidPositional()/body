{
  String unexpectedString="expected value of type 'list(object)' for dict(args), but got \"a\" (string)";
  new BothModesTest().testIfErrorContains("Method dict(args: Iterable, **kwargs) is not applicable for arguments " + "(string, dict): 'args' is string, but should be Iterable","dict('a')").testIfErrorContains(unexpectedString,"dict(['a'])").testIfErrorContains(unexpectedString,"dict([('a')])").testIfErrorContains("too many (3) positional arguments","dict((3,4), (3,2), (1,2))").testIfErrorContains("Tuple has length 3, but exactly two elements are required","dict([('a', 'b', 'c')])");
}
