{
  new BothModesTest().testIfErrorContains("List index out of range (index is 3, but list has 3 elements)","[0, 1, 2][3]").testIfErrorContains("List index out of range (index is -4, but list has 3 elements)","[0, 1, 2][-4]").testIfErrorContains("List index out of range (index is -2, but list has 1 elements)","[0][-2]").testIfErrorContains("List index out of range (index is 1, but list has 1 elements)","[0][1]").testIfErrorContains("List index out of range (index is 1, but list has 0 elements)","[][1]");
}
