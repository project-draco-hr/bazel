{
  new BothModesTest().testEval("[1, 2, 3, 4, 5][::-1]","[5, 4, 3, 2, 1]").testEval("[1, 2, 3, 4, 5][4::-1]","[5, 4, 3, 2, 1]").testEval("[1, 2, 3, 4, 5][:0:-1]","[5, 4, 3, 2]").testEval("[1, 2, 3, 4, 5][3:1:-1]","[4, 3]").testEval("[1, 2, 3, 4, 5][::-2]","[5, 3, 1]").testEval("[1, 2, 3, 4, 5][::-10]","[5]");
}
