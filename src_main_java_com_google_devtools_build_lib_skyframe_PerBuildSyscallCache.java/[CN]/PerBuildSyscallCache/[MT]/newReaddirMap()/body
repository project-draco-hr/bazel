{
  return CacheBuilder.newBuilder().build(new CacheLoader<Pair<Path,Symlinks>,Pair<Collection<Dirent>,IOException>>(){
    @Override public Pair<Collection<Dirent>,IOException> load(    Pair<Path,Symlinks> p){
      try {
        return Pair.of(p.first.readdir(p.second),null);
      }
 catch (      IOException e) {
        return Pair.of(null,e);
      }
    }
  }
);
}
