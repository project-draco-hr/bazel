{
  StringBuilder builder=new StringBuilder();
  builder.append(convertRoots(container,ResourceType.RESOURCES)).append(":").append(convertRoots(container,ResourceType.ASSETS)).append(":").append(container.getManifest().getExecPathString());
  if (includeSymbols) {
    builder.append(":").append(container.getRTxt() == null ? "" : container.getRTxt().getExecPath()).append(":").append(container.getSymbolsTxt() == null ? "" : container.getSymbolsTxt().getExecPath());
  }
  return builder.toString();
}
