{
  Iterables.addAll(inputs,container.getArtifacts());
  inputs.add(container.getManifest());
  inputs.add(container.getRTxt());
  args.add(String.format("%s:%s:%s:%s",convertRoots(container,ResourceType.RESOURCES),convertRoots(container,ResourceType.ASSETS),container.getManifest().getExecPathString(),container.getRTxt() == null ? "" : container.getRTxt().getExecPath()));
}
