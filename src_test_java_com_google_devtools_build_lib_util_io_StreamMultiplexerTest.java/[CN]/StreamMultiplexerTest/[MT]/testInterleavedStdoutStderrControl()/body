{
  out.write(getLatin("Hello, stdout."));
  out.flush();
  err.write(getLatin("Hello, stderr."));
  err.flush();
  ctl.write(getLatin("Hello, control."));
  ctl.flush();
  out.write(getLatin("... and back!"));
  out.flush();
  assertEquals(joinLines("@1@","Hello, stdout.","@2@","Hello, stderr.","@3@","Hello, control.","@1@","... and back!",""),getLatin(multiplexed.toByteArray()));
}
