{
  super.execute(new Runnable(){
    @Override public void run(){
synchronized (lock) {
        if (theInt < 10) {
          theInt++;
          enqueue();
        }
        int concurrentTasks=activeParallelTasks();
        if (concurrentTasks > maxRunningConcurrently) {
          maxRunningConcurrently=concurrentTasks;
        }
      }
    }
  }
);
}
