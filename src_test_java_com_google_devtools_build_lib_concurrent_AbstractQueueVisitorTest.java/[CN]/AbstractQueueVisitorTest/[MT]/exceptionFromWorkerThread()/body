{
  final RuntimeException myException=new IllegalStateException();
  ConcreteQueueVisitor visitor=new ConcreteQueueVisitor();
  visitor.enqueue(new Runnable(){
    @Override public void run(){
      throw myException;
    }
  }
);
  try {
    visitor.work(false);
    fail();
  }
 catch (  Exception e) {
    assertSame(myException,e);
  }
}
