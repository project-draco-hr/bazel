{
  MountMap mounts=new MountMap();
  FileSystem fs=blazeDirs.getFileSystem();
  ImmutableList<Path> exclude=ImmutableList.of(blazeDirs.getWorkspace(),blazeDirs.getOutputBase());
  for (  String pathStr : sandboxOptions.sandboxAddPath) {
    Path path=fs.getPath(pathStr);
    for (    Path exc : exclude) {
      if (path.startsWith(exc)) {
        throw new IllegalArgumentException("Mounting subdirectory of WORKSPACE or OUTPUTBASE to sandbox is not allowed.");
      }
    }
    mounts.putAll(mountChildDirExclude(path,exclude));
  }
  return mounts;
}
