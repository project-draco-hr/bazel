{
  MountMap mounts=new MountMap();
  boolean startsWithFlag=false;
  for (  Path exc : exclude) {
    if (exc.startsWith(child)) {
      startsWithFlag=true;
      break;
    }
  }
  if (!startsWithFlag) {
    mounts.put(child,child);
  }
 else   if (!exclude.contains(child)) {
    mounts.putAll(mountDirExclude(child,exclude));
  }
  return mounts;
}
