{
  if (fullySupported) {
    return new LinuxSandboxRunner(execRoot,sandboxPath,sandboxExecRoot,sandboxTempDir,getWritableDirs(sandboxExecRoot,spawn.getEnvironment()),getInaccessiblePaths(),getBindMounts(blazeDirs),verboseFailures,sandboxOptions.sandboxDebug);
  }
 else {
    return new ProcessWrapperRunner(execRoot,sandboxPath,sandboxExecRoot,verboseFailures);
  }
}
