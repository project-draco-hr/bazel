{
  Environment env=singletonEnv(func.getName(),func);
  if (expectedOutput.charAt(0) == '[') {
    assertEquals("Wrong output for " + callExpression,expectedOutput,eval(callExpression,env).toString());
  }
 else {
    try {
      eval(callExpression,env);
      fail();
    }
 catch (    EvalException e) {
      assertEquals("Wrong exception for " + callExpression,expectedOutput,e.getMessage());
    }
  }
}
