{
  List<String> args=new ArrayList<>();
  args.add("4");
  args.add(signedApk.getExecPathString());
  args.add(zipAlignedApk.getExecPathString());
  return new SpawnAction.Builder().addInput(signedApk).addOutput(zipAlignedApk).setExecutable(AndroidSdkProvider.fromRuleContext(ruleContext).getZipalign()).addArguments(args).setProgressMessage("Zipaligning " + apkName).setMnemonic("AndroidZipAlign").build(ruleContext);
}
