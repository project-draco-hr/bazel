{
  super(name);
  FunctionSignature sig=signature.getSignature();
  FunctionSignature.Shape shape=sig.getShape();
  Preconditions.checkArgument(!shape.hasKwArg() && !shape.hasStarArg() && shape.getNamedOnly() == 0,"no star, star-star or named-only parameters (for now)");
  this.signature=signature;
  this.parameters=ImmutableList.copyOf(sig.getNames());
  this.numMandatoryParameters=shape.getMandatoryPositionals();
  this.onlyNamedArguments=false;
  this.location=location;
}
