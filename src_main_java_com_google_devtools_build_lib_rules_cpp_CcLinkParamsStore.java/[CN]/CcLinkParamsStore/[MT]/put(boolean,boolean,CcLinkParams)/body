{
  Preconditions.checkNotNull(params);
  if (linkingStatically) {
    if (linkShared) {
      staticSharedParams=params;
    }
 else {
      staticNoSharedParams=params;
    }
  }
 else {
    if (linkShared) {
      noStaticSharedParams=params;
    }
 else {
      noStaticNoSharedParams=params;
    }
  }
}
