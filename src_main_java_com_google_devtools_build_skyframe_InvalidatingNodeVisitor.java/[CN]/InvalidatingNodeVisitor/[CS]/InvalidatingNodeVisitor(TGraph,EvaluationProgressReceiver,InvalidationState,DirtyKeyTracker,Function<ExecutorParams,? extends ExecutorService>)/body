{
  this.executor=new AbstractQueueVisitor(true,DEFAULT_THREAD_COUNT,1,TimeUnit.SECONDS,true,"skyframe-invalidator",executorFactory,errorClassifier,ErrorHandler.NullHandler.INSTANCE);
  this.graph=Preconditions.checkNotNull(graph);
  this.invalidationReceiver=invalidationReceiver;
  this.dirtyKeyTracker=Preconditions.checkNotNull(dirtyKeyTracker);
  this.pendingVisitations=state.pendingValues;
}
