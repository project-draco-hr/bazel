{
  for (  Pair<SkyKey,InvalidationType> visitData : ImmutableList.copyOf(pendingVisitations)) {
    visit(visitData.first,visitData.second,!MUST_EXIST);
  }
  executor.awaitQuiescence(true);
  Preconditions.checkState(pendingVisitations.isEmpty(),"All dirty nodes should have been processed: %s",pendingVisitations);
}
