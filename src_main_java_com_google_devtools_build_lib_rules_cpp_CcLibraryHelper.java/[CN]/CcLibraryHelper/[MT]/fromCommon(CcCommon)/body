{
  this.addCopts(common.getCopts()).addDefines(common.getDefines()).addDeps(ruleContext.getPrerequisites("deps",Mode.TARGET)).addIncludeDirs(common.getIncludeDirs()).addLooseIncludeDirs(common.getLooseIncludeDirs()).addPicIndependentObjectFiles(common.getLinkerScripts()).addSources(common.getCAndCppSources()).addSystemIncludeDirs(common.getSystemIncludeDirs()).setNoCopts(common.getNoCopts()).setHeadersCheckingMode(common.determineHeadersCheckingMode());
  return this;
}
