{
  this.addCompilationPrerequisites(common.getSharedLibrariesFromSrcs()).addCompilationPrerequisites(common.getStaticLibrariesFromSrcs()).addCopts(common.getCopts()).addDefines(common.getDefines()).addDeps(ruleContext.getPrerequisites("deps",Mode.TARGET)).addIncludeDirs(common.getIncludeDirs()).addLooseIncludeDirs(common.getLooseIncludeDirs()).addObjectFiles(common.getObjectFilesFromSrcs(false)).addPicIndependentObjectFiles(common.getLinkerScripts()).addPicObjectFiles(common.getObjectFilesFromSrcs(true)).addSources(common.getCAndCppSources()).addSystemIncludeDirs(common.getSystemIncludeDirs()).setNoCopts(common.getNoCopts()).setHeadersCheckingMode(common.determineHeadersCheckingMode());
  return this;
}
