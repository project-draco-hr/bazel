{
  this.addCopts(common.getCopts()).addDefines(common.getDefines()).addDeps(ruleContext.getPrerequisites("deps",Mode.TARGET)).addIncludeDirs(common.getIncludeDirs()).addLooseIncludeDirs(common.getLooseIncludeDirs()).addPicIndependentObjectFiles(common.getLinkerScripts()).addSystemIncludeDirs(common.getSystemIncludeDirs()).setNoCopts(common.getNoCopts()).setHeadersCheckingMode(semantics.determineHeadersCheckingMode(ruleContext));
  return this;
}
