{
  return new CppModel(ruleContext,semantics).addSources(sources).addCopts(copts).setLinkTargetType(linkType).setNeverLink(neverlink).setFake(fake).setAllowInterfaceSharedObjects(emitInterfaceSharedObjects).setCreateDynamicLibrary(emitDynamicLibrary).setSaveTemps(true).setNoCopts(nocopts).setDynamicLibrary(dynamicLibrary).addLinkopts(linkopts).setFeatureConfiguration(featureConfiguration);
}
