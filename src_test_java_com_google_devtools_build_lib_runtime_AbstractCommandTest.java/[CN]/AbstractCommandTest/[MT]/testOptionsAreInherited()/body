{
  ConfiguredRuleClassProvider ruleClassProvider=new ConfiguredRuleClassProvider.Builder().build();
  MoreAsserts.assertSameContents(optionClassesWithDefault(FooOptions.class),BlazeCommandUtils.getOptions(CommandA.class,ImmutableList.<BlazeModule>of(),ruleClassProvider));
  MoreAsserts.assertSameContents(optionClassesWithDefault(FooOptions.class,BarOptions.class),BlazeCommandUtils.getOptions(CommandB.class,ImmutableList.<BlazeModule>of(),ruleClassProvider));
}
