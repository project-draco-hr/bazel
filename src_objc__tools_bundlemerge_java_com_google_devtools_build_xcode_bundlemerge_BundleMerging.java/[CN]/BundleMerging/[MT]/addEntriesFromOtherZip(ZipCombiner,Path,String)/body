{
  try (ZipInputStream zipIn=new ZipInputStream(Files.newInputStream(sourceZip))){
    while (true) {
      ZipEntry zipInEntry=zipIn.getNextEntry();
      if (zipInEntry == null) {
        break;
      }
      combiner.addFile(entryNamesPrefix + zipInEntry.getName(),DOS_EPOCH,zipIn,ZipInputEntry.DEFAULT_DIRECTORY_ENTRY_INFO);
    }
  }
 }
