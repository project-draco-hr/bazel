{
  if (dep == null) {
    return ImmutableList.of();
  }
  MiddlemanFactory factory=env.getMiddlemanFactory();
  Iterable<Artifact> artifacts=dep.getProvider(FileProvider.class).getFilesToBuild();
  return ImmutableList.of(factory.createMiddlemanAllowMultiple(env,actionOwner,ruleContext.getRule().getLabel().getPackageIdentifier().getSourceRoot(),purpose,artifacts,ruleContext.getConfiguration().getMiddlemanDirectory(ruleContext.getRule().getRepository())));
}
