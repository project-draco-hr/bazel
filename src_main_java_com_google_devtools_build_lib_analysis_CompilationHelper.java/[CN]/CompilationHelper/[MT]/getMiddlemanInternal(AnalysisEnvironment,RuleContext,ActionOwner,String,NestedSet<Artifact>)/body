{
  if (filesToBuild == null) {
    return ImmutableList.of();
  }
  MiddlemanFactory factory=env.getMiddlemanFactory();
  return ImmutableList.of(factory.createMiddlemanAllowMultiple(env,actionOwner,ruleContext.getPackageDirectory(),purpose,filesToBuild,ruleContext.getConfiguration().getMiddlemanDirectory()));
}
