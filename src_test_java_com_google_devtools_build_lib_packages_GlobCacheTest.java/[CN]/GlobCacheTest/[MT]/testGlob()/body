{
  assertEmpty(cache.glob(list("*.java"),NONE,false));
  assertThat(cache.glob(list("*.*"),NONE,false)).containsExactly("first.js","first.txt","second.js","second.txt").inOrder();
  assertThat(cache.glob(list("*.*"),list("first.js"),false)).containsExactly("first.txt","second.js","second.txt").inOrder();
  assertThat(cache.glob(list("*.txt","first.*"),NONE,false)).containsExactly("first.txt","second.txt","first.js").inOrder();
}
