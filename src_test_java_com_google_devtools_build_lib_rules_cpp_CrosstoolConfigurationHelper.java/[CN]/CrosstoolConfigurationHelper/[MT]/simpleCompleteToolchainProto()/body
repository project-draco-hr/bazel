{
  CrosstoolConfig.CrosstoolRelease.Builder builder=CrosstoolConfig.CrosstoolRelease.newBuilder().setMajorVersion("12").setMinorVersion("0").setDefaultTargetCpu("k8").addDefaultToolchain(DefaultCpuToolchain.newBuilder().setCpu("k8").setToolchainIdentifier("k8-toolchain"));
  CrosstoolConfig.CToolchain.Builder toolchainBuilder=newIncompleteToolchain();
  toolchainBuilder.setToolchainIdentifier("k8-toolchain").setHostSystemName("i686-unknown-linux-gnu").setTargetSystemName("i686-unknown-linux-gnu").setTargetCpu("k8").setTargetLibc("glibc-2.3.6-grte").setCompiler("gcc-4.3.1").setAbiVersion("gcc-3.4").setAbiLibcVersion("2.3.2").addCxxBuiltinIncludeDirectory("/include/directory");
  builder.addToolchain(toolchainBuilder);
  return builder.build();
}
