{
  return builder.requiresConfigurationFragments(AndroidConfiguration.class,JavaConfiguration.class,CppConfiguration.class).add(attr("$debug_keystore",BuildType.LABEL).cfg(HOST).singleArtifact().value(environment.getLabel(Constants.TOOLS_REPOSITORY + "//tools/android:debug_keystore"))).add(attr(":cc_toolchain_split",BuildType.LABEL).cfg(AndroidRuleClasses.ANDROID_SPLIT_TRANSITION).value(BazelCppRuleClasses.CC_TOOLCHAIN)).setImplicitOutputsFunction(AndroidRuleClasses.ANDROID_BINARY_IMPLICIT_OUTPUTS).build();
}
