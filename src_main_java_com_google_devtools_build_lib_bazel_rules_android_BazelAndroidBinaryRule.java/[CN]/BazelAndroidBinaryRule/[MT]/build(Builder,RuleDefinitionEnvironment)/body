{
  return builder.requiresConfigurationFragments(AndroidConfiguration.class,JavaConfiguration.class,CppConfiguration.class).add(attr(":cc_toolchain_split",Type.LABEL).cfg(AndroidRuleClasses.ANDROID_SPLIT_TRANSITION).value(BazelCppRuleClasses.CC_TOOLCHAIN)).setImplicitOutputsFunction(AndroidRuleClasses.ANDROID_BINARY_IMPLICIT_OUTPUTS).build();
}
