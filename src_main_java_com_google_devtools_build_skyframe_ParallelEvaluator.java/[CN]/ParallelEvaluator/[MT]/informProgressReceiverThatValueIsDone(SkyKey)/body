{
  if (progressReceiver != null) {
    NodeEntry entry=graph.get(key);
    Preconditions.checkState(entry.isDone(),entry);
    SkyValue value=entry.getValue();
    if (value != null) {
      Version valueVersion=entry.getVersion();
      Preconditions.checkState(valueVersion.atMost(graphVersion),"%s should be at most %s in the version partial ordering",valueVersion,graphVersion);
      progressReceiver.evaluated(key,value,valueVersion.equals(graphVersion) ? EvaluationState.BUILT : EvaluationState.CLEAN);
    }
  }
}
