{
  SkyValue value=bubbleErrorInfo == null ? null : bubbleErrorInfo.get(key);
  if (value != null) {
    Preconditions.checkNotNull(entry,"Value cannot have error before evaluation started",key,value);
    return ValueWithMetadata.wrapWithMetadata(value);
  }
  return isDoneForBuild(entry) ? ValueWithMetadata.wrapWithMetadata(entry.getValueMaybeWithMetadata()) : null;
}
