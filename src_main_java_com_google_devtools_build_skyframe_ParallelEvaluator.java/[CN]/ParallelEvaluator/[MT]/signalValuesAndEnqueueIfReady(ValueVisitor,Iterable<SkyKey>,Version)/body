{
  if (visitor != null) {
    for (    SkyKey key : keys) {
      if (graph.get(key).signalDep(version)) {
        visitor.enqueueEvaluation(key);
      }
    }
  }
 else {
    for (    SkyKey key : keys) {
      NodeEntry entry=Preconditions.checkNotNull(graph.get(key),key);
      if (!entry.isDone()) {
        entry.signalDep(version);
      }
    }
  }
}
