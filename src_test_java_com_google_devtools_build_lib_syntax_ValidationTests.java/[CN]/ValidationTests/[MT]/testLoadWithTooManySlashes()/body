{
  String error="First argument of load() is a path, not a label. " + "It should start with a single slash if it is an absolute path.";
  checkError(error,"load('//', 'a')\n");
  checkError(error,"load('//extension', 'a')\n");
  checkError(error,"load('///', 'a')\n");
  checkError(error,"load('///extension', 'a')\n");
}
