{
  NestedSet<Artifact> toolchainExtclasspath=JavaToolchainProvider.getDefaultExtclasspath(ruleContext);
  if (toolchainExtclasspath != null) {
    return ImmutableList.copyOf(toolchainExtclasspath);
  }
  return ruleContext.getPrerequisiteArtifacts("$javac_extdir" + implicitAttributesSuffix,Mode.HOST).list();
}
