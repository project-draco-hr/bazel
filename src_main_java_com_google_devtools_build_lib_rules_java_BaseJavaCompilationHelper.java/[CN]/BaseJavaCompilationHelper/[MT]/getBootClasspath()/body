{
  NestedSet<Artifact> toolchainBootclasspath=JavaToolchainProvider.getDefaultBootclasspath(ruleContext);
  if (toolchainBootclasspath != null) {
    return ImmutableList.copyOf(toolchainBootclasspath);
  }
  return ruleContext.getPrerequisiteArtifacts("$javac_bootclasspath" + implicitAttributesSuffix,Mode.HOST).list();
}
