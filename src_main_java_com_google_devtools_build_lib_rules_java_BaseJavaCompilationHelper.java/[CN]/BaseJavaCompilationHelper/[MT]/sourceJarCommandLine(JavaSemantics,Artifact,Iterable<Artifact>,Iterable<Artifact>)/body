{
  CustomCommandLine.Builder args=CustomCommandLine.builder();
  args.addExecPath("--output",outputJar);
  args.add(SOURCE_JAR_COMMAND_LINE_ARGS);
  args.addExecPaths("--sources",resourceJars);
  args.add("--resources");
  for (  Artifact resource : resources) {
    args.addPaths("%s:%s",resource.getExecPath(),semantics.getJavaResourcePath(resource.getRootRelativePath()));
  }
  return args.build();
}
