{
  assertSyntaxError("package names may contain only A-Z, a-z, 0-9, '/', '-' and '_'","//foo/bar/.:baz");
  assertSyntaxError(BAD_PACKAGE_CHARS,"//foo/./baz:baz");
  assertSyntaxError(BAD_PACKAGE_CHARS,"//./bar/baz:baz");
  assertSyntaxError("target names may not contain '.' as a path segment","//foo:bar/./baz");
  assertSyntaxError("target names may not contain '.' as a path segment","//foo:./bar/baz");
}
