{
  assertSyntaxError(BAD_PACKAGE_CHARS,"//foo/bar/..:baz");
  assertSyntaxError(BAD_PACKAGE_CHARS,"//foo/../baz:baz");
  assertSyntaxError(BAD_PACKAGE_CHARS,"//../bar/baz:baz");
  assertSyntaxError(BAD_PACKAGE_CHARS,"//..:foo");
  assertSyntaxError(TARGET_UPLEVEL,"//foo:bar/../baz");
  assertSyntaxError(TARGET_UPLEVEL,"//foo:../bar/baz");
  assertSyntaxError(TARGET_UPLEVEL,"//foo:bar/baz/..");
  assertSyntaxError(TARGET_UPLEVEL,"//foo:..");
}
