{
  Label.parseAbsolute("//foo:..bar");
  Label.parseAbsolute("//Foo:..bar");
  Label.parseAbsolute("//-Foo:..bar");
  Label.parseAbsolute("//00:..bar");
  Label.parseAbsolute("//package:foo+bar");
  Label.parseAbsolute("//package:foo_bar");
  Label.parseAbsolute("//package:foo=bar");
  Label.parseAbsolute("//package:foo-bar");
  Label.parseAbsolute("//package:foo.bar");
  Label.parseAbsolute("//package:foo@bar");
  Label.parseAbsolute("//package:foo~bar");
}
