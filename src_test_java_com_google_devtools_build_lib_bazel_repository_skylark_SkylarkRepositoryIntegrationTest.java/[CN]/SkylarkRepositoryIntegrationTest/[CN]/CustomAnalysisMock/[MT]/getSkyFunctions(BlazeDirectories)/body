{
  RepositoryFunction localRepositoryFunction=new LocalRepositoryFunction();
  SkylarkRepositoryFunction skylarkRepositoryFunction=new SkylarkRepositoryFunction();
  ImmutableMap<String,RepositoryFunction> repositoryHandlers=ImmutableMap.of(LocalRepositoryRule.NAME,localRepositoryFunction);
  return ImmutableMap.of(SkyFunctions.REPOSITORY_DIRECTORY,new RepositoryDelegatorFunction(directories,repositoryHandlers,skylarkRepositoryFunction,new AtomicBoolean(true)),SkyFunctions.REPOSITORY,new RepositoryLoaderFunction(),FdoSupportValue.SKYFUNCTION,new FdoSupportFunction());
}
