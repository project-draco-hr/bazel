{
  BuildDriver driver=skyframeExecutor.getDriverForTesting();
  EvaluationResult<PrepareDepsOfTargetsUnderDirectoryValue> evaluationResult=driver.evaluate(ImmutableList.of(key),false,SequencedSkyframeExecutor.DEFAULT_THREAD_COUNT,reporter);
  Preconditions.checkState(!evaluationResult.hasError());
  return evaluationResult;
}
