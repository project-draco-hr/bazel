{
  return builder.requiresConfigurationFragments(JavaConfiguration.class,Jvm.class).setImplicitOutputsFunction(BazelJavaRuleClasses.JAVA_BINARY_IMPLICIT_OUTPUTS).add(attr(":proguard",LABEL).cfg(HOST).value(JavaSemantics.PROGUARD).exec()).add(attr(":extra_proguard_specs",LABEL_LIST).value(JavaSemantics.EXTRA_PROGUARD_SPECS)).override(attr("stamp",TRISTATE).value(TriState.NO)).override(attr("use_testrunner",BOOLEAN).value(true)).override(attr("main_class",STRING).value(JUNIT4_RUNNER)).add(attr("test_class",STRING)).build();
}
