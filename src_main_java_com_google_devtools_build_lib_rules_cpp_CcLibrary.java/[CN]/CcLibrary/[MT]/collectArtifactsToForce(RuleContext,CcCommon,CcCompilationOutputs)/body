{
  NestedSetBuilder<Artifact> artifactsToForceBuilder=NestedSetBuilder.stableOrder();
  artifactsToForceBuilder.addTransitive(NestedSetBuilder.wrap(Order.STABLE_ORDER,common.getFilesToCompile(ccCompilationOutputs)));
  for (  AlwaysBuiltArtifactsProvider dep : ruleContext.getPrerequisites("deps",Mode.TARGET,AlwaysBuiltArtifactsProvider.class)) {
    artifactsToForceBuilder.addTransitive(dep.getArtifactsToAlwaysBuild());
  }
  return artifactsToForceBuilder.build();
}
