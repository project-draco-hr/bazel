{
  if (ruleContext.attributes().has("options",BuildType.LABEL)) {
    OptionsProvider optionsProvider=ruleContext.getPrerequisite("options",Mode.TARGET,OptionsProvider.class);
    if (optionsProvider != null) {
      infoplistInputs.addAll(optionsProvider.getInfoplists());
    }
  }
  Artifact infoplist=ruleContext.getPrerequisiteArtifact("infoplist",Mode.TARGET);
  if (infoplist != null) {
    infoplistInputs.add(infoplist);
  }
  Iterable<Artifact> infoplists=ruleContext.getPrerequisiteArtifacts("infoplists",Mode.TARGET).list();
  if (infoplists != null) {
    infoplistInputs.addAll(infoplists);
  }
  return this;
}
