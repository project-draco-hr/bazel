{
  BuildConfiguration.Options commonOptions=buildOptions.get(BuildConfiguration.Options.class);
  if (!commonOptions.useDistinctHostConfiguration) {
    return requestConfig;
  }
 else {
    BuildConfiguration hostConfig=configurationFactory.getHostConfiguration(loadedPackageProvider,testEnv,buildOptions,false);
    if (hostConfig == null) {
      return null;
    }
    return hostConfig;
  }
}
