{
  Set<PathFragment> extraActionPathFragments=ImmutableSet.copyOf(Artifact.asPathFragments(extraActionInputs));
  shadowedAction.updateInputsFromCache(artifactResolver,Collections2.filter(inputPaths,Predicates.in(extraActionPathFragments)));
  Preconditions.checkState(shadowedAction.inputsKnown(),"%s %s",this,shadowedAction);
  updateInputs(shadowedAction.getInputs());
}
