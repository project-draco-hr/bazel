{
  return new EvaluationTestCase(){
    @Override public Environment newEnvironment() throws Exception {
      return Environment.builder(mutability).setSkylark().setEventHandler(getEventHandler()).setGlobals(SkylarkModules.getGlobals(SkylarkModules.MODULES)).setLoadingPhase().build().setupDynamic(PackageFactory.PKG_CONTEXT,new PackageContext(null,null,getEventHandler()));
    }
  }
;
}
