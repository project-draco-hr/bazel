{
  this.sourceVersion=data.getSourceVersion();
  this.targetVersion=data.getTargetVersion();
  this.encoding=data.getEncoding();
  this.javacOptions=ImmutableList.<String>builder().addAll(data.getJavacOptions()).addAll(defaultJavacFlags).build();
  this.javacJvmOptions=ImmutableList.<String>builder().addAll(data.getJavacJvmOptions()).addAll(defaultJavacJvmOpts).build();
}
