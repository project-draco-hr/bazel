{
  JavaToolchainProvider javaToolchain=ruleContext.getPrerequisite(":java_toolchain",Mode.TARGET,JavaToolchainProvider.class);
  if (javaToolchain == null) {
    ruleContext.ruleError("No java_toolchain implicit dependency found. This is probably because" + " your java configuration is not up-to-date.");
    return ImmutableList.of();
  }
  return javaToolchain.getJavacOptions();
}
