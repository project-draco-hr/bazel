{
  ListMultimap<Attribute,Dependency> depNodeNames=getDirectPrerequisiteDependenciesForTesting(eventHandler,target,configurations);
  ImmutableMap<Dependency,ConfiguredTarget> cts=skyframeExecutor.getConfiguredTargetMap(eventHandler,target.getConfiguration(),ImmutableSet.copyOf(depNodeNames.values()),false);
  ImmutableListMultimap.Builder<Attribute,ConfiguredTarget> builder=ImmutableListMultimap.builder();
  for (  Map.Entry<Attribute,Dependency> entry : depNodeNames.entries()) {
    builder.put(entry.getKey(),cts.get(entry.getValue()));
  }
  return builder.build();
}
