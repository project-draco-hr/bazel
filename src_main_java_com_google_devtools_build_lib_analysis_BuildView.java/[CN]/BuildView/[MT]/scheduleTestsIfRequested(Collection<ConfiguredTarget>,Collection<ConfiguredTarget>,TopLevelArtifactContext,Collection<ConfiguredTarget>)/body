{
  Set<String> outputGroups=topLevelOptions.outputGroups();
  if (!outputGroups.contains(TopLevelArtifactProvider.FILES_TO_COMPILE) && !outputGroups.contains(TopLevelArtifactProvider.COMPILATION_PREREQUISITES) && allTestTargets != null) {
    scheduleTests(targetsToTest,targetsToTestExclusive,allTestTargets,topLevelOptions.runTestsExclusively());
  }
}
