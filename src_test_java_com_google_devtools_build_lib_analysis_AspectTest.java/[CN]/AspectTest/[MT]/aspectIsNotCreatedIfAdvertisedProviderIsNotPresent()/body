{
  setRules(new TestAspects.BaseRule(),new TestAspects.LiarRule(),new TestAspects.AspectRequiringProviderRule());
  pkg("a","aspect_requiring_provider(name='a', foo=[':b'])","liar(name='b', foo=[])");
  ConfiguredTarget a=getConfiguredTarget("//a:a");
  assertThat(a.getProvider(TestAspects.RuleInfo.class).getData()).containsExactly("rule //a:a");
}
