{
  Artifact controlFile=intermediateArtifacts.pbxprojControlArtifact();
  register(new BinaryFileWriteAction(context.getActionOwner(),controlFile,xcodegenControlFileBytes(pbxproj,project,objcConfiguration.getMinimumOs()),false));
  register(new SpawnAction.Builder().setMnemonic("GenerateXcodeproj").setExecutable(baseTools.xcodegen()).addArgument("--control").addInputArgument(controlFile).addOutput(pbxproj).addTransitiveInputs(project.getInputsToXcodegen()).build(context));
}
