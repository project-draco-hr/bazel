{
  return spawnJavaOnDarwinActionBuilder(baseTools.actooloribtoolzipDeployJar()).setMnemonic(mnemonic).setCommandLine(new CustomCommandLine.Builder().addPath(zipOutput.getExecPath()).add(archiveRoot).addPath(IBTOOL).add("--minimum-deployment-target").add(objcConfiguration.getMinimumOs()).addPath(input.getExecPath()).build()).addOutput(zipOutput).addInput(input).build(context);
}
