{
  return new ByteSource(){
    @Override public InputStream openStream(){
      return XcodeGenProtos.Control.newBuilder().setPbxproj(pbxproj.getExecPathString()).addAllTarget(project.targets()).addBuildSetting(XcodeGenProtos.XcodeprojBuildSetting.newBuilder().setName("IPHONEOS_DEPLOYMENT_TARGET").setValue(minimumOs).build()).build().toByteString().newInput();
    }
  }
;
}
