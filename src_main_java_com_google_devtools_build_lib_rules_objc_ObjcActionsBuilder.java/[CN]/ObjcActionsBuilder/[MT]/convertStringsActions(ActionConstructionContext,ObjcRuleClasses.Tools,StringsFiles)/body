{
  ImmutableList.Builder<Action> result=new ImmutableList.Builder<>();
  for (  CompiledResourceFile stringsFile : stringsFiles) {
    final Artifact original=stringsFile.getOriginal();
    final Artifact bundled=stringsFile.getBundled().getBundled();
    result.add(new SpawnAction.Builder().setMnemonic("ConvertStringsPlist").setExecutable(baseTools.plmerge()).setCommandLine(new CommandLine(){
      @Override public Iterable<String> arguments(){
        return ImmutableList.of("--source_file",original.getExecPathString(),"--out_file",bundled.getExecPathString());
      }
    }
).addInput(original).addOutput(bundled).build(context));
  }
  return result.build();
}
