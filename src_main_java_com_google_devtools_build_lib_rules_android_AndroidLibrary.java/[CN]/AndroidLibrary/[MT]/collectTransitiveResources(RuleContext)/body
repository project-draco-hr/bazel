{
  NestedSetBuilder<ResourceContainer> builder=NestedSetBuilder.naiveLinkOrder();
  for (  AndroidResourcesProvider resource : Iterables.concat(ruleContext.getPrerequisites("resources",Mode.TARGET,AndroidResourcesProvider.class),ruleContext.getPrerequisites("deps",Mode.TARGET,AndroidResourcesProvider.class))) {
    builder.addTransitive(resource.getTransitiveAndroidResources());
  }
  return builder.build();
}
