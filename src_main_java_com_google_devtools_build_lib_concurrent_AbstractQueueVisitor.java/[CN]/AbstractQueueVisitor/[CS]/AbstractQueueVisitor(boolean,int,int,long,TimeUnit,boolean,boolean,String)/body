{
  Preconditions.checkNotNull(poolName);
  this.concurrent=concurrent;
  this.failFastOnException=failFastOnException;
  this.failFastOnInterrupt=failFastOnInterrupt;
  this.ownThreadPool=true;
  this.pool=concurrent ? new ThreadPoolExecutor(corePoolSize,maxPoolSize,keepAliveTime,units,getWorkQueue(),new ThreadFactoryBuilder().setNameFormat(poolName + " %d").build()) : null;
}
