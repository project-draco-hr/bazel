{
  this.testCase=checkNotNull(testCase);
  this.ruleName=checkNotNull(ruleName);
  this.packageName=checkNotNull(packageName);
  this.targetName=checkNotNull(targetName);
  this.buildString=new StringBuilder().append(String.format("%s(",this.ruleName)).append(String.format("name='%s',",this.targetName));
}
