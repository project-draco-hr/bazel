{
  this.stmts=ImmutableList.<Statement>builder().addAll(preludeStatements).addAll(result.statements).build();
  this.comments=ImmutableList.copyOf(result.comments);
  this.containsErrors=result.containsErrors;
  this.contentHashCode=contentHashCode;
  if (!result.statements.isEmpty()) {
    setLocation(lexer.createLocation(result.statements.get(0).getLocation().getStartOffset(),result.statements.get(result.statements.size() - 1).getLocation().getEndOffset()));
  }
 else {
    setLocation(Location.fromPathFragment(lexer.getFilename()));
  }
}
