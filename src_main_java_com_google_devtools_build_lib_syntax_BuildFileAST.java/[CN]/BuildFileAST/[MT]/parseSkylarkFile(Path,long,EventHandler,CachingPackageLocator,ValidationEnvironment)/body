{
  ParserInputSource input=ParserInputSource.create(file,fileSize);
  Lexer lexer=new Lexer(input,eventHandler,false);
  Parser.ParseResult result=Parser.parseFileForSkylark(lexer,eventHandler,locator,validationEnvironment);
  return new BuildFileAST(lexer,ImmutableList.<Statement>of(),result,HashCode.fromBytes(file.getMD5Digest()).toString());
}
