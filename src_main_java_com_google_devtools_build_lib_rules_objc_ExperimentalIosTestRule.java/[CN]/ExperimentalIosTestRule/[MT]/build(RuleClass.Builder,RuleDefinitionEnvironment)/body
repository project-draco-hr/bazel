{
  return builder.setImplicitOutputsFunction(ImplicitOutputsFunction.fromFunctions(ReleaseBundlingSupport.IPA,XcodeSupport.PBXPROJ)).override(attr(IosTest.TARGET_DEVICE,LABEL).allowedFileTypes().allowedRuleClasses(Constants.IOS_DEVICE_RULE_CLASSES).value(env.getLabel("//tools/objc/sim_devices:default"))).add(attr("plugins",LABEL_LIST).allowedFileTypes(FileType.of("_deploy.jar"))).add(attr("$test_template",LABEL).value(env.getLabel("//tools/objc:ios_test.sh.bazel_template"))).add(attr("$test_runner",LABEL).value(env.getLabel("//tools/objc:testrunner"))).build();
}
