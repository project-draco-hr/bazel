{
  return builder.setImplicitOutputsFunction(ImplicitOutputsFunction.fromFunctions(ReleaseBundlingSupport.IPA,XcodeSupport.PBXPROJ)).override(attr(IosTest.TARGET_DEVICE,LABEL).allowedFileTypes().allowedRuleClasses(Constants.IOS_DEVICE_RULE_CLASSES).value(env.getLabel("//tools/objc/sim_devices:default"))).add(attr("$test_template",LABEL).value(env.getLabel("//tools/objc:ios_test.sh.bazel_template"))).build();
