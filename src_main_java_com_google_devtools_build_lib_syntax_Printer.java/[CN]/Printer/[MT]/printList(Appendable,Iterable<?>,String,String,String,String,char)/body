{
  boolean printSeparator=false;
  int len=0;
  append(buffer,before);
  for (  Object o : list) {
    if (printSeparator) {
      append(buffer,separator);
    }
    write(buffer,o,quotationMark);
    printSeparator=true;
    len++;
  }
  if (singletonTerminator != null && len == 1) {
    append(buffer,singletonTerminator);
  }
  return append(buffer,after);
}
