{
  scratch.file("test/aspect.bzl","def _impl(target, ctx):","  ctx.new_file('missing_in_action.txt')","  return struct()","","MyAspect = aspect(implementation=_impl)");
  scratch.file("test/BUILD","java_library(name = 'xxx',)");
  reporter.removeHandler(failFastHandler);
  try {
    update(ImmutableList.of("test/aspect.bzl%MyAspect"),"//test:xxx");
    fail();
  }
 catch (  ViewCreationFailedException e) {
  }
  assertContainsEvent("ERROR /workspace/test/BUILD:1:1: in java_library rule //test:xxx: \n" + "\n" + "\n"+ "The following files have no generating action:\n"+ "test/missing_in_action.txt\n");
}
