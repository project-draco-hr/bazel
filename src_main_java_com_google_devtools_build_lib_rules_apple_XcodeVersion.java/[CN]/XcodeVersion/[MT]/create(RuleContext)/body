{
  DottedVersion version=DottedVersion.fromString(ruleContext.attributes().get(XcodeVersionRule.VERSION_ATTR_NAME,STRING));
  List<String> aliases=ruleContext.attributes().get(XcodeVersionRule.ALIASES_ATTR_NAME,STRING_LIST);
  return new RuleConfiguredTargetBuilder(ruleContext).addProvider(RunfilesProvider.class,RunfilesProvider.EMPTY).addProvider(XcodeVersionProvider.class,new XcodeVersionProvider(ruleContext.getLabel(),version,aliases)).build();
}
