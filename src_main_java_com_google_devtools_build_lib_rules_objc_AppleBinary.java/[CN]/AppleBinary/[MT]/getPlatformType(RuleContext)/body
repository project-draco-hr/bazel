{
  try {
    return getPlatformType(ruleContext.attributes().get(AppleBinaryRule.PLATFORM_TYPE_ATTR_NAME,STRING));
  }
 catch (  IllegalArgumentException exception) {
    throw ruleContext.throwWithAttributeError(AppleBinaryRule.PLATFORM_TYPE_ATTR_NAME,String.format(UNSUPPORTED_PLATFORM_TYPE_ERROR_FORMAT,ruleContext.attributes().get(AppleBinaryRule.PLATFORM_TYPE_ATTR_NAME,STRING)));
  }
}
