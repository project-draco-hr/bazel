{
  String[] stubApplicationData=getResourceAsString("stub_application_data.txt").split("\n");
  realClassName=stubApplicationData[0];
  packageName=stubApplicationData[1];
  Log.v("StubApplication",String.format("StubApplication created. Android package is %s, real application class is %s.",packageName,realClassName));
  externalResourceFile=getExternalResourceFile();
  IncrementalClassLoader.inject(StubApplication.class.getClassLoader(),packageName,getDexList(packageName));
  realApplication=instantiateRealApplication();
}
