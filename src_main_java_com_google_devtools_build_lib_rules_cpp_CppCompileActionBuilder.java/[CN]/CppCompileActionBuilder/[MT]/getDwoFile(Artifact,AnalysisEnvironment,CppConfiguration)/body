{
  boolean isObjectOutput=CppFileTypes.OBJECT_FILE.matches(outputFile.getExecPath()) || CppFileTypes.PIC_OBJECT_FILE.matches(outputFile.getExecPath());
  if (cppConfiguration != null && cppConfiguration.useFission() && isObjectOutput) {
    return artifactFactory.getDerivedArtifact(FileSystemUtils.replaceExtension(outputFile.getRootRelativePath(),".dwo"),outputFile.getRoot());
  }
 else {
    return null;
  }
}
