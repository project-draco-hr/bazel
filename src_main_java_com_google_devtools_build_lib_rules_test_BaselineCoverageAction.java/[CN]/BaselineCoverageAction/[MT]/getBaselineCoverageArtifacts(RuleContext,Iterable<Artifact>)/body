{
  Artifact coverageData=ruleContext.getAnalysisEnvironment().getDerivedArtifact(Util.getWorkspaceRelativePath(ruleContext.getTarget()).getChild("baseline_coverage.dat"),ruleContext.getConfiguration().getTestLogsDirectory());
  ruleContext.registerAction(new BaselineCoverageAction(ruleContext.getActionOwner(),instrumentedFiles,coverageData));
  return ImmutableList.of(coverageData);
}
