{
  return builder.requiresConfigurationFragments(AppleConfiguration.class).exemptFromConstraintChecking("this rule refines configuration variables and does not build actual content").add(attr(VERSION_ATTR_NAME,STRING).mandatory().nonconfigurable("this rule determines configuration")).add(attr(ALIASES_ATTR_NAME,STRING_LIST).nonconfigurable("this rule determines configuration")).add(attr(DEFAULT_IOS_SDK_VERSION_ATTR_NAME,STRING).nonconfigurable("this rule determines configuration")).add(attr(DEFAULT_WATCHOS_SDK_VERSION_ATTR_NAME,STRING).nonconfigurable("this rule determines configuration")).add(attr(DEFAULT_TVOS_SDK_VERSION_ATTR_NAME,STRING).nonconfigurable("this rule determines configuration")).add(attr(DEFAULT_MACOSX_SDK_VERSION_ATTR_NAME,STRING).nonconfigurable("this rule determines configuration")).build();
}
