{
  this.currentBuild=new WebStatusBuildLog(event.getCommandId());
  this.serverModule.commandStarted();
  String webStatusServerUrl="http://localhost:" + port;
  this.reporter.handle(Event.info("Status page: " + webStatusServerUrl + "/tests/"+ this.currentBuild.getCommandId()+ " (alternative link: "+ webStatusServerUrl+ WebStatusServerModule.LAST_TEST_URI+ " )"));
}
