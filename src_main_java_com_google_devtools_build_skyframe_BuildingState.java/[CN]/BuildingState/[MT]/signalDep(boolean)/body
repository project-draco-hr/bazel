{
  signaledDeps++;
  if (isDirty() && !rebuilding()) {
    if (childChanged) {
      dirtyState=NodeEntry.DirtyState.REBUILDING;
    }
 else     if (dirtyState == NodeEntry.DirtyState.CHECK_DEPENDENCIES && isReady() && dirtyDirectDepIterator == null) {
      dirtyState=NodeEntry.DirtyState.VERIFIED_CLEAN;
    }
  }
  return isReady();
}
