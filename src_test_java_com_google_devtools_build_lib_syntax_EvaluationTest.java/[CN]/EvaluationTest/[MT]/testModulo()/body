{
  assertThat(eval("6 % 2")).isEqualTo(0);
  assertThat(eval("6 % 4")).isEqualTo(2);
  assertThat(eval("3 % 6")).isEqualTo(3);
  assertThat(eval("7 % -4")).isEqualTo(-1);
  assertThat(eval("-7 % 4")).isEqualTo(1);
  assertThat(eval("-7 % -4")).isEqualTo(-3);
  checkEvalError("integer modulo by zero","5 % 0");
}
