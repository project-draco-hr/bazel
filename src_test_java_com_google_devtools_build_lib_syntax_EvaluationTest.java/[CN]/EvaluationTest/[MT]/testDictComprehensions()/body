{
  assertEquals(Collections.emptyMap(),eval("{a : a for a in []}"));
  assertEquals(ImmutableMap.of(1,1,2,2),eval("{b : b for b in [1, 2]}"));
  assertEquals(ImmutableMap.of("a","v_a","b","v_b"),eval("{c : 'v_' + c for c in ['a', 'b']}"));
  assertEquals(ImmutableMap.of("k_a","a","k_b","b"),eval("{'k_' + d : d for d in ['a', 'b']}"));
  assertEquals(ImmutableMap.of("k_a","v_a","k_b","v_b"),eval("{'k_' + e : 'v_' + e for e in ['a', 'b']}"));
  assertEquals(ImmutableMap.of(5,6),eval("{x+y : x*y for x, y in [[2, 3]]}"));
}
