{
  assertEquals(Collections.emptyMap(),eval("{x : x for x in []}"));
  assertEquals(ImmutableMap.of(1,1,2,2),eval("{x : x for x in [1, 2]}"));
  assertEquals(ImmutableMap.of("a","v_a","b","v_b"),eval("{x : 'v_' + x for x in ['a', 'b']}"));
  assertEquals(ImmutableMap.of("k_a","a","k_b","b"),eval("{'k_' + x : x for x in ['a', 'b']}"));
  assertEquals(ImmutableMap.of("k_a","v_a","k_b","v_b"),eval("{'k_' + x : 'v_' + x for x in ['a', 'b']}"));
}
