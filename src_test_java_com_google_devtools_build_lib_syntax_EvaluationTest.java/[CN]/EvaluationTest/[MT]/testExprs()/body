{
  newTest().testStatement("'%sx' % 'foo' + 'bar'","fooxbar").testStatement("('%sx' % 'foo') + 'bar'","fooxbar").testStatement("'%sx' % ('foo' + 'bar')","foobarx").testStatement("123 + 456",579).testStatement("456 - 123",333).testStatement("8 % 3",2).testIfErrorContains("unsupported operand type(s) for %: 'int' and 'string'","3 % 'foo'");
}
