{
  Function sum=new AbstractFunction("sum"){
    @Override public Object call(    List<Object> args,    Map<String,Object> kwargs,    FuncallExpression ast,    Environment env){
      int sum=0;
      for (      Object arg : args) {
        sum+=(Integer)arg;
      }
      return sum;
    }
  }
;
  update(sum.getName(),sum);
  assertEquals(21,eval("sum(1, 2, 3, 4, 5, 6)"));
  assertEquals(sum,eval("sum"));
  assertEquals(0,eval("sum(a=1, b=2)"));
}
