{
  Object x=eval("[1,2] + [3,4]");
  assertEquals(Arrays.asList(1,2,3,4),x);
  assertFalse(EvalUtils.isImmutable(x));
  x=eval("(1,2) + (3,4)");
  assertEquals(Arrays.asList(1,2,3,4),x);
  assertTrue(EvalUtils.isImmutable(x));
  checkEvalError("(1,2) + [3,4]","can only concatenate list (not \"tuple\") to list");
}
