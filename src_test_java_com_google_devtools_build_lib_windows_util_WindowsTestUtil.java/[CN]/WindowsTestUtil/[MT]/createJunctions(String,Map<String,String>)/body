{
  List<String> args=new ArrayList<>();
  boolean first=true;
  for (  Map.Entry<String,String> e : links.entrySet()) {
    if (first) {
      args.add("cmd.exe /c");
      first=false;
    }
 else {
      args.add("&&");
    }
    args.add(String.format("mklink /j \"%s/%s\" \"%s/%s\"",scratchRoot,e.getKey(),scratchRoot,e.getValue()));
  }
  runCommand(args);
}
