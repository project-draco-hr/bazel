{
  Iterable<XcodeVersionProvider> xcodeVersionProviders=ruleContext.getPrerequisites(XcodeConfigRule.VERSIONS_ATTR_NAME,Mode.TARGET,XcodeVersionProvider.class);
  ImmutableList.Builder<Label> labelsContainingAlias=ImmutableList.builder();
  for (  XcodeVersionProvider versionProvider : xcodeVersionProviders) {
    if (versionProvider.getAliases().contains(alias) || versionProvider.getVersion().toString().equals(alias)) {
      labelsContainingAlias.add(versionProvider.getLabel());
    }
  }
  ruleContext.ruleError(String.format("'%s' is registered to multiple labels (%s) in a single xcode version " + "configuration",alias,Joiner.on(", ").join(labelsContainingAlias.build())));
}
