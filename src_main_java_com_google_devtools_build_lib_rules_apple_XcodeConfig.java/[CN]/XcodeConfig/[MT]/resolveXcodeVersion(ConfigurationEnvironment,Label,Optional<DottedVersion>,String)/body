{
  Rule xcodeConfigRule=getRuleForLabel(xcodeConfigLabel,"xcode_config",env,errorDescription);
  DottedVersion dottedVersion=resolveExplicitlyDefinedVersion(env,xcodeConfigRule,xcodeVersionOverrideFlag);
  if (dottedVersion != null) {
    return Optional.of(dottedVersion);
  }
 else {
    return xcodeVersionOverrideFlag;
  }
}
