{
  Optional<DottedVersion> xcodeVersion=xcodeConfigProvider.getXcodeVersion();
  if (config.getBitcodeMode() != AppleBitcodeMode.NONE && xcodeVersion.isPresent() && xcodeVersion.get().compareTo(MINIMUM_BITCODE_XCODE_VERSION) < 0) {
    ruleContext.ruleError(String.format("apple_bitcode mode '%s' is unsupported for xcode version '%s'",config.getBitcodeMode(),xcodeVersion.get()));
  }
}
