{
  PathFragment pkg=packageIdentifier.getPackageFragment();
  for (int i=pkg.segmentCount(); i >= 0; i--) {
    PackageIdentifier prefix=PackageIdentifier.create(packageIdentifier.getRepository(),pkg.subFragment(0,i));
    if (prefixes.contains(prefix)) {
      return prefix;
    }
  }
  return null;
}
