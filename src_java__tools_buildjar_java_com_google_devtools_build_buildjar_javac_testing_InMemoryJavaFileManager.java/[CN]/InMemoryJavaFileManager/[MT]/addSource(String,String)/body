{
  Path path=fileSystem.getPath("/",SLASH_MATCHER.trimLeadingFrom(DOT_MATCHER.replaceFrom(className,'/') + ".java"));
  try {
    Files.createDirectories(path.getParent());
    Files.write(path,Arrays.asList(lines),UTF_8);
  }
 catch (  IOException e) {
    throw new IOError(e);
  }
  sources.add(path);
}
