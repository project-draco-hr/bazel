{
  Path execRoot=directories.getExecRoot().getRelative(repositoryName.getPathUnderExecRoot());
  Path outputDir=execRoot.getRelative(directories.getRelativeOutputPath()).getRelative(outputDirName);
  if (middleman) {
    return INTERNER.intern(Root.middlemanRoot(execRoot,outputDir,repositoryName.isMain()));
  }
  return INTERNER.intern(Root.asDerivedRoot(execRoot,outputDir.getRelative(name),repositoryName.isMain()));
}
