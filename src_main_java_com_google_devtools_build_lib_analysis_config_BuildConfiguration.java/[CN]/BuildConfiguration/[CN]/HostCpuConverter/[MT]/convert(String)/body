{
  if (input.isEmpty()) {
switch (OS.getCurrent()) {
case DARWIN:
      return "darwin";
case FREEBSD:
    return "freebsd";
case WINDOWS:
switch (CPU.getCurrent()) {
case X86_64:
    return "x64_windows";
}
break;
case LINUX:
switch (CPU.getCurrent()) {
case X86_32:
return "piii";
case X86_64:
return "k8";
case ARM:
return "arm";
}
}
return "unknown";
}
return input;
}
