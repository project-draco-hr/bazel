{
  if (transition == Attribute.ConfigurationTransition.NONE) {
    return;
  }
 else   if (this.transition != HostTransition.INSTANCE) {
    Verify.verify(this.transition == Attribute.ConfigurationTransition.NONE || this.transition.toString().contains("LipoDataTransition"));
    this.transition=getCurrentTransitions().getDynamicTransition(transition);
  }
}
