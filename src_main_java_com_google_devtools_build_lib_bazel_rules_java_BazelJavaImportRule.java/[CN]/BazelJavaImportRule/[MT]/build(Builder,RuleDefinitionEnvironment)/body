{
  return builder.add(attr("deps",LABEL_LIST).allowedRuleClasses(ALLOWED_DEPS).allowedFileTypes().validityPredicate(ANY_EDGE)).add(attr("exports",LABEL_LIST).allowedRuleClasses(ALLOWED_DEPS).allowedFileTypes().validityPredicate(ANY_EDGE)).add(attr("runtime_deps",LABEL_LIST).allowedFileTypes(JavaSemantics.JAR).allowedRuleClasses(ALLOWED_DEPS).skipAnalysisTimeFileTypeCheck()).build();
}
