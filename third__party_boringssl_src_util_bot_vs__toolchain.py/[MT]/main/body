def main():
    if (not sys.platform.startswith(('win32', 'cygwin'))):
        return 0
    commands = {'update': Update, }
    if ((len(sys.argv) < 2) or (sys.argv[1] not in commands)):
        print  >> sys.stderr, ('Expected one of: %s' % ', '.join(commands))
        return 1
    return commands[sys.argv[1]](*sys.argv[2:])
