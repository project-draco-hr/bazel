{
  scratch.file("recursive/BUILD","filegroup(name = 'x', srcs = ['foo', '//recursivetoo:x'])");
  scratch.file("recursivetoo/BUILD","filegroup(name = 'x', srcs = ['foo', '//recursive:x'])");
  ResolvedTargets<Target> result=parseCompileOneDep("//recursive:foo","//recursivetoo:foo");
  assertFalse(result.hasError());
  assertThat(result.getTargets()).hasSize(2);
}
