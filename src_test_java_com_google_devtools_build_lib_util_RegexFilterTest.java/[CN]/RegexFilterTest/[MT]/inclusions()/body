{
  createFilter("a/b,+^c,_test$");
  assertEquals("(?:(?>a/b)|(?>^c)|(?>_test$))",filter.toString());
  assertIncluded("a/b");
  assertIncluded("a/b/c");
  assertIncluded("c");
  assertIncluded("c/d");
  assertIncluded("e/a/b");
  assertIncluded("f/1/2/3/_test");
  assertExcluded("a");
  assertExcluded("a/c");
  assertExcluded("d");
  assertExcluded("e/f/g");
  assertExcluded("f/_test2");
}
