{
  createFilter("-a/b,-^c,-_test$");
  assertEquals("-(?:(?>a/b)|(?>^c)|(?>_test$))",filter.toString());
  assertExcluded("a/b");
  assertExcluded("a/b/c");
  assertExcluded("c");
  assertExcluded("c/d");
  assertExcluded("f/a/b/d");
  assertExcluded("f/a_test");
  assertIncluded("a");
  assertIncluded("a/c");
  assertIncluded("d");
  assertIncluded("e/f/g");
  assertIncluded("f/a_test_case");
}
