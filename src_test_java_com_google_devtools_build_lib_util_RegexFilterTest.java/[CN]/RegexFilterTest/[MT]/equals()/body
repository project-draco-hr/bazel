{
  new EqualsTester().addEqualityGroup(createFilter("a,b,c"),createFilter("a,b,c")).addEqualityGroup(createFilter("a,b,c,d")).addEqualityGroup(createFilter("a,b,-c"),createFilter("a,b,-c")).addEqualityGroup(createFilter("a,b,-c,-d")).addEqualityGroup(createFilter("-a,-b,-c"),createFilter("-a,-b,-c")).addEqualityGroup(createFilter("-a,-b,-c,-d")).addEqualityGroup(createFilter(""),createFilter("")).testEquals();
}
