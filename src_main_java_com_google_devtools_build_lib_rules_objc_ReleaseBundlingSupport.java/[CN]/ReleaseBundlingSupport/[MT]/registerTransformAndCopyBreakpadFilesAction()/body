{
  for (  Entry<Artifact,Artifact> breakpadFiles : getBreakpadFiles().entrySet()) {
    ruleContext.registerAction(new SpawnAction.Builder().setMnemonic("CopyBreakpadFile").setShellCommand(String.format("sed -r \"1 s/^(MODULE \\w* \\w* \\w*).*$/\\1 %s/\" < %s > %s",ruleContext.getLabel().getName(),breakpadFiles.getKey().getExecPathString(),breakpadFiles.getValue().getExecPathString())).addInput(breakpadFiles.getKey()).addOutput(breakpadFiles.getValue()).build(ruleContext));
  }
}
