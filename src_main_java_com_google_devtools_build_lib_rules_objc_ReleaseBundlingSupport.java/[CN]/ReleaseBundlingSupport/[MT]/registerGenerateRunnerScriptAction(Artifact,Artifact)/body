{
  ImmutableList<Substitution> substitutions=ImmutableList.of(Substitution.of("%app_name%",ruleContext.getLabel().getName()),Substitution.of("%ipa_file%",ipaInput.getRootRelativePath().getPathString()),Substitution.of("%iossim%",attributes.iossim().getRootRelativePath().getPathString()));
  ruleContext.registerAction(new TemplateExpansionAction(ruleContext.getActionOwner(),attributes.runnerScriptTemplate(),runnerScript,substitutions,true));
  return this;
}
