{
  String fingerprintCommand="/usr/libexec/PlistBuddy -c 'Print DeveloperCertificates:0' /dev/stdin <<< " + "$(" + extractPlistCommand(provisioningProfile) + ") | "+ "openssl x509 -inform DER -noout -fingerprint | "+ "cut -d= -f2 | sed -e 's#:##g'";
  return String.format("/usr/bin/codesign --force --sign $(%s) --entitlements %s %s",fingerprintCommand,entitlements.getExecPathString(),appDir);
}
