def _CallIncrementalInstall(self, incremental, native_libs=None, start_type='no'):
    if incremental:
        apk = None
    else:
        apk = self._APK
    incremental_install.IncrementalInstall(adb_path=self._ADB_PATH, execroot=self._EXEC_ROOT, stub_datafile=self._STUB_DATAFILE, dexmanifest=self._DEXMANIFEST, apk=apk, resource_apk=self._RESOURCE_APK, native_libs=native_libs, output_marker=self._OUTPUT_MARKER, adb_jobs=1, start_type=start_type, user_home_dir='/home/root')
