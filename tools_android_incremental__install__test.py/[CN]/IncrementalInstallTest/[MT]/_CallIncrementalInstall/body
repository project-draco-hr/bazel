def _CallIncrementalInstall(self, incremental, start_app=False):
    if incremental:
        apk = None
    else:
        apk = self._APK
    incremental_install.IncrementalInstall(adb_path=self._ADB_PATH, execroot=self._EXEC_ROOT, stub_datafile=self._STUB_DATAFILE, dexmanifest=self._DEXMANIFEST, apk=apk, resource_apk=self._RESOURCE_APK, output_marker=self._OUTPUT_MARKER, adb_jobs=1, start_app=start_app, user_home_dir='/home/root')
