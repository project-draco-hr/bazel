{
  List<Throwable> dynamicFailures=dynamicTestToFailures.get(test);
  boolean failed=!globalFailures.isEmpty() || !dynamicFailures.isEmpty();
  return new TestResult.Builder().name(test.getDisplayName()).className(getDescription().getDisplayName()).properties(Collections.<String,String>emptyMap()).failures(dynamicFailures).runTimeInterval(runTime).status(status).numTests(1).numFailures(failed ? 1 : 0).childResults(Collections.<TestResult>emptyList()).build();
}
