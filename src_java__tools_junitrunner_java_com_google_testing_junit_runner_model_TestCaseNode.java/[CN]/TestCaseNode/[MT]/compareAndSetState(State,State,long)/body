{
  if (fromState == null || toState == null || state == null) {
    throw new NullPointerException();
  }
  if (fromState == state && toState != state) {
    state=toState;
    runTimeInterval=runTimeInterval == null ? new Interval(now,now) : runTimeInterval.withEndMillis(now);
    return true;
  }
  return false;
}
