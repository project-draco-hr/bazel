{
  AndroidSdkProvider sdk=AndroidSdkProvider.fromRuleContext(ruleContext);
  ruleContext.registerAction(new SpawnAction.Builder().setExecutable(sdk.getAidl()).addInput(idl).addOutput(preprocessed).addArgument("--preprocess").addArgument(preprocessed.getExecPathString()).addArgument(idl.getExecPathString()).setProgressMessage("Android IDL preprocessing").setMnemonic("AndroidIDLPreprocess").build(ruleContext));
}
