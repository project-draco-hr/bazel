{
  Path buildFile=scratch.file("" + packageId.getPathFragment() + "/BUILD",lines);
  registerBuildFile(packageId,buildFile);
  Package.Builder externalPkg=Package.newExternalPackageBuilder(buildFile.getRelative("WORKSPACE"),"TESTING");
  externalPkg.setWorkspaceName(TestConstants.WORKSPACE_NAME);
  return packageFactory.createPackageForTesting(packageId,externalPkg.build(),buildFile,packageLocator,reporter);
}
