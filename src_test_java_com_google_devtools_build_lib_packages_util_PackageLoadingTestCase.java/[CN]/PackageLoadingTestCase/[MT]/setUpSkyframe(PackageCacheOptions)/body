{
  PathPackageLocator pkgLocator=PathPackageLocator.create(packageCacheOptions.packagePath,reporter,rootDirectory,rootDirectory);
  skyframeExecutor.preparePackageLoading(pkgLocator,packageCacheOptions.defaultVisibility,true,ruleClassProvider.getDefaultsPackageContent(),UUID.randomUUID());
  skyframeExecutor.setDeletedPackages(ImmutableSet.copyOf(packageCacheOptions.deletedPackages));
}
