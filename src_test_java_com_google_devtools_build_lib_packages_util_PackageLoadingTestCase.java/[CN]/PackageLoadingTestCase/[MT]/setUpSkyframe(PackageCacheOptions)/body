{
  PathPackageLocator pkgLocator=PathPackageLocator.create(outputBase,packageCacheOptions.packagePath,reporter,rootDirectory,rootDirectory);
  OptionsParser enforcedTestOptions=enforceTestInvocationPolicyOnRuleClassProvider(ruleClassProvider);
  skyframeExecutor.preparePackageLoading(pkgLocator,packageCacheOptions.defaultVisibility,true,7,ruleClassProvider.getDefaultsPackageContent(enforcedTestOptions),UUID.randomUUID());
  skyframeExecutor.setDeletedPackages(ImmutableSet.copyOf(packageCacheOptions.getDeletedPackages()));
}
