{
  assertEquals(buildFile_3A,locatorWithSymlinks.getPackageBuildFile(PackageIdentifier.createInDefaultRepo("A")));
  assertEquals(buildFile_3B,locatorWithSymlinks.getPackageBuildFile(PackageIdentifier.createInDefaultRepo("B")));
  assertEquals(buildFile_3CI,locatorWithSymlinks.getPackageBuildFile(PackageIdentifier.createInDefaultRepo("C/I")));
  try {
    locatorWithSymlinks.getPackageBuildFile(PackageIdentifier.createInDefaultRepo("C/D"));
    fail();
  }
 catch (  BuildFileNotFoundException e) {
    assertThat(e).hasMessage("no such package 'C/D': BUILD file not found on package path");
  }
}
