{
  super(BuildInfoHelper.BUILD_INFO_ACTION_OWNER,Artifact.NO_ARTIFACTS,ImmutableList.of(stableStatus,volatileStatus));
  this.options=options;
  this.stableStatus=stableStatus;
  this.volatileStatus=volatileStatus;
  this.username=System.getProperty("user.name");
  this.hostname=NetUtil.findShortHostName();
  this.timestamp=System.currentTimeMillis();
  this.getWorkspaceStatusCommand=options.get().workspaceStatusCommand.equals(EMPTY_FRAGMENT) ? null : new CommandBuilder().addArgs(options.get().workspaceStatusCommand.toString()).setEnv(runtime.getClientEnv()).setWorkingDir(runtime.getWorkspace()).useShell(true).build();
}
