{
  Preconditions.checkNotNull(universeScope);
  if (canUseSkyQuery(orderedResults,universeScope,packagePath)) {
    return new SkyQueryEnvironment(keepGoing,strictScope,loadingPhaseThreads,labelFilter,eventHandler,settings,functions,targetPatternEvaluator.getOffset(),graphFactory,universeScope,packagePath);
  }
 else {
    return new BlazeQueryEnvironment(transitivePackageLoader,packageProvider,targetPatternEvaluator,keepGoing,strictScope,loadingPhaseThreads,labelFilter,eventHandler,settings,functions);
  }
}
