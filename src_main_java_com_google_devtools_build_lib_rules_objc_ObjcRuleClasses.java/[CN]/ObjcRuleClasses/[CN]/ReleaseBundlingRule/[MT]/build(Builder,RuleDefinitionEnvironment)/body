{
  return builder.add(attr("entitlements",LABEL).legacyAllowAnyFileType()).add(attr("provisioning_profile",LABEL).value(env.getLabel("//tools/objc:default_provisioning_profile")).allowedFileTypes(FileType.of(".mobileprovision"))).add(attr("app_icon",STRING)).add(attr("launch_image",STRING)).add(attr("bundle_id",STRING).value(new Attribute.ComputedDefault(){
    @Override public Object getDefault(    AttributeMap rule){
      return "example." + rule.getName();
    }
  }
)).add(attr("families",STRING_LIST).value(ImmutableList.of(TargetDeviceFamily.IPHONE.getNameInRule()))).add(attr("$bundlemerge",LABEL).cfg(HOST).exec().value(env.getLabel("//tools/objc:bundlemerge"))).build();
}
