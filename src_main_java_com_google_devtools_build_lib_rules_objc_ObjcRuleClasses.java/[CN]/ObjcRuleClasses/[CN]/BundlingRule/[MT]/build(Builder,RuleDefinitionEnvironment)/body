{
  return builder.add(attr("infoplist",LABEL).allowedFileTypes(PLIST_TYPE)).add(attr("infoplists",BuildType.LABEL_LIST).allowedFileTypes(PLIST_TYPE)).add(attr("families",STRING_LIST).value(ImmutableList.of(TargetDeviceFamily.IPHONE.getNameInRule()))).add(attr("$momcwrapper",LABEL).cfg(HOST).exec().value(env.getToolsLabel("//tools/objc:momcwrapper"))).add(attr("$swiftstdlibtoolwrapper",LABEL).cfg(HOST).exec().value(env.getToolsLabel("//tools/objc:swiftstdlibtoolwrapper"))).build();
}
