{
  return builder.add(attr("srcs",LABEL_LIST).direct_compile_time_input().allowedFileTypes(SRCS_TYPE)).add(attr("non_arc_srcs",LABEL_LIST).direct_compile_time_input().allowedFileTypes(NON_ARC_SRCS_TYPE)).add(attr("pch",LABEL).direct_compile_time_input().allowedFileTypes(FileType.of(".pch"))).override(attr("deps",LABEL_LIST).direct_compile_time_input().allowedRuleClasses(ALLOWED_DEPS_RULE_CLASSES).allowedFileTypes()).add(attr("non_propagated_deps",LABEL_LIST).direct_compile_time_input().allowedRuleClasses(ALLOWED_DEPS_RULE_CLASSES).allowedFileTypes()).add(attr("defines",STRING_LIST)).add(attr("enable_modules",BOOLEAN)).build();
}
