{
  if (!ruleContext.attributes().has("launch_storyboard",LABEL)) {
    return false;
  }
  Artifact launchStoryboard=ruleContext.getPrerequisiteArtifact("launch_storyboard",Mode.TARGET);
  DottedVersion flagMinimumOs=objcConfiguration(ruleContext).getMinimumOs();
  return launchStoryboard != null && flagMinimumOs.compareTo(MIN_LAUNCH_STORYBOARD_OS_VERSION) >= 0;
}
