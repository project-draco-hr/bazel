{
  PathFragment relativePath=(owningExecutable != null) ? owningExecutable.getRootRelativePath() : Util.getWorkspaceRelativePath(context.getRule());
  String basename=relativePath.getBaseName();
  PathFragment inputManifestPath=relativePath.replaceName(basename + ".runfiles_manifest");
  return context.getAnalysisEnvironment().getDerivedArtifact(inputManifestPath,context.getConfiguration().getBinDirectory());
}
