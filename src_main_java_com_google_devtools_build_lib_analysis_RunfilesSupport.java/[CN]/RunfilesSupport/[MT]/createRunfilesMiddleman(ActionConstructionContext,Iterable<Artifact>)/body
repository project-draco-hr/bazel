{
  Iterable<Artifact> inputs=IterablesChain.<Artifact>builder().add(allRunfilesArtifacts).addElement(runfilesManifest).build();
  return context.getAnalysisEnvironment().getMiddlemanFactory().createRunfilesMiddleman(context.getActionOwner(),owningExecutable,inputs,context.getConfiguration().getMiddlemanDirectory());
}
