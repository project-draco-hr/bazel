{
  Label label=null;
  try {
    label=Label.parseAbsolute(target);
    env.getTarget(label);
    return label;
  }
 catch (  Label.SyntaxException|NoSuchPackageException|NoSuchTargetException e) {
    throw new InvalidConfigurationException("Error parsing or loading " + target + ": "+ e.getMessage(),e);
  }
}
