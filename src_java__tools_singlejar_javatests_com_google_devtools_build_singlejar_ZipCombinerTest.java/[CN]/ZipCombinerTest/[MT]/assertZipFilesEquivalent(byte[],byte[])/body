{
  assertEquals(x.length,y.length);
  for (int i=0; i < x.length; i++) {
    if (x[i] != y[i]) {
      assertEquals("at position " + i,0x08,x[i] ^ y[i]);
      assertTrue(x[i - 7] == 'P' && x[i - 6] == 'K' && x[i - 5] == 3 && x[i - 4] == 4 || x[i - 9] == 'P' && x[i - 8] == 'K' && x[i - 7] == 1 && x[i - 6] == 2);
    }
  }
}
