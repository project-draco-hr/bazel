{
  IterablesChain.Builder<String> builder=IterablesChain.builder();
  assertTrue(builder.isEmpty());
  builder.addElement("a");
  assertFalse(builder.isEmpty());
  builder=IterablesChain.builder();
  assertTrue(builder.isEmpty());
  builder.add(ImmutableList.of("a"));
  assertFalse(builder.isEmpty());
}
