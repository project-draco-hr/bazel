{
  events.setFailFast(false);
  Path buildFile=scratch.file("/foo/bar/BUILD","include(\"//nonexistent:foo\")\n");
  BuildFileAST buildFileAST=BuildFileAST.parseBuildFile(buildFile,events.reporter(),emptyLocator,false);
  assertEquals(1,buildFileAST.getStatements().size());
  events.assertContainsEvent("Package 'nonexistent' not found");
}
