{
  setFailFast(false);
  Path buildFile=scratch.file("/foo/bar/BUILD","include(\"//nonexistent:foo\")\n");
  BuildFileAST buildFileAST=BuildFileAST.parseBuildFile(buildFile,getEventHandler(),Environment.EMPTY_PACKAGE_LOCATOR,false);
  assertThat(buildFileAST.getStatements()).hasSize(1);
  assertContainsEvent("Package 'nonexistent' not found");
}
