{
  Path buildFile=scratch.file("/BUILD","# a file in the build language","","x = [1,2,'foo',4] + [1,2, \"%s%d\" % ('foo', 1)]");
  BuildFileAST buildfile=BuildFileAST.parseBuildFile(buildFile,getEventHandler(),false);
  assertTrue(buildfile.exec(env,getEventHandler()));
  assertEquals(Arrays.<Object>asList(1,2,"foo",4,1,2,"foo1"),env.lookup("x"));
}
