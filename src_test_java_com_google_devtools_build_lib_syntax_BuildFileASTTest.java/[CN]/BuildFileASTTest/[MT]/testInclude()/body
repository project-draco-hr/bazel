{
  scratch.file("/foo/bar/BUILD","c = 4\n" + "d = 5\n");
  Path buildFile=scratch.file("/BUILD","a = 2\n" + "include(\"//foo/bar:BUILD\")\n" + "b = 4\n");
  BuildFileAST buildFileAST=BuildFileAST.parseBuildFile(buildFile,getEventHandler(),locator,false);
  assertFalse(buildFileAST.containsErrors());
  assertThat(buildFileAST.getStatements()).hasSize(5);
}
