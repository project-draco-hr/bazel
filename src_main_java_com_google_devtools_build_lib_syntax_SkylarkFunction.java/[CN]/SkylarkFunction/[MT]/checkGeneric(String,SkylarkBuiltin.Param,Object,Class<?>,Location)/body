{
  if (!genericType.equals(Object.class) && !param.generic1().isAssignableFrom(genericType)) {
    String mainType=EvalUtils.getDataTypeNameFromClass(param.type());
    throw new EvalException(loc,String.format("expected %s of %ss for '%s' but got %s of %ss instead\n%s.%s: %s",mainType,EvalUtils.getDataTypeNameFromClass(param.generic1()),paramName,EvalUtils.getDatatypeName(value),EvalUtils.getDataTypeNameFromClass(genericType),getName(),paramName,param.doc()));
  }
}
