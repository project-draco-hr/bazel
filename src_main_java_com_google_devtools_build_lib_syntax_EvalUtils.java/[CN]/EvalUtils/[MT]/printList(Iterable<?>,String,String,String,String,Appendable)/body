{
  boolean printSeparator=false;
  int len=0;
  buffer.append(before);
  for (  Object o : list) {
    if (printSeparator) {
      buffer.append(separator);
    }
    prettyPrintValue(o,buffer);
    printSeparator=true;
    len++;
  }
  if (singletonTerminator != null && len == 1) {
    buffer.append(singletonTerminator);
  }
  buffer.append(after);
}
