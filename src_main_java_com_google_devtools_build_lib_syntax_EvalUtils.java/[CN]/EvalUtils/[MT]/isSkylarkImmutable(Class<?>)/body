{
  if (c.isAnnotationPresent(Immutable.class)) {
    return true;
  }
 else   if (c.equals(String.class) || c.equals(Integer.class) || c.equals(Boolean.class)|| SkylarkList.class.isAssignableFrom(c)|| ImmutableMap.class.isAssignableFrom(c)|| NestedSet.class.isAssignableFrom(c)) {
    return true;
  }
 else {
    for (    Class<?> classObject : quasiImmutableClasses) {
      if (classObject.isAssignableFrom(c)) {
        return true;
      }
    }
  }
  return false;
}
