{
  if (o instanceof Tuple) {
    for (    Object item : (Tuple)o) {
      if (!isImmutable(item)) {
        return false;
      }
    }
    return true;
  }
  if (o instanceof SkylarkMutable) {
    return false;
  }
  if (o instanceof SkylarkValue) {
    return ((SkylarkValue)o).isImmutable();
  }
  return isImmutable(o.getClass());
}
