{
  if (o instanceof SkylarkValue) {
    return ((SkylarkValue)o).isImmutable();
  }
  if (!(o instanceof List<?>)) {
    return isImmutable(o.getClass());
  }
  if (!isTuple((List<?>)o)) {
    return false;
  }
  for (  Object item : (List<?>)o) {
    if (!isImmutable(item)) {
      return false;
    }
  }
  return true;
}
