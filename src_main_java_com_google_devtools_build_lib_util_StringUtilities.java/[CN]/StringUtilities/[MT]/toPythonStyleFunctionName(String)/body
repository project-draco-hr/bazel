{
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < javaStyleFunctionName.length(); i++) {
    char c=javaStyleFunctionName.charAt(i);
    if (Character.isUpperCase(c)) {
      sb.append('_').append(Character.toLowerCase(c));
    }
 else {
      sb.append(c);
    }
  }
  return sb.toString();
}
