{
  StringBuilder builder=new StringBuilder();
  for (  String parent : parents) {
    builder.append("# " + parent + "\n");
  }
  builder.append("maven_jar(\n" + "    name = \"" + name() + "\",\n"+ "    artifact = \""+ toMavenArtifactString()+ "\",\n"+ (hasCustomRepository() ? "    repository = \"" + repository + "\",\n" : "")+ ")");
  return builder.toString();
}
