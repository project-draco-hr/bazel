{
  RootedPath fileRootedPath=RootedPath.toRootedPath(pkg.getSourceRoot(),pkg.getPackageIdentifier().getPackageFragment().getRelative(fileName));
  FileValue result=(FileValue)env.getValue(FileValue.key(fileRootedPath));
  if (result != null && !result.exists()) {
    throw new IOException();
  }
}
