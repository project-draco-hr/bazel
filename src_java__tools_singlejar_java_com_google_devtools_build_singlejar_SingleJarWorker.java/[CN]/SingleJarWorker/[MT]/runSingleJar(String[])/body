{
  try {
    return SingleJar.singleRun(args);
  }
 catch (  IOException e) {
    System.err.println("SingleJar threw exception : " + e.getMessage());
    return 1;
  }
catch (  Exception e) {
    originalSterr.println("SingleJar threw an unexpected exception : " + e.getMessage());
    e.printStackTrace(originalSterr);
    System.exit(1);
    return 1;
  }
}
