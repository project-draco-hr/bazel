{
  Package pkg=scratchPackage("pkg","sh_library(","    name = 'bread',","    srcs = select({","        ':one': ['rye.sh'],","        ':two': ['wheat.sh'],","    }))");
  Package deserialized=deserializer.deserialize(codedInputFromPackage(pkg));
  AttributeMap attrs=RawAttributeMapper.of(deserialized.getRule("bread"));
  assertThat(attrs.get("srcs",BuildType.LABEL_LIST)).containsExactly(Label.parseAbsolute("//pkg:rye.sh"),Label.parseAbsolute("//pkg:wheat.sh"));
}
