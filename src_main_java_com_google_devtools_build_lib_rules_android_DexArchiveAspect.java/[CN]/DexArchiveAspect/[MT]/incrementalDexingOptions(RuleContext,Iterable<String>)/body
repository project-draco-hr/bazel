{
  Set<String> args=new TreeSet<>();
  if (ruleContext.getConfiguration().isCodeCoverageEnabled()) {
    args.add("--nolocals");
  }
  Iterables.addAll(args,Iterables.transform(Iterables.filter(dexopts,new FlagMatcher(getAndroidConfig(ruleContext).getDexoptsSupportedInIncrementalDexing())),FlagConverter.DX_TO_DEXBUILDER));
  return ImmutableList.copyOf(args);
}
