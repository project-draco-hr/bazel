{
  AspectDefinition.Builder result=new AspectDefinition.Builder(NAME).requireProvider(JavaCompilationArgsProvider.class).add(attr("$dexbuilder",LABEL).cfg(HOST).exec().value(Label.parseAbsoluteUnchecked(TOOLS_REPOSITORY + "//tools/android:dexbuilder"))).requiresConfigurationFragments(AndroidConfiguration.class);
  for (  String attr : TRANSITIVE_ATTRIBUTES) {
    result.attributeAspect(attr,DexArchiveAspect.class);
  }
  return result.build();
}
