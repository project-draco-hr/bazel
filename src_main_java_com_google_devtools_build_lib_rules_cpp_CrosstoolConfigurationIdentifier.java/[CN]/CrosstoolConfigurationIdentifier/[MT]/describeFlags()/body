{
  StringBuilder message=new StringBuilder();
  if (getCpu() != null) {
    message.append(" --cpu='").append(getCpu()).append("'");
  }
  if (getCompiler() != null) {
    message.append(" --compiler='").append(getCompiler()).append("'");
  }
  if (getLibc() != null) {
    message.append(" --glibc='").append(getLibc()).append("'");
  }
  return message.toString();
}
