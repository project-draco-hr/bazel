{
  ImmutableList.Builder<QueryFunction> functions=ImmutableList.builder();
  for (  BlazeModule module : runtime.getBlazeModules()) {
    functions.addAll(module.getQueryFunctions());
  }
  return AbstractBlazeQueryEnvironment.newQueryEnvironment(runtime.getPackageManager().newTransitiveLoader(),runtime.getPackageManager(),runtime.getTargetPatternEvaluator(),keepGoing,orderedResults,loadingPhaseThreads,runtime.getReporter(),settings,functions.build());
}
