{
  if (paramFileInfo == null) {
    return null;
  }
  if (!paramFileInfo.always() && getParamFileSize(executableArgs,arguments,commandLine) < configuration.getMinParamFileSize()) {
    return null;
  }
  PathFragment paramFilePath=ParameterFile.derivePath(Iterables.getFirst(outputs,null).getRootRelativePath());
  return analysisEnvironment.getDerivedArtifact(paramFilePath,configuration.getBinDirectory());
}
