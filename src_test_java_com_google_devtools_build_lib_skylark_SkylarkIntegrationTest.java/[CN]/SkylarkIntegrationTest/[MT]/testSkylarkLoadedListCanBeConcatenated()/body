{
  scratch.file("test/skylark/extension.bzl","l1 = ('a',)","l2 = ['a']");
  scratch.file("test/skylark/BUILD","load('/test/skylark/extension', 'l1')","","m = l1 + ('b',) # tuple","genrule(name = ''.join(m), cmd = 'echo hi >@', outs = ['a.txt'])");
  getConfiguredTarget("//test/skylark:ab");
  scratch.file("test/skylark2/BUILD","load('/test/skylark/extension', 'l2')","","m = l2 + ['b']","genrule(name = ''.join(m), cmd = 'echo hi >@', outs = ['b.txt'])");
  getConfiguredTarget("//test/skylark2:ab");
}
