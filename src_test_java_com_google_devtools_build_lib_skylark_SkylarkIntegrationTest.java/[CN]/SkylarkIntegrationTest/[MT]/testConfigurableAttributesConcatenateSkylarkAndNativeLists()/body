{
  writeConfigFile();
  scratch.file("test/skylark/extension.bzl","def return_select():","    return select({'//conditions:a': ['foo.in']}) + ['skylark.list']");
  scratch.file("test/skylark/BUILD","load('/test/skylark/extension', 'return_select')","genrule(","    name = 'gen',","    srcs = return_select() + ['native.list'],","    outs = ['gen.out'],","    cmd = 'echo hi > $@')");
  useConfiguration("--test_arg=a");
  getConfiguredTarget("//test/skylark:gen");
}
