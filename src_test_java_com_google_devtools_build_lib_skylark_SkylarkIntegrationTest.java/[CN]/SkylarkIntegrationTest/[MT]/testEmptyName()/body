{
  scratch.file("test/skylark/extension.bzl","def custom_rule_impl(ctx):","  return None","","custom_rule = rule(implementation = custom_rule_impl,","  attrs = {'': attr.label()})");
  scratch.file("test/skylark/BUILD","load('/test/skylark/extension', 'custom_rule')","","custom_rule(name = 'cr', attr1 = '//test/skylark:file')");
  reporter.removeHandler(failFastHandler);
  try {
    getTarget("//test/skylark:cr");
    fail();
  }
 catch (  Exception e) {
    assertContainsEvent("Attribute name cannot be empty");
  }
