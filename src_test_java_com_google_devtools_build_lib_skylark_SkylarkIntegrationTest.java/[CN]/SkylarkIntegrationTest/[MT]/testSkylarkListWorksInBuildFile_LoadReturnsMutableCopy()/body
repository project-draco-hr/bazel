{
  scratch.file("test/skylark/extension.bzl","l = ['a']");
  scratch.file("test/skylark/BUILD","load('/test/skylark/extension', 'l')","","l.append('b')","genrule(name = ''.join(l), cmd = 'echo hi >@', outs = ['a.txt'])");
  getConfiguredTarget("//test/skylark:ab");
  scratch.file("test/skylark2/BUILD","load('/test/skylark/extension', 'l')","","l.append('c')","genrule(name = ''.join(l), cmd = 'echo hi >@', outs = ['a.txt'])");
  getConfiguredTarget("//test/skylark2:ac");
}
