{
  scratch.file("test/skylark/extension.bzl","l1 = ['a']");
  scratch.file("test/skylark/BUILD","load('/test/skylark/extension', 'l1')","","l2 = l1 + ['b']","genrule(name = l2[0], cmd = 'echo hi >@', outs = ['a.txt'])","genrule(name = l2[1], cmd = 'echo hi >@', outs = ['b.txt'])");
  getConfiguredTarget("//test/skylark:a");
  getConfiguredTarget("//test/skylark:b");
}
