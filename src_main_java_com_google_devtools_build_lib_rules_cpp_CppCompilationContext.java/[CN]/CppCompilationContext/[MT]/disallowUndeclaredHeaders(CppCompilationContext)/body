{
  return new CppCompilationContext(context.commandLineContext,context.compilationPrerequisites,NestedSetBuilder.<PathFragment>emptySet(Order.STABLE_ORDER),NestedSetBuilder.<PathFragment>emptySet(Order.STABLE_ORDER),context.declaredIncludeSrcs,context.pregreppedHdrs,context.moduleInfo,context.picModuleInfo,context.transitiveModuleMaps,context.directModuleMaps,context.cppModuleMap,context.provideTransitiveModuleMaps,context.useHeaderModules);
