{
  if (pushedToken != null) {
    token=pushedToken;
    pushedToken=null;
  }
 else {
    if (token == null || token.kind != TokenKind.EOF) {
      token=tokens.next();
      while (token.kind == TokenKind.COMMENT) {
        makeComment(token);
        token=tokens.next();
      }
    }
  }
  if (DEBUGGING) {
    System.err.print(token);
  }
}
