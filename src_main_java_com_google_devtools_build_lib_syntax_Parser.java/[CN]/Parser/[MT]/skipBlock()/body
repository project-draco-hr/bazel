{
  int start=token.left;
  Token blockToken=token;
  syncTo(EnumSet.of(TokenKind.COLON,TokenKind.EOF));
  if (parsingMode != PYTHON) {
    reportError(lexer.createLocation(start,token.right),"syntax error at '" + blockToken + "': This is not supported in BUILD files. "+ "Move the block to a .bzl file and load it");
  }
  expect(TokenKind.COLON);
  skipSuite();
}
