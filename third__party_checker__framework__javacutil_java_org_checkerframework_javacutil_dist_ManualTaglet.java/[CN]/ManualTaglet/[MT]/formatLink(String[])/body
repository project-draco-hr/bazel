{
  String anchor, text;
  if (parts.length < 2) {
    anchor="";
    text="Checker Framework";
  }
 else {
    anchor=parts[0];
    text=parts[1];
  }
  return String.format("<A HREF=\"http://types.cs.washington.edu/checker-framework/current/checker-framework-manual.html%s\">%s</A>",anchor,text);
}
