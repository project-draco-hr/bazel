{
  JavaCompilationArgs args=JavaCompilationArgs.builder().addSourcesTransitiveCompilationArgs(deps,true,isNeverLink ? ClasspathType.COMPILE_ONLY : ClasspathType.BOTH).build();
  NestedSet<Artifact> directJars=isStrict() ? getNonRecursiveCompileTimeJarsFromProvider(deps,isNeverLink) : null;
  addArgsAndJarsToAttributes(args,directJars);
}
