{
  watcher.stopPumping();
  try {
    watcher.join();
  }
 catch (  InterruptedException e) {
    Thread.currentThread().interrupt();
  }
  if (!headerFilter.foundHeader()) {
    InputStream input=testLogPath.getInputStream();
    try {
      ByteStreams.copy(input,outErr.getOutputStream());
    }
  finally {
      input.close();
    }
  }
}
