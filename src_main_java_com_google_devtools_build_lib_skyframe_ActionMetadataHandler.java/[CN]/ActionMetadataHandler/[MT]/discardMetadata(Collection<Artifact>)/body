{
  Preconditions.checkState(injectedArtifacts.isEmpty(),"Artifacts cannot be injected before action execution: %s",injectedArtifacts);
  metadataDiscarded=true;
  outputArtifactData.keySet().removeAll(artifactList);
  additionalOutputData.keySet().removeAll(artifactList);
  missingArtifacts.keySet().removeAll(artifactList);
  if (!outputArtifactData.isEmpty()) {
    LoggingUtil.logToRemote(Level.SEVERE,"Not all outputs cleared: " + outputArtifactData,new IllegalStateException());
  }
}
