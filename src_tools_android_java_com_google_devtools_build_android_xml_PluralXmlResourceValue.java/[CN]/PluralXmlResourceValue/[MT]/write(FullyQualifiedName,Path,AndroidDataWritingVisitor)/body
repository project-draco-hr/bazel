{
  ValuesResourceDefinition definition=mergedDataWriter.define(key).derivedFrom(source).startTag(PLURALS).named(key).closeTag();
  for (  Entry<String,String> plural : values.entrySet()) {
    definition=definition.startItemTag().attribute("quantity").setTo(plural.getKey()).closeTag().addCharactersOf(plural.getValue()).endTag();
  }
  definition.endTag().save();
}
