{
  if (value.equals("yes")) {
    return TriState.YES;
  }
 else   if (value.equals("no")) {
    return TriState.NO;
  }
 else   if (value.equals("auto")) {
    return TriState.AUTO;
  }
 else {
    throw new PackageDeserializationException(String.format("Invalid tristate value: '%s'",value));
  }
}
