{
  Location location=null;
  if (event.hasLocation()) {
    location=context.deserializeLocation(event.getLocation());
  }
  String message=event.getMessage();
switch (event.getKind()) {
case ERROR:
    eventHandler.handle(Event.error(location,message));
  break;
case WARNING:
eventHandler.handle(Event.warn(location,message));
break;
case INFO:
eventHandler.handle(Event.info(location,message));
break;
case PROGRESS:
eventHandler.handle(Event.progress(location,message));
break;
default :
break;
}
}
