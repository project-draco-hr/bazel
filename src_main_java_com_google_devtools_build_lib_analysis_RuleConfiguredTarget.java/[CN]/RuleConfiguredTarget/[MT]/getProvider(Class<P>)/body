{
  AnalysisUtils.checkProvider(providerClass);
  Object provider=providers.get(providerClass);
  if (provider == null) {
    for (    Aspect aspect : aspects) {
      provider=aspect.getProviders().get(providerClass);
      if (provider != null) {
        break;
      }
    }
  }
  return providerClass.cast(provider);
}
