{
  if (checkArgs(args)) {
    try {
      BuildEncyclopediaProcessor processor=new BuildEncyclopediaProcessor(createRuleClassProvider());
      processor.generateDocumentation(args[0].split(","),getArgsOrNull(args,1),getArgsOrNull(args,2));
    }
 catch (    BuildEncyclopediaDocException e) {
      fail(e,false);
    }
catch (    Throwable e) {
      fail(e,true);
    }
  }
}
