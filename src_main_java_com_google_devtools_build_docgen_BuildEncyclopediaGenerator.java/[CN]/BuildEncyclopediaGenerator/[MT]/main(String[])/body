{
  if (checkArgs(args)) {
    try {
      BuildEncyclopediaProcessor processor=new BuildEncyclopediaProcessor(createRuleClassProvider(args[1]));
      processor.generateDocumentation(args[0].split(","),getArgsOrNull(args,2),getArgsOrNull(args,3));
    }
 catch (    BuildEncyclopediaDocException e) {
      fail(e,false);
    }
catch (    Throwable e) {
      fail(e,true);
    }
  }
}
