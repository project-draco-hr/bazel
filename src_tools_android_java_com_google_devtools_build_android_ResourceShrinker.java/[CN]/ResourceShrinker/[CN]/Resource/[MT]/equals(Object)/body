{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  Resource resource=(Resource)o;
  if (name != null ? !name.equals(resource.name) : resource.name != null) {
    return false;
  }
  if (type != resource.type) {
    return false;
  }
  return true;
}
