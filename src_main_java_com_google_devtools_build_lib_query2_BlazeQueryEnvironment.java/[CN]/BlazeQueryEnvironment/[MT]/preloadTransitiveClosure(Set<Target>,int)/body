{
  if (maxDepth >= MAX_DEPTH_FULL_SCAN_LIMIT && transitivePackageLoader != null) {
    try {
      transitivePackageLoader.sync(eventHandler,targets,ImmutableSet.<Label>of(),keepGoing,loadingPhaseThreads,-1);
    }
 catch (    InterruptedException e) {
      throw new QueryException("interrupted");
    }
  }
}
