{
  toolchain.setToolchainIdentifier(toolchain.getToolchainIdentifier() + "-" + name).setSupportsEmbeddedRuntimes(true).setDynamicRuntimesFilegroup(createRuntimeLibrariesFilegroup(name,gccVersion,toolchain.getTargetCpu(),armThumb,RuntimeType.DYNAMIC)).setStaticRuntimesFilegroup(createRuntimeLibrariesFilegroup(name,gccVersion,toolchain.getTargetCpu(),armThumb,RuntimeType.STATIC));
}
