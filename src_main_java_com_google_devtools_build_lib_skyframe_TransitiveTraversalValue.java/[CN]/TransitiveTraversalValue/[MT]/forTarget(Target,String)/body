{
  if (target instanceof Rule) {
    Rule rule=(Rule)target;
    return new TransitiveTraversalValue(rule.getRuleClassObject().canHaveAnyProvider(),toStringSet(rule.getRuleClassObject().getAdvertisedProviders()),firstErrorMessage);
  }
  return new TransitiveTraversalValue(false,ImmutableList.<String>of(),firstErrorMessage);
}
