{
  Preconditions.checkState(inputTreeArtifact.isTreeArtifact() && outputTreeArtifact.isTreeArtifact(),"Either %s or %s is not a TreeArtifact",inputTreeArtifact,outputTreeArtifact);
  this.inputTreeArtifact=inputTreeArtifact;
  this.outputTreeArtifact=outputTreeArtifact;
  this.spawnActionBuilder=new SpawnAction.Builder();
}
