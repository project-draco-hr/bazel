{
  indent(ascii,level);
switch (type) {
case INTEGER:
{
      ascii.append("<*I");
      ascii.append(toString());
      ascii.append(">");
      break;
    }
case REAL:
{
    ascii.append("<*R");
    ascii.append(toString());
    ascii.append(">");
    break;
  }
case BOOLEAN:
{
  if (boolValue) {
    ascii.append("<*BY>");
  }
 else {
    ascii.append("<*BN>");
  }
}
}
}
