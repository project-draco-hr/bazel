{
  CToolchain.Builder toolchain=createBaseX86Toolchain().setToolchainIdentifier("x86-" + gccVersion).setTargetCpu("x86").setCompiler("gcc-" + gccVersion).addAllToolPath(ndkPaths.createToolpaths("x86-" + gccVersion,"i686-linux-android",excludedTools)).addAllCxxBuiltinIncludeDirectory(ndkPaths.createToolchainIncludePaths("x86-" + gccVersion,"i686-linux-android",gccVersion)).setBuiltinSysroot(ndkPaths.createBuiltinSysroot("x86")).addCompilerFlag(stackProtrectorFlag);
  stlImpl.addStlImpl(toolchain,gccVersion);
  return toolchain;
}
