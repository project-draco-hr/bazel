{
  Executor executor=actionExecutionContext.getExecutor();
  Collection<Artifact> returnValue=null;
  try {
    returnValue=executor.getContext(CppCompileActionContext.class).findAdditionalInputs(this,actionExecutionContext);
  }
 catch (  ExecException e) {
    throw e.toActionExecutionException("Include scanning of rule '" + getOwner().getLabel() + "'",executor.getVerboseFailures(),this);
  }
  this.additionalInputs=returnValue == null ? ImmutableList.<Artifact>of() : returnValue;
  return returnValue;
}
