{
  RuleClass parentRuleClass=createParentRuleClass();
  RuleClass childRuleClass=createChildRuleClass(parentRuleClass);
  assertThat(parentRuleClass.getRequiredConfigurationFragments()).containsExactly(DummyFragment.class);
  assertThat(childRuleClass.getRequiredConfigurationFragments()).containsExactly(DummyFragment.class);
}
