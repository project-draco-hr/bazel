{
  try {
    Files.createDirectories(rOutput.getParent());
    final Path source=generatedSourceRoot.resolve("R.txt");
    if (Files.exists(source)) {
      Files.copy(source,rOutput);
    }
 else {
      Files.createFile(rOutput);
    }
  }
 catch (  IOException e) {
    Throwables.propagate(e);
  }
}
