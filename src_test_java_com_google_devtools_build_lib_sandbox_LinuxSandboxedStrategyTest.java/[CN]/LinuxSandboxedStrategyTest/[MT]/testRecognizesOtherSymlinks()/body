{
  ImmutableList<String> inputFile=ImmutableList.of("a/a/x.txt","a/a/y.txt");
  Map<String,String> testFiles=ImmutableMap.of("a/a/x.txt","../b/x.txt","a/a/y.txt","","a/b/x.txt","");
  Map<String,String> assertMounts=ImmutableMap.of("a/a/x.txt","a/b/x.txt","a/a/y.txt","a/a/y.txt");
  assertThat(userFriendlyMounts(testFiles,inputFile)).isEqualTo(userFriendlyAsserts(assertMounts));
}
