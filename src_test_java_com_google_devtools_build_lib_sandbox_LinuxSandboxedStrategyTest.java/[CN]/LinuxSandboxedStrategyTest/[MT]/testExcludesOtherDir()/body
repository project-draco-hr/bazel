{
  ImmutableList<String> inputFile=ImmutableList.of("a/x.txt","a/y.txt");
  Map<String,String> testFiles=ImmutableMap.of("a/x.txt","","a/y.txt","","a/b/","");
  List<String> assertMounts=ImmutableList.of("a/x.txt","a/y.txt");
  assertThat(userFriendlyMounts(testFiles,inputFile)).isEqualTo(userFriendlyAsserts(assertMounts));
}
