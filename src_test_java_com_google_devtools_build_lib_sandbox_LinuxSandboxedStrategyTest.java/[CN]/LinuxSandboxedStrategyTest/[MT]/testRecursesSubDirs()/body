{
  ImmutableList<String> inputFile=ImmutableList.of("a/b");
  Map<String,String> testFiles=ImmutableMap.of("a/b/x.txt","","a/b/y.txt","z.txt","a/b/z.txt","");
  List<String> assertMounts=ImmutableList.of("a/b/x.txt","a/b/y.txt","a/b/z.txt");
  assertThat(userFriendlyMounts(testFiles,inputFile)).isEqualTo(userFriendlyAsserts(assertMounts));
}
