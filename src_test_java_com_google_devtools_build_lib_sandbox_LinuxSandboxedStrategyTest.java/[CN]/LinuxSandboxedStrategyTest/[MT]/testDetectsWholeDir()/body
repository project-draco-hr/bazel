{
  ImmutableList<String> inputFile=ImmutableList.of("a/x.txt","a/z.txt");
  Map<String,String> testFiles=ImmutableMap.of("a/x.txt","","a/z.txt","");
  List<String> assertMounts=ImmutableList.of("a");
  assertThat(userFriendlyMounts(testFiles,inputFile)).isEqualTo(userFriendlyAsserts(assertMounts));
}
