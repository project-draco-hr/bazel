{
  Path targetDir=workspaceDir.getRelative("runfiles");
  targetDir.createDirectory();
  Path testFile=workspaceDir.getRelative("testfile");
  FileSystemUtils.createEmptyFile(testFile);
  Path manifestFile=workspaceDir.getRelative("MANIFEST");
  FileSystemUtils.writeContent(manifestFile,Charset.defaultCharset(),String.format("x/testfile %s\nx/emptyfile \n",testFile.getPathString()));
  Map mounts=LinuxSandboxedStrategy.parseManifestFile(targetDir,manifestFile.getPathFile());
  assertThat(userFriendlyMap(mounts)).isEqualTo(userFriendlyMap(ImmutableMap.of(fileSystem.getPath("/runfiles/x/testfile"),testFile,fileSystem.getPath("/runfiles/x/emptyfile"),fileSystem.getPath("/dev/null"))));
}
