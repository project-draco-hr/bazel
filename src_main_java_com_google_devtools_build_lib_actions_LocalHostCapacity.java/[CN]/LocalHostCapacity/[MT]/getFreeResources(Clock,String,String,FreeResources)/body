{
  if (isDisabled) {
    return null;
  }
  try {
    String statContent=readContent(statFile);
    return new FreeResources(localClock,new ProcMeminfoParser(meminfoFile),statContent,prevStats);
  }
 catch (  IOException|IllegalArgumentException e) {
    disableProcFsUse(e);
    return null;
  }
}
