{
  AspectDefinition withAspects=new AspectDefinition.Builder("attribute_aspect").attributeAspect("srcs",TestAspectFactory.class).attributeAspect("deps",new NativeAspectClass<TestAspectFactory>(TestAspectFactory.class)).build();
  assertThat(withAspects.getAttributeAspects()).containsEntry("srcs",new NativeAspectClass<TestAspectFactory>(TestAspectFactory.class));
  assertThat(withAspects.getAttributeAspects()).containsEntry("deps",new NativeAspectClass<TestAspectFactory>(TestAspectFactory.class));
}
