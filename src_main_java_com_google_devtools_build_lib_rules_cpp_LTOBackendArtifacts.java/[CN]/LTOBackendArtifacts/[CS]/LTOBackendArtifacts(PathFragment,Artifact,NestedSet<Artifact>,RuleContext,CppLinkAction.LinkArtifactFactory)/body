{
  this.bitcodeFile=bitcodeFile;
  PathFragment obj=ltoOutputRootPrefix.getRelative(bitcodeFile.getRootRelativePath());
  objectFile=linkArtifactFactory.create(ruleContext,obj);
  imports=linkArtifactFactory.create(ruleContext,FileSystemUtils.replaceExtension(obj,".imports"));
  index=linkArtifactFactory.create(ruleContext,FileSystemUtils.replaceExtension(obj,".thinlto.index"));
  beCommandline=linkArtifactFactory.create(ruleContext,FileSystemUtils.replaceExtension(obj,".thinlto_commandline.txt"));
  bitcodeFiles=allBitCodeFiles;
}
