{
  if (SHARED_LIBRARY_FILETYPES.matches(artifact.getFilename())) {
    return isCppRuntime ? SolibSymlinkAction.getCppRuntimeSymlink(ruleContext,artifact,solibDirOverride,configuration).getArtifact() : SolibSymlinkAction.getDynamicLibrarySymlink(ruleContext,artifact,false,true,configuration).getArtifact();
  }
 else {
    return artifact;
  }
}
