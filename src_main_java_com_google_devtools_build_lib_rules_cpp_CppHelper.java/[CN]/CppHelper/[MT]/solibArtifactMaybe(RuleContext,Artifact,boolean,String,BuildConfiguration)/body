{
  if (SHARED_LIBRARY_FILETYPES.matches(artifact.getFilename())) {
    return isCppRuntime ? SolibSymlinkAction.getCppRuntimeSymlink(ruleContext,artifact,solibDirOverride,configuration) : SolibSymlinkAction.getDynamicLibrarySymlink(ruleContext,artifact,false,true,configuration);
  }
 else {
    return artifact;
  }
}
