{
  return builder.requiresConfigurationFragments(JavaConfiguration.class).add(attr("source_version",STRING).mandatory()).add(attr("target_version",STRING).mandatory()).add(attr("encoding",STRING).mandatory()).add(attr("xlint",STRING_LIST).value(ImmutableList.<String>of())).add(attr("misc",STRING_LIST).value(ImmutableList.<String>of())).add(attr("jvm_opts",STRING_LIST).value(ImmutableList.<String>of("-client"))).add(attr("header_compiler",LABEL).cfg(HOST).allowedFileTypes(FileTypeSet.ANY_FILE).exec()).build();
}
