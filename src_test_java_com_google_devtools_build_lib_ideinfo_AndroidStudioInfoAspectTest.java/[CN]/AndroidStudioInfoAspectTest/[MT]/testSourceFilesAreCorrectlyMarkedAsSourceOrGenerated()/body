{
  if (!isNativeTest()) {
    return;
  }
  scratch.file("com/google/example/BUILD","genrule(","   name = 'gen',","   outs = ['gen.java'],","   cmd = '',",")","java_library(","    name = 'lib',","    srcs = ['Test.java', ':gen'],",")");
  Map<String,RuleIdeInfo> ruleIdeInfos=buildRuleIdeInfo("//com/google/example:lib");
  RuleIdeInfo ruleIdeInfo=getRuleInfoAndVerifyLabel("//com/google/example:lib",ruleIdeInfos);
  assertThat(ruleIdeInfo.getJavaRuleIdeInfo().getSourcesList()).containsExactly(ArtifactLocation.newBuilder().setRootPath(targetConfig.getGenfilesDirectory().getPath().getPathString()).setRelativePath("com/google/example/gen.java").setIsSource(false).build(),ArtifactLocation.newBuilder().setRootPath(directories.getWorkspace().getPathString()).setRelativePath("com/google/example/Test.java").setIsSource(true).build());
}
