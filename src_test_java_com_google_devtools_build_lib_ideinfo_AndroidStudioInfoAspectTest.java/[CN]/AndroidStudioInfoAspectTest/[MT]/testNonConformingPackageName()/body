{
  if (!isNativeTest()) {
    return;
  }
  scratch.file("bad/package/google/example/BUILD","android_library(","  name = 'test',","  srcs = ['Test.java'],",")");
  Map<String,RuleIdeInfo> ruleIdeInfos=buildRuleIdeInfo("//bad/package/google/example:test");
  RuleIdeInfo ruleInfo=getRuleInfoAndVerifyLabel("//bad/package/google/example:test",ruleIdeInfos);
  assertThat(ruleInfo.getAndroidRuleIdeInfo().getJavaPackage()).isEqualTo("bad.package.google.example");
}
