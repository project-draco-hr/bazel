{
  scratch.file("bad/package/google/example/BUILD","android_library(","  name = 'test',","  srcs = ['Test.java'],",")");
  String target="//bad/package/google/example:test";
  Map<String,RuleIdeInfo> ruleIdeInfos=buildRuleIdeInfo(target);
  RuleIdeInfo ruleInfo=getRuleInfoAndVerifyLabel(target,ruleIdeInfos);
  assertThat(ruleInfo.getAndroidRuleIdeInfo().getJavaPackage()).isEqualTo("bad.package.google.example");
}
