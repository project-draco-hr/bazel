{
  scratch.file("java/com/google/example/BUILD","java_library(","   name = 'foobar',","   srcs = ['FooBar.java'],",")","java_test(","   name = 'FooBarTest',","   srcs = ['FooBarTest.java'],","   deps = [':foobar'],",")");
  String target="//java/com/google/example:FooBarTest";
  Map<String,RuleIdeInfo> ruleIdeInfos=buildRuleIdeInfo(target);
  RuleIdeInfo testInfo=getRuleInfoAndVerifyLabel(target,ruleIdeInfos);
  assertThat(testInfo.getKind()).isEqualTo(Kind.JAVA_TEST);
  assertThat(relativePathsForSourcesOf(testInfo)).containsExactly("java/com/google/example/FooBarTest.java");
  assertThat(testInfo.getDependenciesList()).containsExactly("//java/com/google/example:foobar");
  assertThat(transform(testInfo.getJavaRuleIdeInfo().getJarsList(),LIBRARY_ARTIFACT_TO_STRING)).containsExactly("<jar:java/com/google/example/FooBarTest.jar>" + "<source:java/com/google/example/FooBarTest-src.jar>");
}
