{
  Set<AspectWithParameters> aspectCandidates=new LinkedHashSet<>();
  aspectCandidates.addAll(attribute.getAspectsWithParameters(originalRule));
  if (aspectWithParameters != null) {
    for (    AspectClass aspect : aspectWithParameters.getDefinition().getAttributeAspects().get(attribute.getName())) {
      aspectCandidates.add(new AspectWithParameters(aspect,aspectWithParameters.getParameters()));
    }
  }
  return aspectCandidates;
}
