{
  this.ruleContext=ruleContext;
  this.attributes=new Attributes(ruleContext,buildConfiguration);
  this.targetType=targetType;
  if (targetType != TargetType.PROTO_TARGET) {
    this.intermediateArtifacts=new IntermediateArtifacts(ruleContext,"_protos","protos",ruleContext.getConfiguration());
  }
 else {
    this.intermediateArtifacts=ObjcRuleClasses.intermediateArtifacts(ruleContext);
  }
}
