{
  return new RuleConfiguredTargetBuilder(ruleContext).add(RunfilesProvider.class,RunfilesProvider.EMPTY).add(OptionsProvider.class,new OptionsProvider.Builder().addCopts(ruleContext.getTokenizedStringListAttr("copts")).addInfoplists(ruleContext.getPrerequisiteArtifacts("infoplists",Mode.TARGET).list()).build()).build();
}
