{
  Preconditions.checkNotNull(target,String.format("'%s' attribute '%s'",from == null ? "(null)" : from.getLabel().toString(),attribute == null ? "(null)" : attribute.getName()));
  if (depth > maxDepth) {
    return;
  }
  if (from != null) {
    observeEdge(from,attribute,target);
    visitAspectsIfRequired(from,attribute,target,depth,count);
  }
  visitedMap.put(target.getPackage(),target);
  visitTargetNode(target,depth,count);
}
