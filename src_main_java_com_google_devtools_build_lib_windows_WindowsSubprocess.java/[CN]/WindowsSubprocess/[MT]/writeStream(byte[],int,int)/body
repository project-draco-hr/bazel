{
  checkLiveness();
  int remaining=len;
  int currentOffset=off;
  while (remaining != 0) {
    int written=WindowsProcesses.nativeWriteStdin(nativeProcess,b,currentOffset,remaining);
    if (written == -1) {
      throw new IOException(WindowsProcesses.nativeGetLastError(nativeProcess));
    }
    remaining-=written;
    currentOffset+=written;
  }
}
