{
  return builder.add(attr(":host_jdk",LABEL).cfg(HOST).value(JavaSemantics.HOST_JDK)).add(attr("jars",LABEL_LIST).mandatory().nonEmpty().allowedFileTypes(JavaSemantics.JAR)).add(attr("srcjar",LABEL).allowedFileTypes(JavaSemantics.SOURCE_JAR,JavaSemantics.JAR).direct_compile_time_input()).removeAttribute("deps").add(attr("neverlink",BOOLEAN).value(false)).add(attr("constraints",STRING_LIST).orderIndependent().nonconfigurable("used in Attribute.validityPredicate implementations (loading time)")).build();
}
