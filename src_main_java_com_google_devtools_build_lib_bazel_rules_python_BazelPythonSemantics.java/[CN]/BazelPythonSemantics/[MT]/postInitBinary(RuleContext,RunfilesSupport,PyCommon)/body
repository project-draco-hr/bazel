{
  if (ruleContext.getConfiguration().buildPythonZip()) {
    FilesToRunProvider zipper=ruleContext.getExecutablePrerequisite("$zipper",Mode.HOST);
    if (!ruleContext.hasErrors()) {
      createPythonZipAction(ruleContext,common.getExecutable(),common.getPythonZipArtifact(),common.determineMainExecutableSource(false),zipper,runfilesSupport);
    }
  }
}
