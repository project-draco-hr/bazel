{
  if ((tree.mods.flags & Flags.FINAL) != Flags.FINAL) {
    return false;
  }
  if (!constantType(tree.getType())) {
    return false;
  }
  if (tree.getInitializer() != null) {
    Boolean result=tree.getInitializer().accept(CONSTANT_VISITOR,null);
    if (result == null || !result) {
      return false;
    }
  }
  return true;
}
