{
  ImmutableMap.Builder<String,String> builder=ImmutableMap.builder();
  if (platform == Platform.IOS_DEVICE || platform == Platform.IOS_SIMULATOR) {
    builder.put(AppleConfiguration.APPLE_SDK_VERSION_ENV_NAME,getIosSdkVersion().toString()).put(AppleConfiguration.APPLE_SDK_PLATFORM_ENV_NAME,platform.getNameInPlist());
  }
  return builder.build();
}
