{
  xEmptyDirectory.setWritable(false);
  Path xChildOfReadonlyDir=xEmptyDirectory.getChild("x");
  if (supportsSymlinks) {
    try {
      xChildOfReadonlyDir.createSymbolicLink(xNothing);
      fail();
    }
 catch (    IOException e) {
      assertEquals(xChildOfReadonlyDir + " (Permission denied)",e.getMessage());
    }
  }
}
