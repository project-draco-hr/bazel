{
  path.setExecutable(true);
  assertTrue(path.isExecutable());
  assertTrue(link.isExecutable());
  path.setExecutable(false);
  assertFalse(path.isExecutable());
  assertFalse(link.isExecutable());
  path.setExecutable(true);
  assertTrue(path.isExecutable());
  assertTrue(link.isExecutable());
}
