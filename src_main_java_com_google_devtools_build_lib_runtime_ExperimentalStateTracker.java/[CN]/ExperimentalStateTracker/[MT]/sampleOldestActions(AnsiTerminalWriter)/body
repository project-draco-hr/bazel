{
  int count=0;
  long nanoTime=clock.nanoTime();
  for (  String action : runningActions) {
    count++;
    terminalWriter.newline().append("    " + describeAction(action,nanoTime));
    if (count >= SAMPLE_SIZE) {
      break;
    }
  }
  if (count < runningActions.size()) {
    terminalWriter.append(" ...");
  }
}
