{
  final String prefix="; last test: ";
  if (!shortVersion && mostRecentTest != null) {
    if (terminalWriter != null) {
      terminalWriter.normal().append(prefix + shortenedLabelString(mostRecentTest.getTarget().getLabel(),width - prefix.length()));
    }
    return true;
  }
 else {
    return false;
  }
}
