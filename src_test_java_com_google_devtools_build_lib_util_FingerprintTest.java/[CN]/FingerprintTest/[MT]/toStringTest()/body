{
  Fingerprint f1=new Fingerprint();
  f1.addString("Hello ");
  f1.addString("World!");
  String fp=f1.hexDigestAndReset();
  Fingerprint f2=new Fingerprint();
  f2.addString("Hello ");
  assertThat(fp).isNotEqualTo(f2.toString());
  f2.addString("World!");
  assertThat(fp).isEqualTo(f2.hexDigestAndReset());
}
