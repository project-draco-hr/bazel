{
  new EnvironmentGroupMaker("foo_env").setEnvironments("a","b","c").setFulfills("a","b").setFulfills("b","c").setDefaults("a").make();
  scratchFile("hello/BUILD",getDependencyRule(),getDependingRule(constrainedTo("//foo_env:c")));
  assertNotNull(getConfiguredTarget("//hello:main"));
  assertNoEvents();
}
