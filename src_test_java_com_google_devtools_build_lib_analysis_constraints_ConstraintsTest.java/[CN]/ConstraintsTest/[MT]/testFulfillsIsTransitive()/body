{
  new EnvironmentGroupMaker("foo_env").setEnvironments("a","b","c").setFulfills("a","b").setFulfills("b","c").setDefaults().make();
  scratch.file("hello/BUILD",getDependencyRule(constrainedTo("//foo_env:a")),getDependingRule(constrainedTo("//foo_env:c")));
  assertNotNull(getConfiguredTarget("//hello:main"));
  assertNoEvents();
}
