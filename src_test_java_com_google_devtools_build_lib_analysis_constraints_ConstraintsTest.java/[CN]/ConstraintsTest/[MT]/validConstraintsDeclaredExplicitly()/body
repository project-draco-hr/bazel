{
  new EnvironmentGroupMaker("buildenv/foo").setEnvironments("a","b").setDefaults("a").make();
  scratch.file("hello/BUILD",getDependencyRule(constrainedTo("//buildenv/foo:a","//buildenv/foo:b")),getDependingRule(constrainedTo("//buildenv/foo:b")));
  assertNotNull(getConfiguredTarget("//hello:main"));
  assertNoEvents();
}
