{
  new EnvironmentGroupMaker("foo_env").setEnvironments("a","b").setDefaults("a").make();
  scratchFile("hello/BUILD",getDependencyRule(constrainedTo("//foo_env:a","//foo_env:b")),getDependingRule(constrainedTo("//foo_env:b")));
  assertNotNull(getConfiguredTarget("//hello:main"));
  assertNoEvents();
}
