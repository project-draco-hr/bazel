{
  new EnvironmentGroupMaker("foo_env").setEnvironments("a","b").setDefaults("a").make();
  scratchFile("hello/BUILD","package(default_compatible_with = ['//foo_env:b'])",getDependencyRule(),getDependingRule(compatibleWith("//foo_env:b")));
  assertNotNull(getConfiguredTarget("//hello:main"));
  assertNoEvents();
}
