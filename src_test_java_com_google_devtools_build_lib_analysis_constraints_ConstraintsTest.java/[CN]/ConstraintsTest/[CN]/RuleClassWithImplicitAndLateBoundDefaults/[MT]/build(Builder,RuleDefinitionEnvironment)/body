{
  return builder.setUndocumented().add(Attribute.attr("$implicit",Type.LABEL).value(Label.parseAbsoluteUnchecked("//helpers:implicit"))).add(Attribute.attr(":latebound",Type.LABEL).value(new Attribute.LateBoundLabel<BuildConfiguration>(){
    @Override public Label getDefault(    Rule rule,    BuildConfiguration configuration){
      return Label.parseAbsoluteUnchecked("//helpers:latebound");
    }
  }
)).build();
}
