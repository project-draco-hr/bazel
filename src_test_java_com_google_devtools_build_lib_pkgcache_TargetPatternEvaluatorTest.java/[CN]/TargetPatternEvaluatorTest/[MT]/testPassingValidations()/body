{
  expectValidationPass("foo:bar");
  expectValidationPass("foo:all");
  expectValidationPass("foo/...:all");
  expectValidationPass("foo:*");
  expectValidationPass("//foo");
  expectValidationPass("foo");
  expectValidationPass("foo/bar");
  expectValidationPass("//foo:bar");
  expectValidationPass("//foo:all");
  expectValidationPass("//foo/all");
  expectValidationPass("java/com/google/foo/Bar.java");
  expectValidationPass("//foo/...:all");
}
