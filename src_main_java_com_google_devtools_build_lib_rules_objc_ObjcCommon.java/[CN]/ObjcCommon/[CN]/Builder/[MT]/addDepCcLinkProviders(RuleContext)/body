{
  for (  TransitiveInfoCollection dep : ruleContext.getPrerequisites("deps",Mode.TARGET)) {
    if (dep.getProvider(CppRunfilesProvider.class) != null) {
      CcLinkParamsProvider ccLinkParamsProvider=dep.getProvider(CcLinkParamsProvider.class);
      this.depCcLinkProviders=Iterables.concat(this.depCcLinkProviders,ImmutableList.<CcLinkParamsProvider>of(ccLinkParamsProvider));
    }
  }
  return this;
}
