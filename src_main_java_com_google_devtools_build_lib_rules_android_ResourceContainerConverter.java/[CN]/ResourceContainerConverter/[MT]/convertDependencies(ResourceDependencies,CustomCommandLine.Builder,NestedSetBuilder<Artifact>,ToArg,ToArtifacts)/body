{
  if (dependencies != null) {
    if (!dependencies.getTransitiveResources().isEmpty()) {
      cmdBuilder.addJoinStrings("--data",",",Iterables.unmodifiableIterable(Iterables.transform(dependencies.getTransitiveResources(),toArg)));
    }
    if (!dependencies.getDirectResources().isEmpty()) {
      cmdBuilder.addJoinStrings("--directData",",",Iterables.unmodifiableIterable(Iterables.transform(dependencies.getDirectResources(),toArg)));
    }
    inputs.addTransitive(NestedSetBuilder.wrap(Order.NAIVE_LINK_ORDER,FluentIterable.from(dependencies.getResources()).transformAndConcat(toArtifacts)));
  }
}
