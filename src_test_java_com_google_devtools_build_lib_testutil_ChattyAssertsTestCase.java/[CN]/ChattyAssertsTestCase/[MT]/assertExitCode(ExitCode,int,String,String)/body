{
  int expectedExitCodeValue=expectedExitCode.getNumericExitCode();
  if (exitCode != expectedExitCodeValue) {
    fail(String.format("expected exit code '%s' <%d> but exit code was <%d> and stdout was <%s> " + "and stderr was <%s>",expectedExitCode.name(),expectedExitCodeValue,exitCode,stdout,stderr));
  }
}
