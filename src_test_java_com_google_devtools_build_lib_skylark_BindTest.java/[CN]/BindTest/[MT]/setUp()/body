  super.setUp();
  SkylarkUtil.setup(scratch);
  scratch.file("test/BUILD","load('/rules/java_rules_skylark', 'java_library')","java_library(name = 'giraffe',","    srcs = ['Giraffe.java'],",")","java_library(name = 'safari',","    srcs = ['Safari.java'],","    deps = ['//external:long-horse'],",")");
