{
  invalidatePackages();
  ConfiguredTarget giraffeTarget=getConfiguredTarget("//test:giraffe");
  Artifact giraffeArtifact=ActionsTestUtil.getFirstArtifactEndingWith(getFilesToBuild(giraffeTarget),"giraffe.jar");
  ConfiguredTarget safariTarget=getConfiguredTarget("//test:safari");
  Action safariAction=getGeneratingAction(ActionsTestUtil.getFirstArtifactEndingWith(getFilesToBuild(safariTarget),"safari.jar"));
  assertThat(safariAction.getInputs()).contains(giraffeArtifact);
}
