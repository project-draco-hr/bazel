{
  return new VariablesExtension(){
    @Override public void addVariables(    Builder builder){
      if (ruleContext.getPrerequisiteArtifact("pch",Mode.TARGET) != null) {
        builder.addVariable(PCH_FILE_VARIABLE_NAME,ruleContext.getPrerequisiteArtifact("pch",Mode.TARGET).getExecPathString());
      }
    }
  }
;
}
