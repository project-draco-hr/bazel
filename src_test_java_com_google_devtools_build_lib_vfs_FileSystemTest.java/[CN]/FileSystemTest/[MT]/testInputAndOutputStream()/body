{
  OutputStream outStream=xFile.getOutputStream();
  for (int i=33; i < 126; i++) {
    outStream.write(i);
  }
  outStream.close();
  InputStream inStream=xFile.getInputStream();
  for (int i=33; i < 126; i++) {
    int readValue=inStream.read();
    assertEquals(i,readValue);
  }
  inStream.close();
}
