{
  this.execRoot=execRoot;
  this.sandboxPath=sandboxPath;
  this.sandboxExecRoot=sandboxExecRoot;
  this.argumentsFilePath=sandboxPath.getParentDirectory().getRelative(sandboxPath.getBaseName() + ".params");
