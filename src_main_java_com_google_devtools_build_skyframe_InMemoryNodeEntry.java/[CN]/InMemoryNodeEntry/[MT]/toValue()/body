{
  if (isDone()) {
    return getErrorInfo() == null ? getValue() : null;
  }
 else   if (isChanged() || isDirty()) {
    return (getDirtyBuildingState().getLastBuildValue() == null) ? null : ValueWithMetadata.justValue(getDirtyBuildingState().getLastBuildValue());
  }
 else {
    return null;
  }
}
